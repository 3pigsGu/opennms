        resource-xpath="/MBean" >
        <xml-object name="NotificTasksQueued" type="counter" xpath="Attribute[@name=NotificationTasksQueued]" />
        <xml-object name="BinaryNoticeAttemp" type="counter" xpath="Attribute[@name=BinaryNoticesAttempted]" />
        <xml-object name="JavaNoticesAttempt" type="counter" xpath="Attribute[@name=JavaNoticesAttempted]" />
        <xml-object name="BinaryNoticeSuccee" type="counter" xpath="Attribute[@name=BinaryNoticesSucceeded]" />
        <xml-object name="JavaNoticesSucceed" type="counter" xpath="Attribute[@name=JavaNoticesSucceeded]" />
        <xml-object name="BinaryNoticeFailed" type="counter" xpath="Attribute[@name=BinaryNoticesFailed]" />
        <xml-object name="JavaNoticesFailed" type="counter" xpath="Attribute[@name=JavaNoticesFailed]" />
        <xml-object name="BinaryNoticeInterr" type="counter" xpath="Attribute[@name=BinaryNoticesInterrupted]" />
        <xml-object name="JavaNoticesInterru" type="counter" xpath="Attribute[@name=JavaNoticesInterrupted]" />
        <xml-object name="UnknowNoticeInterr" type="counter" xpath="Attribute[@name=UnknownNoticesInterrupted]" />
