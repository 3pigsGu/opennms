<xml-groups>
    <xml-group name="opennms-queued" resource-type="node"
        resource-xpath="/MBean" >
        <xml-object name="ONMSQueCreates" type="counter" xpath="Attribute[@name=CreatesCompleted]" />
        <xml-object name="ONMSQueItemDeque" type="counter" xpath="Attribute[@name=DequeuedItems]" />
        <xml-object name="ONMSQueDequeOps" type="counter" xpath="Attribute[@name=DequeuedOperations]" />
        <xml-object name="ONMSQueEnqueOps" type="counter" xpath="Attribute[@name=EnqueuedOperations]" />
        <xml-object name="ONMSQueErrors" type="counter" xpath="Attribute[@name=Errors]" />
        <xml-object name="ONMSQuePromo" type="counter" xpath="Attribute[@name=PromotionCount]" />
        <xml-object name="ONMSQueSigOpsCompl" type="counter" xpath="Attribute[@name=SignificantOpsCompleted]" />
        <xml-object name="ONMSQueSigOpsDeque" type="counter" xpath="Attribute[@name=SignificantOpsDequeued]" />
        <xml-object name="ONMSQueSigOpsEnque" type="counter" xpath="Attribute[@name=SignificantOpsEnqueued]" />
        <xml-object name="ONMSQueOpsPend" type="GAUGE" xpath="Attribute[@name=TotalOperationsPending]" />
        <xml-object name="ONMSQueUpdates" type="counter" xpath="Attribute[@name=UpdatesCompleted]" />
    </xml-group>
</xml-groups>
