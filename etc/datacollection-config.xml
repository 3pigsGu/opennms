<?xml version="1.0"?>
<datacollection-config
	rrdRepository = "@root.install.share@/rrd/snmp/">
	<snmp-collection name="default"
		maxVarsPerPdu = "50"
		snmpStorageFlag = "all">
		<rrd step = "300">
			<rra>RRA:AVERAGE:0.5:1:8928</rra>
			<rra>RRA:AVERAGE:0.5:12:8784</rra>
			<rra>RRA:MIN:0.5:12:8784</rra>
			<rra>RRA:MAX:0.5:12:8784</rra>
		</rrd>
		<groups>
			<group 	name = "mib2-interfaces" ifType = "all">
				<mibObj oid=".1.3.6.1.2.1.2.2.1.10" instance="ifIndex" alias="ifInOctets"    type="counter"/>
				<mibObj oid=".1.3.6.1.2.1.2.2.1.13" instance="ifIndex" alias="ifInDiscards"  type="counter"/>
				<mibObj oid=".1.3.6.1.2.1.2.2.1.14" instance="ifIndex" alias="ifInErrors"    type="counter"/>
				<mibObj oid=".1.3.6.1.2.1.2.2.1.16" instance="ifIndex" alias="ifOutOctets"   type="counter"/>
				<mibObj oid=".1.3.6.1.2.1.2.2.1.19" instance="ifIndex" alias="ifOutDiscards" type="counter"/>
				<mibObj oid=".1.3.6.1.2.1.2.2.1.20" instance="ifIndex" alias="ifOutErrors"   type="counter"/>
			</group>
			
			<group 	name = "mib2-interfaces-net-snmp" ifType = "all">
				<mibObj oid=".1.3.6.1.2.1.2.2.1.10" instance="ifIndex" alias="ifInOctets"    type="counter"/>
				<mibObj oid=".1.3.6.1.2.1.2.2.1.14" instance="ifIndex" alias="ifInErrors"    type="counter"/>
				<mibObj oid=".1.3.6.1.2.1.2.2.1.16" instance="ifIndex" alias="ifOutOctets"   type="counter"/>
				<mibObj oid=".1.3.6.1.2.1.2.2.1.19" instance="ifIndex" alias="ifOutDiscards" type="counter"/>
				<mibObj oid=".1.3.6.1.2.1.2.2.1.20" instance="ifIndex" alias="ifOutErrors"   type="counter"/>
			</group>
			
			<group 	name = "mib2-host-resources-storage" ifType = "ignore">
				<!-- Note that hrStorage is really a table -->
				<!-- and I'm ignoring the hrStorageIndex altogether -->
				<mibObj oid=".1.3.6.1.2.1.25.2.3.1.4" instance="2" alias="hrStorageAllocationUnits"  type="counter"/>
				<mibObj oid=".1.3.6.1.2.1.25.2.3.1.5" instance="2" alias="hrStorageSize"  type="counter"/>
				<mibObj oid=".1.3.6.1.2.1.25.2.3.1.6" instance="2" alias="hrStorageUsed"  type="counter"/>
			</group>
					
			<group 	name = "mib2-host-resources-system" ifType = "ignore">
				<mibObj oid=".1.3.6.1.2.1.25.1.1" instance="0" alias="hrSystemUptime"    type="timeticks"/>
				<mibObj oid=".1.3.6.1.2.1.25.1.5" instance="0" alias="hrSystemNumUsers"  type="gauge"/>
				<mibObj oid=".1.3.6.1.2.1.25.1.6" instance="0" alias="hrSystemProcesses" type="gauge"/>
			</group>
			
			<group 	name = "mib2-host-resources-memory" ifType = "ignore">
				<mibObj oid=".1.3.6.1.2.1.25.2.2" instance="0" alias="hrMemorySize"    type="integer"/>
			</group>
			
			<group 	name = "cisco-router" ifType = "ignore">
				<mibObj oid=".1.3.6.1.4.1.9.2.1.58" instance="0" alias="avgBusy5"    type="integer"/>
				<mibObj oid=".1.3.6.1.4.1.9.2.1.8"  instance="0" alias="freeMem"     type="gauge"/>
				<mibObj oid=".1.3.6.1.4.1.9.2.1.46" instance="0" alias="bufferFail"  type="counter"/>
				<mibObj oid=".1.3.6.1.4.1.9.2.1.47" instance="0" alias="bufferNoMem" type="counter"/>
			</group>

			<group name = "cisco-router-protocols" ifType = "all">
                                <mibObj oid=".1.3.6.1.4.1.9.2.2.1.1.44" instance="ifIndex" alias="locIfipInOct" type="counter"/>
                                <mibObj oid=".1.3.6.1.4.1.9.2.2.1.1.45" instance="ifIndex" alias="locIfipOutOct" type="counter"/>
                                <mibObj oid=".1.3.6.1.4.1.9.2.2.1.1.64" instance="ifIndex" alias="locIfnovellInOct" type="counter"/>
                                <mibObj oid=".1.3.6.1.4.1.9.2.2.1.1.65" instance="ifIndex" alias="locIfnovellOutOct" type="counter"/>
                                <mibObj oid=".1.3.6.1.4.1.9.2.2.1.1.76" instance="ifIndex" alias="locIfbridgedInOct" type="counter"/>
                                <mibObj oid=".1.3.6.1.4.1.9.2.2.1.1.77" instance="ifIndex" alias="locIfbridgedOutOct" type="counter"/>
                        </group>
			
			<group 	name = "bay-router" ifType = "ignore">
				<mibObj oid=".1.3.6.1.4.1.18.3.3.2.5.1.1.9"  instance="0" alias="wfkernelTasksTotal"  type="gauge"/>
				<mibObj oid=".1.3.6.1.4.1.18.3.3.2.5.1.1.10" instance="0" alias="wfkernelTasksInQue"  type="counter"/>
				<mibObj oid=".1.3.6.1.4.1.18.3.3.2.5.1.1.3"  instance="0" alias="wfkernelMemoryFree"  type="counter"/>
				<mibObj oid=".1.3.6.1.4.1.18.3.3.2.5.1.1.3"  instance="0" alias="wfkernelBuffersFree" type="counter"/>
			</group>
			
			<group 	name = "3Com-router-perf" ifType = "ignore">
				<mibObj oid=".1.3.6.1.4.1.43.2.33.1.1.2.1.4" instance="0" alias="a3perfBufMemAvail"      type="integer"/>
				<mibObj oid=".1.3.6.1.4.1.43.2.33.1.1.2.1.5" instance="0" alias="a3perfBufMemFailed"     type="integer"/>
				<mibObj oid=".1.3.6.1.4.1.43.2.33.1.1.2.1.3" instance="0" alias="a3perfBufMemTotal"      type="integer"/>
				<mibObj oid=".1.3.6.1.4.1.43.2.33.1.1.1"     instance="0" alias="a3perfBufMemTotalAvail" type="integer"/>
			</group>
			
			<group 	name = "3com-router-sys" ifType = "ignore">
				<mibObj oid=".1.3.6.1.4.1.43.2.13.3.1.1.5" instance="0" alias="a3sysMemSize" type="integer"/>
				<mibObj oid=".1.3.6.1.4.1.43.2.13.8.4"     instance="0" alias="a3sysCpuUtil" type="integer"/>
			</group>

			<group name = "netware-host" ifType = "ignore">
				<mibObj oid=".1.3.6.1.2.1.25.3.3.1.2" instance="1" alias="cpuPercentBusy" type="integer" />
				<mibObj oid=".1.3.6.1.2.1.25.1.6" instance="0" alias="numberOfNLMsLoaded" type="gauge" />
				<mibObj oid=".1.3.6.1.2.1.25.2.2" instance="0" alias="memorySize" type="integer" />
				<mibObj oid=".1.3.6.1.2.1.25.2.3.1.4" instance="3" alias="cacheBuffersUnits" type="integer" />
				<mibObj oid=".1.3.6.1.2.1.25.2.3.1.5" instance="3" alias="cacheBuffersSize" type="integer" />
				<mibObj oid=".1.3.6.1.2.1.25.2.3.1.6" instance="3" alias="cacheBuffersUsed" type="integer" />
				<mibObj oid=".1.3.6.1.2.1.25.2.3.1.4" instance="6" alias="codeDataMemoryUnits" type="integer" />
				<mibObj oid=".1.3.6.1.2.1.25.2.3.1.5" instance="6" alias="codeDataMemorySize" type="integer" />
				<mibObj oid=".1.3.6.1.2.1.25.2.3.1.6" instance="6" alias="codeDataMemoryUsed" type="integer" />
				<mibObj oid=".1.3.6.1.4.1.23.2.28.2.7" instance="0" alias="currentOpenFiles" type="integer" />
				<mibObj oid=".1.3.6.1.4.1.23.2.28.3.2" instance="0" alias="licensedConnections" type="integer" />
				<mibObj oid=".1.3.6.1.4.1.23.2.28.2.14.1.4" instance="1" alias="freeSpaceOnSys" type="integer" />
				<mibObj oid=".1.3.6.1.4.1.23.2.28.2.14.1.5" instance="1" alias="freeableSpaceOnSys" type="integer" />
				<mibObj oid=".1.3.6.1.4.1.23.2.28.2.14.1.4" instance="3" alias="freeSpaceOnVol2" type="integer" />
				<mibObj oid=".1.3.6.1.4.1.23.2.28.2.14.1.5" instance="3" alias="freeableSpaceOnVol2" type="integer" />
			</group>
			<group name = "windows-host" ifType = "ignore">
				<mibObj oid=".1.3.6.1.2.1.25.3.3.1.2" instance="1" alias="cpuPercentBusy" type="integer" />
				<!-- Drive information is from the hrStorage table. -->
				<!-- I'm cheating and assuming that the second and third -->
				<!-- entries refer to Drive_C and Drive_D -->
				<mibObj oid=".1.3.6.1.2.1.25.2.3.1.4" instance="2" alias="unitsDriveC" type="integer" />
				<mibObj oid=".1.3.6.1.2.1.25.2.3.1.5" instance="2" alias="sizeDriveC" type="integer" />
				<mibObj oid=".1.3.6.1.2.1.25.2.3.1.6" instance="2" alias="usedDriveC" type="integer" />
				<mibObj oid=".1.3.6.1.2.1.25.2.3.1.4" instance="3" alias="unitsDriveD" type="integer" />
				<mibObj oid=".1.3.6.1.2.1.25.2.3.1.5" instance="3" alias="sizeDriveD" type="integer" />
				<mibObj oid=".1.3.6.1.2.1.25.2.3.1.6" instance="3" alias="usedDriveD" type="integer" />
				<!-- Memory in KBytes -->
				<mibObj oid=".1.3.6.1.2.1.25.2.2" instance="0" alias="memorySize" type="integer" />
			</group>

			<group name = "checkpoint-host" ifType = "ignore">
				<mibObj oid=".1.3.6.1.2.1.25.1.5" instance="0" alias="stateInfoStored" type="integer" />
				<mibObj oid=".1.3.6.1.2.1.25.1.6" instance="0" alias="processContexts" type="integer" />
				<mibObj oid=".1.3.6.1.2.1.25.2.3.1.6" instance="1" alias="allocatedStorage" type="integer" />
				<mibObj oid=".1.3.6.1.4.1.94.1.21.1.6.1" instance="0" alias="cpuUtilization" type="integer" />
				<mibObj oid=".1.3.6.1.4.1.2620.1.1.4" instance="0" alias="pktsAccepted" type="integer" />
				<mibObj oid=".1.3.6.1.4.1.2620.1.1.5" instance="0" alias="pktsRejected" type="integer" />
				<mibObj oid=".1.3.6.1.4.1.2620.1.1.6" instance="0" alias="pktsDropped" type="integer" />
				<mibObj oid=".1.3.6.1.4.1.2620.1.1.7" instance="0" alias="pktsLogged" type="integer" />
			</group>

			<group name = "domino-stats" ifType = "ignore">
                                <mibObj oid=".1.3.6.1.4.1.334.72.1.1.6.3.6"   instance="0" alias="attachedUsers"       type="integer" />
                                <mibObj oid=".1.3.6.1.4.1.334.72.1.1.4.1"     instance="0" alias="deadMail"            type="integer" />
                                <mibObj oid=".1.3.6.1.4.1.334.72.1.1.4.2"     instance="0" alias="deliveredMail"       type="integer" />
                                <mibObj oid=".1.3.6.1.4.1.334.72.1.1.4.5"     instance="0" alias="transferredMail"     type="integer" />
                                <mibObj oid=".1.3.6.1.4.1.334.72.1.1.4.6"     instance="0" alias="waitingMail"         type="integer" />
                                <mibObj oid=".1.3.6.1.4.1.334.72.1.1.4.7"     instance="0" alias="msgsWaitForDlvry"    type="integer" />
                                <mibObj oid=".1.3.6.1.4.1.334.72.1.1.4.9"     instance="0" alias="aveMailDeliveryTime" type="integer" />
                                <mibObj oid=".1.3.6.1.4.1.334.72.1.1.4.11"    instance="0" alias="aveMailSizeDlvrd"    type="integer" />
                                <mibObj oid=".1.3.6.1.4.1.334.72.1.1.4.19"    instance="0" alias="mailTxFailures"      type="integer" />
                                <mibObj oid=".1.3.6.1.4.1.334.72.1.1.5.6.4.0" instance="0" alias="replFailures"        type="integer" />
                                <mibObj oid=".1.3.6.1.4.1.334.72.1.1.6.3.2"   instance="0" alias="aveTransactPerMin"   type="integer" />
                                <mibObj oid=".1.3.6.1.4.1.334.72.1.1.6.3.9"   instance="0" alias="maxUsers"            type="integer" />
                                <mibObj oid=".1.3.6.1.4.1.334.72.1.1.9.2"     instance="0" alias="allocatedMem"        type="integer" />
                                <mibObj oid=".1.3.6.1.4.1.334.72.1.1.9.5"     instance="0" alias="freeMemory"          type="integer" />
                                <mibObj oid=".1.3.6.1.4.1.334.72.1.1.8.3.1.4" instance="1" alias="1stHDDFreeSpace"     type="integer" />
                                <mibObj oid=".1.3.6.1.4.1.334.72.1.1.8.3.1.4" instance="2" alias="2ndHDDFreeSpace"     type="integer" />
                                <mibObj oid=".1.3.6.1.4.1.334.72.1.1.16.1"    instance="0" alias="totalApptmtReserv"   type="integer" />
                                <mibObj oid=".1.3.6.1.4.1.334.72.1.1.16.6"    instance="0" alias="totalCalendarUsers"  type="integer" />
                        </group>

			<group name = "compaq-insight-manager-stats" ifType = "ignore">
				 <mibObj oid="1.3.6.1.4.1.232.11.2.4.1.1.5"    instance="0" alias="cimPctHddUsed1"  type="integer" />
				 <mibObj oid="1.3.6.1.4.1.232.11.2.4.1.1.5"    instance="1" alias="cimPctHddUsed2"  type="integer" />
				 <mibObj oid="1.3.6.1.4.1.232.11.2.3.1.1.3"    instance="0" alias="cim5MinCpuUtilPct"  type="integer" />
			</group>	
			<group name="ucd-loadavg" ifType="ignore">
				<mibObj oid=".1.3.6.1.4.1.2021.10.1.5" instance="1" alias="loadavg1" type="integer" /> 
				<mibObj oid=".1.3.6.1.4.1.2021.10.1.5" instance="2" alias="loadavg5" type="integer" /> 
				<mibObj oid=".1.3.6.1.4.1.2021.10.1.5" instance="3" alias="loadavg15" type="integer" /> 
			</group>

			<group  name="mib2-interfaces-sunw" ifType="all">
                                <mibObj oid=".1.3.6.1.2.1.2.2.1.10" instance="ifIndex" alias="ifInOctets"        type="counter"/>
                                <mibObj oid=".1.3.6.1.2.1.2.2.1.11" instance="ifIndex" alias="ifInUcastPkts"     type="counter"/>
                                <mibObj oid=".1.3.6.1.2.1.2.2.1.12" instance="ifIndex" alias="ifInNUcastPkts"    type="counter"/>
                                <mibObj oid=".1.3.6.1.2.1.2.2.1.13" instance="ifIndex" alias="ifInDiscards"      type="counter"/>
                                <mibObj oid=".1.3.6.1.2.1.2.2.1.14" instance="ifIndex" alias="ifInErrors"        type="counter"/>
                                <mibObj oid=".1.3.6.1.2.1.2.2.1.15" instance="ifIndex" alias="ifInUnknownProtos" type="counter"/>
                                <mibObj oid=".1.3.6.1.2.1.2.2.1.16" instance="ifIndex" alias="ifOutOctets"       type="counter"/>
                                <mibObj oid=".1.3.6.1.2.1.2.2.1.17" instance="ifIndex" alias="ifOutUcastPkts"    type="counter"/>
                                <mibObj oid=".1.3.6.1.2.1.2.2.1.18" instance="ifIndex" alias="ifOutNUcastPkts"   type="counter"/>
                                <mibObj oid=".1.3.6.1.2.1.2.2.1.19" instance="ifIndex" alias="ifOutDiscards"     type="counter"/>
                                <mibObj oid=".1.3.6.1.2.1.2.2.1.20" instance="ifIndex" alias="ifOutErrors"       type="counter"/>
                        </group>

                        <group  name="sunHostPerf" ifType = "ignore">
                                <mibObj oid=".1.3.6.1.4.1.42.3.13.1"  instance="0" alias="rsUserProcessTime"   type="counter"/>
                                <mibObj oid=".1.3.6.1.4.1.42.3.13.2"  instance="0" alias="rsNiceModeTime"      type="counter"/>
                                <mibObj oid=".1.3.6.1.4.1.42.3.13.3"  instance="0" alias="rsSystemProcessTime" type="counter"/>
                                <mibObj oid=".1.3.6.1.4.1.42.3.13.4"  instance="0" alias="rsIdleModeTime"      type="counter"/>
                                <mibObj oid=".1.3.6.1.4.1.42.3.13.5"  instance="0" alias="rsDiskXfer1"         type="counter"/>
                                <mibObj oid=".1.3.6.1.4.1.42.3.13.6"  instance="0" alias="rsDiskXfer2"         type="counter"/>
                                <mibObj oid=".1.3.6.1.4.1.42.3.13.7"  instance="0" alias="rsDiskXfer3"         type="counter"/>
                                <mibObj oid=".1.3.6.1.4.1.42.3.13.8"  instance="0" alias="rsDiskXfer4"         type="counter"/>
                                <mibObj oid=".1.3.6.1.4.1.42.3.13.9"  instance="0" alias="rsVPagesIn"          type="counter"/>
                                <mibObj oid=".1.3.6.1.4.1.42.3.13.10" instance="0" alias="rsVPagesOut"         type="counter"/>
                                <mibObj oid=".1.3.6.1.4.1.42.3.13.11" instance="0" alias="rsVSwapIn"           type="counter"/>
                                <mibObj oid=".1.3.6.1.4.1.42.3.13.12" instance="0" alias="rsVSwapOut"          type="counter"/>
                                <mibObj oid=".1.3.6.1.4.1.42.3.13.13" instance="0" alias="rsVIntr"             type="counter"/>
                                <mibObj oid=".1.3.6.1.4.1.42.3.13.14" instance="0" alias="rsIfInPackets"       type="counter"/>
                                <mibObj oid=".1.3.6.1.4.1.42.3.13.15" instance="0" alias="rsIfOutPackets"      type="counter"/>
                                <mibObj oid=".1.3.6.1.4.1.42.3.13.16" instance="0" alias="rsIfInErrors"        type="counter"/>
                                <mibObj oid=".1.3.6.1.4.1.42.3.13.17" instance="0" alias="rsIfOutErrors"       type="counter"/>
                                <mibObj oid=".1.3.6.1.4.1.42.3.13.18" instance="0" alias="rsIfCollisions"      type="counter"/>
                        </group>
		</groups>

		<systems>
			<systemDef name = "Enterprise">
				<sysoidMask>.1.3.6.1.4.1.</sysoidMask>
				<collect>
					<includeGroup>mib2-interfaces-net-snmp</includeGroup>
				</collect>
			</systemDef>		
			
			<systemDef name = "Net-SNMP">
				<sysoidMask>.1.3.6.1.4.1.2021.250.</sysoidMask>
				<collect>
					<includeGroup>mib2-interfaces-net-snmp</includeGroup>
					<includeGroup>mib2-host-resources-storage</includeGroup> 
					<includeGroup>mib2-host-resources-system</includeGroup> 
					<includeGroup>mib2-host-resources-memory</includeGroup> 
					<includeGroup>ucd-loadavg</includeGroup> 
				</collect>
			</systemDef>
			
			<systemDef name = "Windows NT/2000">
				<sysoid>.1.3.6.1.4.1.311.1.1.3.1.1</sysoid>
				<collect>
					<includeGroup>mib2-interfaces</includeGroup>
					<includeGroup>windows-host</includeGroup>
					<includeGroup>checkpoint-host</includeGroup>
					<includeGroup>domino-stats</includeGroup>
					<includeGroup>compaq-insight-manager-stats</includeGroup>
				</collect>
			</systemDef>
			<systemDef name = "Citrix">
				<sysoid>.1.3.6.1.4.1.311.1.1.3.1.2</sysoid>
				<collect>
					<includeGroup>mib2-interfaces</includeGroup>
					<includeGroup>windows-host</includeGroup>
				</collect>
			</systemDef>
			
			<systemDef name = "CheckPoint">
				<sysoid>.1.3.6.1.4.1.2620.1</sysoid>
				<collect>
					<includeGroup>mib2-interfaces</includeGroup>
					<includeGroup>checkpoint-host</includeGroup>
				</collect>
			</systemDef>
				
			<systemDef name = "Netware">
				<sysoidMask>.1.3.6.1.4.1.23.1.6.</sysoidMask>
				<collect>
					<includeGroup>mib2-interfaces</includeGroup>
					<includeGroup>netware-host</includeGroup>
				</collect>
			</systemDef>
			
			<systemDef name = "Cisco Routers">
				<sysoidMask>.1.3.6.1.4.1.9.1.</sysoidMask>
				<collect>
					<includeGroup>mib2-interfaces</includeGroup>
					<includeGroup>cisco-router</includeGroup>
					<includeGroup>cisco-router-protocols</includeGroup>
				</collect>
			</systemDef>
			
			<systemDef name = "Bay Routers">
				<sysoidMask>.1.3.6.1.4.1.18.</sysoidMask>
				<collect>
					<includeGroup>mib2-interfaces</includeGroup>
					<includeGroup>bay-router</includeGroup>
				</collect>
			</systemDef>
			
			<systemDef name = "3Com Routers">
				<sysoidMask>.1.3.6.1.4.1.43.</sysoidMask>
				<collect>
					<includeGroup>mib2-interfaces</includeGroup>
					<includeGroup>3com-router-sys</includeGroup>
					<includeGroup>3com-router-perf</includeGroup>
				</collect>
			</systemDef>
			<systemDef name = "Sun Enterprise Agent v1.0.3">
				<sysoidMask>.1.3.6.1.4.1.42.</sysoidMask>
                                <collect>
					<includeGroup>mib2-interfaces-sunw</includeGroup>
					<includeGroup>sunHostPerf</includeGroup>
                                </collect>
                        </systemDef>

		</systems>
	</snmp-collection>
</datacollection-config>
