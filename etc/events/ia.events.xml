
<events>
    <event>
        <uei>uei.opennms.org/internal/inventory/configurationChanged</uei>
        <event-label>IA OpenNMS-defined Inventory changed configuration event.</event-label>
        <descr>&lt;p&gt;Inventory changed configuration event.&lt;/p&gt; &lt;p&gt;</descr>
        <logmsg dest='logndisplay'>&lt;p&gt;Inventory changed configuration event.&lt;/p&gt;</logmsg>
        <severity>Warning</severity>
    </event>
    <event>
        <uei>uei.opennms.org/internal/inventory/Success</uei>
        <event-label>IA OpenNMS-defined Inventory success event.</event-label>
        <descr>&lt;p&gt;Inventory request successfully completed.&lt;/p&gt; &lt;p&gt;</descr>
        <logmsg dest='logndisplay'>&lt;p&gt;Inventory request successfully completed.&lt;/p&gt;</logmsg>
        <severity>Normal</severity>
    </event>
    <event>
        <uei>uei.opennms.org/internal/inventory/Failure</uei>
        <event-label>IA OpenNMS-defined Inventory failure event.</event-label>
        <descr>&lt;p&gt;Inventory request failure.&lt;/p&gt; &lt;p&gt;</descr>
        <logmsg dest='logndisplay'>&lt;p&gt;Inventory request failure.&lt;/p&gt;</logmsg>
        <severity>Minor</severity>
    </event>
</events>

                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                
