<?xml version="1.0" encoding="UTF-8"?>
<passive-status-configuration 
xmlns:this="http://xmlns.opennms.org/xsd/passive-status-configuration" 
xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" >
  <passive-events>
    <passive-event uei="uei.opennms.org/services/passiveServiceStatus">
      <status-key>
        <node-label>
          <event-token is-parm="true" name="nodeLabel" value="Router1"/>
        </node-label>
        <ipaddr>
          <event-token is-parm="true" name="ipAddr" value="192.168.1.1"/>
        </ipaddr>
        <service-name>
          <event-token is-parm="true" name="serviceName" value="myPassiveService"/>
        </service-name>
        <status>
          <event-token is-parm="true" name="status" value="Down"/>
        </status>
      </status-key>
    </passive-event>
  </passive-events>
</passive-status-configuration>
