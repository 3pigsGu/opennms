<?xml version="1.0" encoding="UTF-8" standalone="no"?><notifd-configuration match-all="true" status="off">
        
	<auto-acknowledge acknowledge="uei.opennms.org/nodes/serviceUnresponsive" resolution-prefix="RESOLVED: " uei="uei.opennms.org/nodes/serviceResponsive">
                          <match>nodeid</match>
                          <match>interfaceid</match>
                          <match>serviceid</match>
        </auto-acknowledge>
	
        <auto-acknowledge acknowledge="uei.opennms.org/nodes/nodeLostService" resolution-prefix="RESOLVED: " uei="uei.opennms.org/nodes/nodeRegainedService">
                          <match>nodeid</match>
                          <match>interfaceid</match>
                          <match>serviceid</match>
        </auto-acknowledge>
        
        <auto-acknowledge acknowledge="uei.opennms.org/nodes/interfaceDown" resolution-prefix="RESOLVED: " uei="uei.opennms.org/nodes/interfaceUp">
                          <match>nodeid</match>
                          <match>interfaceid</match>
        </auto-acknowledge>
        
        <auto-acknowledge acknowledge="uei.opennms.org/nodes/nodeDown" resolution-prefix="RESOLVED: " uei="uei.opennms.org/nodes/nodeUp">
                          <match>nodeid</match>
        </auto-acknowledge>
        
        <auto-acknowledge acknowledge="uei.opennms.org/correlation/remote/wideSpreadOutage" resolution-prefix="RESOLVED: " uei="uei.opennms.org/correlation/remote/wideSpreadOutageResolved">
                          <match>nodeid</match>
                          <match>interfaceid</match>
                          <match>serviceid</match>
        </auto-acknowledge>
        
        <queue>
                <queue-id>default</queue-id>
                <interval>20s</interval>
                <handler-class>
                        <name>org.opennms.netmgt.notifd.DefaultQueueHandler</name>
                </handler-class>
        </queue>
</notifd-configuration>