<?xml version="1.0" encoding="UTF-8" standalone="no"?><jmx-datacollection-config rrdRepository="${install.share.dir}/rrd/snmp/">
  <jmx-collection name="jboss">
    <rrd step="300">
      <rra>RRA:AVERAGE:0.5:1:2016</rra>
      <rra>RRA:AVERAGE:0.5:12:1488</rra>
      <rra>RRA:AVERAGE:0.5:288:366</rra>
      <rra>RRA:MAX:0.5:288:366</rra>
      <rra>RRA:MIN:0.5:288:366</rra>
    </rrd>
    <mbeans>
      <mbean name="SystemInfo" objectname="jboss.system:type=ServerInfo">
        <attrib alias="FreeMemory" name="FreeMemory" type="gauge"/>
        <attrib alias="TotalMemory" name="TotalMemory" type="gauge"/>
      </mbean>
      <mbean name="DefaultDSManagedConnectionPool" objectname="jboss.jca:service=ManagedConnectionPool,name=DefaultDS">
        <attrib alias="DefDS_AvailConns" name="AvailableConnectionCount" type="gauge"/>
        <attrib alias="DefDS_Conns" name="ConnectionCount" type="gauge"/>
        <attrib alias="DefDS_InUseConns" name="InUseConnectionCount" type="gauge"/>
        <attrib alias="DefDS_CreatedConns" name="ConnectionCreatedCount" type="counter"/>
        <attrib alias="DefDS_DestroyConns" name="ConnectionDestroyedCount" type="counter"/>
      </mbean>
      <!-- JmsXA Connector Pool
          <mbean name="JmsXAManagedConnectionPool" objectname="jboss.jca:service=ManagedConnectionPool,name=JmsXA">  
              <attrib name="AvailableConnectionCount"   alias="JmsXA_AvailConns"    type="gauge"/> 
              <attrib name="ConnectionCount"            alias="JmsXA_Conns"         type="gauge"/>  
              <attrib name="InUseConnectionCount"       alias="JmsXA_InUseConns"    type="gauge"/>  
              <attrib name="ConnectionCreatedCount"     alias="JmsXA_CreatedConns"  type="gauge"/>  
              <attrib name="ConnectionDestroyedCount"   alias="JmsXA_DestroyConns"  type="gauge"/>  
          </mbean-->
      <!-- Destination Manager
          <mbean name="MQ_DestinationManager" objectname="jboss.mq:service=DestinationManager">  
              <attrib name="ClientCount"   alias="MQ_ClientCount"    type="gauge"/> 
          </mbean-->
      <!-- an example of a Queue 
          <mbean name="MQ_DestinationQueueA" objectname="jboss.mq.destination:service=Queue,name=A">  
              <attrib name="QueueDepth"       alias="A_QueueDepth"       type="gauge"/> 
              <attrib name="ReceiversCount"   alias="A_QueueRecv"        type="gauge"/> 
              <attrib name="MaxDepth"         alias="A_QueueMaxDepth"    type="gauge"/> 
          </mbean-->
      <!-- an example of a Topic
          <mbean name="SecuredTopic" objectname="jboss.mq.destination:service=Topic,name=securedTopic">  
              <attrib name="DurableMessageCount"         alias="ST_DurMsg"      type="gauge"/> 
              <attrib name="NonDurableMessageCount"      alias="ST_NonDurMsg"   type="gauge"/> 
              <attrib name="NonDurableSubscriptionCount" alias="ST_NonDurSub"   type="gauge"/> 
              <attrib name="DurableSubscriptionCount"    alias="ST_DurSub"      type="gauge"/> 
              <attrib name="AllMessageCount"             alias="ST_AllMsg"      type="gauge"/> 
              <attrib name="MaxDepth"                    alias="ST_MaxDepth"    type="gauge"/> 
              <attrib name="AllSubscriptionsCount"       alias="ST_AllSub"      type="gauge"/> 
          </mbean-->
      <!-- Global Request Processor -->
      <mbean name="GlobalRequestProcessor" objectname="jboss.web:type=GlobalRequestProcessor,name=http-0.0.0.0-8080">
        <attrib alias="GRP_requests" name="requestCount" type="counter"/>
        <attrib alias="GRP_maxTime" name="maxTime" type="gauge"/>
        <attrib alias="GRP_bytesSent" name="bytesSent" type="counter"/>
        <attrib alias="GRP_bytesRec" name="bytesReceived" type="counter"/>
        <attrib alias="GRP_procTime" name="processingTime" type="counter"/>
        <attrib alias="GRP_errors" name="errorCount" type="counter"/>
      </mbean>
      <!-- Thread Pool -->
      <mbean name="ThreadPool" objectname="jboss.web:type=ThreadPool,name=http-0.0.0.0-8080">
        <attrib alias="BusyThreads" name="currentThreadsBusy" type="gauge"/>
        <attrib alias="Threads" name="currentThreadCount" type="gauge"/>
        <attrib alias="MinSpareThreads" name="minSpareThreads" type="gauge"/>
        <attrib alias="MaxSpareThreads" name="maxSpareThreads" type="gauge"/>
        <attrib alias="MaxThreads" name="maxThreads" type="gauge"/>
      </mbean>
    </mbeans>
  </jmx-collection>
  <jmx-collection name="jsr160">
    <rrd step="300">
      <rra>RRA:AVERAGE:0.5:1:2016</rra>
      <rra>RRA:AVERAGE:0.5:12:1488</rra>
      <rra>RRA:AVERAGE:0.5:288:366</rra>
      <rra>RRA:MAX:0.5:288:366</rra>
      <rra>RRA:MIN:0.5:288:366</rra>
    </rrd>
    <mbeans>
      <mbean name="OpenNMS Queued" objectname="OpenNMS:Name=Queued">
        <attrib alias="ONMSQueCreates" name="CreatesCompleted" type="counter"/>
        <attrib alias="ONMSQueItemDeque" name="DequeuedItems" type="counter"/>
        <attrib alias="ONMSQueDequeOps" name="DequeuedOperations" type="counter"/>
        <attrib alias="ONMSQueEnqueOps" name="EnqueuedOperations" type="counter"/>
        <attrib alias="ONMSQueErrors" name="Errors" type="counter"/>
        <attrib alias="ONMSQuePromo" name="PromotionCount" type="counter"/>
        <attrib alias="ONMSQueSigOpsCompl" name="SignificantOpsCompleted" type="counter"/>
        <attrib alias="ONMSQueSigOpsDeque" name="SignificantOpsDequeued" type="counter"/>
        <attrib alias="ONMSQueSigOpsEnque" name="SignificantOpsEnqueued" type="counter"/>
        <attrib alias="ONMSQueOpsPend" name="TotalOperationsPending" type="gauge"/>
        <attrib alias="ONMSQueUpdates" name="UpdatesCompleted" type="counter"/>
      </mbean>
      <mbean name="OpenNMS Pollerd" objectname="OpenNMS:Name=Pollerd">
        <attrib alias="ONMSPollCount" name="NumPolls" type="counter"/>
        <attrib alias="ONMSPollerThreadAct" name="ActiveThreads" type="gauge"/>
        <attrib alias="ONMSPollerTasksTot" name="TasksTotal" type="counter"/>
        <attrib alias="ONMSPollerTasksCpt" name="TasksCompleted" type="counter"/>
      </mbean>
      <mbean name="OpenNMS Vacuumd" objectname="OpenNMS:Name=Vacuumd">
        <attrib alias="ONMSAutomCount" name="NumAutomations" type="counter"/>
      </mbean>
      <mbean name="OpenNMS Collectd" objectname="OpenNMS:Name=Collectd">
        <attrib alias="ONMSCollectThrdAct" name="ActiveThreads" type="gauge"/>
        <attrib alias="ONMSCollectPoolThrd" name="NumPoolThreads" type="gauge"/>
        <attrib alias="ONMSCollectPoolMax" name="MaxPoolThreads" type="gauge"/>
        <attrib alias="ONMSCollectPoolPeak" name="PeakPoolThreads" type="gauge"/>
        <attrib alias="ONMSCollectTasksTot" name="TasksTotal" type="counter"/>
        <attrib alias="ONMSCollectTasksCpt" name="TasksCompleted" type="counter"/>
        <attrib alias="ONMSCollectSvcCount" name="CollectableServiceCount" type="gauge"/>
      </mbean>
      <mbean name="OpenNMS.Capsd" objectname="OpenNMS:Name=Capsd">
        <attrib alias="ActiveSuspecThreads" name="ActiveSuspectThreads" type="gauge"/>
        <attrib alias="ActiveRescanThreads" name="ActiveRescanThreads" type="gauge"/>
        <attrib alias="SuspectCompletTasks" name="SuspectCompletedTasks" type="counter"/>
        <attrib alias="RescanCompletTasks" name="RescanCompletedTasks" type="counter"/>
        <attrib alias="SuspectTotalTasks" name="SuspectTotalTasks" type="counter"/>
        <attrib alias="RescanTotalTasks" name="RescanTotalTasks" type="counter"/>
        <attrib alias="SuspTaskComplRatio" name="SuspectTaskCompletionRatio" type="gauge"/>
        <attrib alias="RescTaskComplRatio" name="RescanTaskCompletionRatio" type="gauge"/>
        <attrib alias="SuspectQueueSize" name="SuspectQueueSize" type="gauge"/>
        <attrib alias="RescanQueueSize" name="RescanQueueSize" type="gauge"/>
      </mbean>
      <mbean name="OpenNMS.JettyServer" objectname="OpenNMS:Name=JettyServer">
        <attrib alias="HttpsConnTotal" name="HttpsConnectionsTotal" type="counter"/>
        <attrib alias="HttpsConnOpen" name="HttpsConnectionsOpen" type="gauge"/>
        <attrib alias="HttpsConnOpenMax" name="HttpsConnectionsOpenMax" type="gauge"/>
        <attrib alias="HttpConnTotal" name="HttpConnectionsTotal" type="counter"/>
        <attrib alias="HttpConnOpen" name="HttpConnectionsOpen" type="gauge"/>
        <attrib alias="HttpConnOpenMax" name="HttpConnectionsOpenMax" type="gauge"/>
      </mbean>
      <mbean name="OpenNMS.Statsd" objectname="OpenNMS:Name=Statsd">
        <attrib alias="StaReportsStarted" name="ReportsStarted" type="counter"/>
        <attrib alias="StaReportsCompleted" name="ReportsCompleted" type="counter"/>
        <attrib alias="StaReportsPersisted" name="ReportsPersisted" type="counter"/>
        <attrib alias="StaReportRunTime" name="ReportRunTime" type="counter"/>
      </mbean>
      <mbean name="OpenNMS.Trapd" objectname="OpenNMS:Name=Trapd">
        <attrib alias="TrapsDiscarded" name="TrapsDiscarded" type="counter"/>
        <attrib alias="TrapsErrored" name="TrapsErrored" type="counter"/>
        <attrib alias="V1TrapsReceived" name="V1TrapsReceived" type="counter"/>
        <attrib alias="V2cTrapsReceived" name="V2cTrapsReceived" type="counter"/>
        <attrib alias="V3TrapsReceived" name="V3TrapsReceived" type="counter"/>
      </mbean>
      <mbean name="OpenNMS.Notifd" objectname="OpenNMS:Name=Notifd">
        <attrib alias="NotificTasksQueued" name="NotificationTasksQueued" type="counter"/>
        <attrib alias="BinaryNoticeAttemp" name="BinaryNoticesAttempted" type="counter"/>
        <attrib alias="JavaNoticesAttempt" name="JavaNoticesAttempted" type="counter"/>
        <attrib alias="BinaryNoticeSuccee" name="BinaryNoticesSucceeded" type="counter"/>
        <attrib alias="JavaNoticesSucceed" name="JavaNoticesSucceeded" type="counter"/>
        <attrib alias="BinaryNoticeFailed" name="BinaryNoticesFailed" type="counter"/>
        <attrib alias="JavaNoticesFailed" name="JavaNoticesFailed" type="counter"/>
        <attrib alias="BinaryNoticeInterr" name="BinaryNoticesInterrupted" type="counter"/>
        <attrib alias="JavaNoticesInterru" name="JavaNoticesInterrupted" type="counter"/>
        <attrib alias="UnknowNoticeInterr" name="UnknownNoticesInterrupted" type="counter"/>
      </mbean>
      <mbean name="OpenNMS.Manager" objectname="OpenNMS:Name=Manager">
        <attrib alias="Uptime" name="onmsUptime" type="gauge"/>
      </mbean>
      <mbean name="JVM Memory" objectname="java.lang:type=OperatingSystem">
        <attrib alias="FreeMemory" name="FreePhysicalMemorySize" type="gauge"/>
        <attrib alias="TotalMemory" name="TotalPhysicalMemorySize" type="gauge"/>
        <attrib alias="FreeSwapSpace" name="FreeSwapSpaceSize" type="gauge"/>
        <attrib alias="TotalSwapSpace" name="TotalSwapSpaceSize" type="gauge"/>
      </mbean>
      <mbean name="JVM Threading" objectname="java.lang:type=Threading">
        <attrib alias="ThreadCount" name="ThreadCount" type="gauge"/>
        <attrib alias="PeakThreadCount" name="PeakThreadCount" type="gauge"/>
        <attrib alias="DaemonThreadCount" name="DaemonThreadCount" type="gauge"/>
        <attrib alias="CurThreadCpuTime" name="CurrentThreadCpuTime" type="gauge"/>
      </mbean>
      <mbean name="JVM ClassLoading" objectname="java.lang:type=ClassLoading">
        <attrib alias="TotLoadedClasses" name="TotalLoadedClassCount" type="gauge"/>
        <attrib alias="LoadedClasses" name="LoadedClassCount" type="gauge"/>
        <attrib alias="UnloadedClass" name="UnloadedClassCount" type="gauge"/>
      </mbean>
      <mbean name="JVM MemoryPool:Eden Space" objectname="java.lang:type=MemoryPool,name=Eden Space">
        <attrib alias="EdenCollUseThrsh" name="CollectionUsageThreshold" type="gauge"/>
        <attrib alias="EdenCollUseThrshCnt" name="CollectionUsageThresholdCount" type="gauge"/>
        <comp-attrib alias="EdenUsage" name="Usage" type="Composite">
          <comp-member alias="EdenUsageInit" name="init" type="gauge"/>
          <comp-member alias="EdenUsageMax" name="max" type="gauge"/>
          <comp-member alias="EdenUsageUsed" name="used" type="gauge"/>
          <comp-member alias="EdenUsgCmmttd" name="committed" type="gauge"/>
        </comp-attrib>
        <comp-attrib alias="EdenPeakUsage" name="PeakUsage" type="Composite">
          <comp-member alias="EdenPeakUsageInit" name="init" type="gauge"/>
          <comp-member alias="EdenPeakUsageMax" name="max" type="gauge"/>
          <comp-member alias="EdenPeakUsageUsed" name="used" type="gauge"/>
          <comp-member alias="EdenPeakUsgCmmttd" name="committed" type="gauge"/>
        </comp-attrib>
      </mbean>
      <mbean name="JVM MemoryPool:Survivor Space" objectname="java.lang:type=MemoryPool,name=Survivor Space">
        <attrib alias="SurvCollUseThresh" name="CollectionUsageThreshold" type="gauge"/>
        <attrib alias="SurvCollUseThrshCnt" name="CollectionUsageThresholdCount" type="gauge"/>
        <comp-attrib alias="SurvUsage" name="Usage" type="Composite">
          <comp-member alias="SurvUsageInit" name="init" type="gauge"/>
          <comp-member alias="SurvUsageMax" name="max" type="gauge"/>
          <comp-member alias="SurvUsageUsed" name="used" type="gauge"/>
          <comp-member alias="SurvUsgCmmttd" name="committed" type="gauge"/>
        </comp-attrib>
        <comp-attrib alias="SurvPeakUsage" name="PeakUsage" type="Composite">
          <comp-member alias="SurvPeakUsageInit" name="init" type="gauge"/>
          <comp-member alias="SurvPeakUsageMax" name="max" type="gauge"/>
          <comp-member alias="SurvPeakUsageUsed" name="used" type="gauge"/>
          <comp-member alias="SurvPeakUsgCmmttd" name="committed" type="gauge"/>
        </comp-attrib>
      </mbean>
      <mbean name="JVM MemoryPool:Perm Gen" objectname="java.lang:type=MemoryPool,name=Perm Gen">
        <attrib alias="PermCollUseThresh" name="CollectionUsageThreshold" type="gauge"/>
        <attrib alias="PermCollUseThrshCnt" name="CollectionUsageThresholdCount" type="gauge"/>
        <comp-attrib alias="PermUsage" name="Usage" type="Composite">
          <comp-member alias="PermUsageInit" name="init" type="gauge"/>
          <comp-member alias="PermUsageMax" name="max" type="gauge"/>
          <comp-member alias="PermUsageUsed" name="used" type="gauge"/>
          <comp-member alias="PermUsgCmmttd" name="committed" type="gauge"/>
        </comp-attrib>
      </mbean>
      <mbean name="JVM MemoryPool:Old Gen" objectname="java.lang:type=MemoryPool,name=Old Gen">
        <attrib alias="OGenCollUseThresh" name="CollectionUsageThreshold" type="gauge"/>
        <attrib alias="OGenCollUseThrshCnt" name="CollectionUsageThresholdCount" type="gauge"/>
        <comp-attrib alias="OGenUsage" name="Usage" type="Composite">
          <comp-member alias="OGenUsageInit" name="init" type="gauge"/>
          <comp-member alias="OGenUsageMax" name="max" type="gauge"/>
          <comp-member alias="OGenUsageUsed" name="used" type="gauge"/>
          <comp-member alias="OGenUsgCmmttd" name="committed" type="gauge"/>
        </comp-attrib>
      </mbean>

      <mbean name="JVM GarbageCollector:Copy" objectname="java.lang:type=GarbageCollector,name=Copy">
        <attrib alias="CopyCollCnt" name="CollectionCount" type="counter"/>
        <attrib alias="CopyCollTime" name="CollectionTime" type="counter"/>
        <comp-attrib alias="CopyLastGcInfo" name="LastGcInfo" type="Composite">
          <comp-member alias="CopyGcThreadCnt" name="GcThreadCount" type="gauge"/>
          <comp-member alias="CopyDuration" name="duration" type="gauge"/>
          <comp-member alias="CopyEndTime" name="endTime" type="gauge"/>
        </comp-attrib>
      </mbean>
      <mbean name="JVM GarbageCollector:MarkSweepCompact" objectname="java.lang:type=GarbageCollector,name=MarkSweepCompact">
        <attrib alias="MSCCollCnt" name="CollectionCount" type="counter"/>
        <attrib alias="MSCCollTime" name="CollectionTime" type="counter"/>
        <comp-attrib alias="MSCLastGcInfo" name="LastGcInfo" type="Composite">
          <comp-member alias="MSCGcThreadCnt" name="GcThreadCount" type="gauge"/>
          <comp-member alias="MSCDuration" name="duration" type="gauge"/>
          <comp-member alias="MSCEndTime" name="endTime" type="gauge"/>
        </comp-attrib>
      </mbean>
      <mbean name="JVM GarbageCollector:ParNew" objectname="java.lang:type=GarbageCollector,name=ParNew">
        <attrib alias="ParNewCollCnt" name="CollectionCount" type="counter"/>
        <attrib alias="ParNewCollTime" name="CollectionTime" type="counter"/>
        <comp-attrib alias="ParNewLastGcInfo" name="LastGcInfo" type="Composite">
          <comp-member alias="ParNewGcThreadCnt" name="GcThreadCount" type="gauge"/>
          <comp-member alias="ParNewDuration" name="duration" type="gauge"/>
          <comp-member alias="ParNewEndTime" name="endTime" type="gauge"/>
        </comp-attrib>
      </mbean>
      <mbean name="JVM GarbageCollector:ConcurrentMarkSweep" objectname="java.lang:type=GarbageCollector,name=ConcurrentMarkSweep">
        <attrib alias="CMSCollCnt" name="CollectionCount" type="counter"/>
        <attrib alias="CMSCollTime" name="CollectionTime" type="counter"/>
        <comp-attrib alias="CMSLastGcInfo" name="LastGcInfo" type="Composite">
          <comp-member alias="CMSGcThreadCnt" name="GcThreadCount" type="gauge"/>
          <comp-member alias="CMSDuration" name="duration" type="gauge"/>
          <comp-member alias="CMSEndTime" name="endTime" type="gauge"/>
        </comp-attrib>
      </mbean>
      <mbean name="JVM GarbageCollector:PS MarkSweep" objectname="java.lang:type=GarbageCollector,name=PS MarkSweep">
        <attrib alias="PSMSCollCnt" name="CollectionCount" type="counter"/>
        <attrib alias="PSMSCollTime" name="CollectionTime" type="counter"/>
        <comp-attrib alias="PSMSLastGcInfo" name="LastGcInfo" type="Composite">
          <comp-member alias="PSMSGcThreadCnt" name="GcThreadCount" type="gauge"/>
          <comp-member alias="PSMSDuration" name="duration" type="gauge"/>
          <comp-member alias="PSMSEndTime" name="endTime" type="gauge"/>
        </comp-attrib>
      </mbean>
      <mbean name="JVM GarbageCollector:PS Scavenge" objectname="java.lang:type=GarbageCollector,name=PS Scavenge">
        <attrib alias="PSSCollCnt" name="CollectionCount" type="counter"/>
        <attrib alias="PSSCollTime" name="CollectionTime" type="counter"/>
        <comp-attrib alias="PSSLastGcInfo" name="LastGcInfo" type="Composite">
          <comp-member alias="PSSGcThreadCnt" name="GcThreadCount" type="gauge"/>
          <comp-member alias="PSSDuration" name="duration" type="gauge"/>
          <comp-member alias="PSSEndTime" name="endTime" type="gauge"/>
        </comp-attrib>
      </mbean>

    </mbeans>
  </jmx-collection>
</jmx-datacollection-config>