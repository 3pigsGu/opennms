<?xml version="1.0" encoding="ISO-8859-1"?>
<linkd-configuration threads="1" initial_sleep_time="30000"
                     snmp_poll_interval="3600000" discovery_link_interval="60000"
	                 use-lldp-discovery="true" 
		             use-bridge-discovery="true"
		             use-ospf-discovery="true"
                     use-cdp-discovery="false" 
                     use-ip-route-discovery="false"
                     force-ip-route-discovery-on-ethernet="false">

<!-- The following is very important:
if you want to walk ip route table 
on junos device you have to ad the junos
device oid in the following vendor element
Here are a list of the junos oids found
in India and sunnyvale lab
-->
    <iproutes>
        <vendor vendor_name="junos" sysoidRootMask=".1.3.6.1.4.1.2636.1.1.1"
                class-name="org.opennms.netmgt.linkd.snmp.IpCidrRouteTable">
       		<specific>2.9</specific>
       		<specific>2.10</specific>
       		<specific>2.11</specific>
       		<specific>2.20</specific>
       		<specific>2.25</specific>
       		<specific>2.29</specific>
       		<specific>2.30</specific>
       		<specific>2.31</specific>
       		<specific>2.41</specific>
       		<specific>2.57</specific>
       </vendor>
    </iproutes>    

    <vlans>
        <vendor vendor_name="Extreme Networks" sysoidRootMask=".1.3.6.1.4.1.1916"
                class-name="org.opennms.netmgt.linkd.snmp.ExtremeNetworkVlanTable">
            <specific>2.11</specific>
        </vendor>
    </vlans>

    <package name="example1">
		<filter>IPADDR != '0.0.0.0'</filter>
		<include-range begin="1.1.1.1" end="254.254.254.254"/>
    </package>
    
</linkd-configuration>
