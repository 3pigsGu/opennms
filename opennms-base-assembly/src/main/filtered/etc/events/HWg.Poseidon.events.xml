<?xml version="1.0" encoding="UTF-8"?><events xmlns="http://xmlns.opennms.org/xsd/eventconf">
    <event>
        <mask>
            <maskelement>
                <mename>id</mename>
                <mevalue>.1.3.6.1.4.1.21796.3.3</mevalue>
            </maskelement>
            <maskelement>
                <mename>generic</mename>
                <mevalue>6</mevalue>
            </maskelement>
            <maskelement>
                <mename>specific</mename>
                <mevalue>1</mevalue>
            </maskelement>
        </mask>
        <uei>uei.opennms.org/traps/POSEIDON-MIB/inpAlarmStateChanged</uei>
        <event-label>POSEIDON-MIB defined trap event: inpAlarmStateChanged</event-label>
        <descr>&lt;p&gt;Input state entered/left alarm condition.
		This trap always follows tsTrapAlarmStart/tsTrapAlarmEnd.&lt;/p&gt;&lt;table&gt;
	&lt;tr&gt;&lt;td&gt;&lt;b&gt;

	sysName&lt;/b&gt;&lt;/td&gt;&lt;td&gt;
	%parm[#1]%;&lt;/td&gt;&lt;td&gt;&lt;p&gt;&lt;/p&gt;&lt;/td&gt;&lt;/tr&gt;
	&lt;tr&gt;&lt;td&gt;&lt;b&gt;

	infoAddressMAC&lt;/b&gt;&lt;/td&gt;&lt;td&gt;
	%parm[#2]%;&lt;/td&gt;&lt;td&gt;&lt;p&gt;&lt;/p&gt;&lt;/td&gt;&lt;/tr&gt;
	&lt;tr&gt;&lt;td&gt;&lt;b&gt;

	inpName&lt;/b&gt;&lt;/td&gt;&lt;td&gt;
	%parm[#3]%;&lt;/td&gt;&lt;td&gt;&lt;p&gt;&lt;/p&gt;&lt;/td&gt;&lt;/tr&gt;
	&lt;tr&gt;&lt;td&gt;&lt;b&gt;

	inpValue&lt;/b&gt;&lt;/td&gt;&lt;td&gt;
	%parm[#4]%;&lt;/td&gt;&lt;td&gt;&lt;p&gt;
		off(0)
		on(1)
	&lt;/p&gt;&lt;/td&gt;&lt;/tr&gt;
	&lt;tr&gt;&lt;td&gt;&lt;b&gt;

	inpAlarmState&lt;/b&gt;&lt;/td&gt;&lt;td&gt;
	%parm[#5]%;&lt;/td&gt;&lt;td&gt;&lt;p&gt;
		normal(0)
		alarm(1)
	&lt;/p&gt;&lt;/td&gt;&lt;/tr&gt;&lt;/table&gt;</descr>
        <logmsg dest="logndisplay">&lt;p&gt;
	inpAlarmStateChanged trap received
	sysName=%parm[#1]%
	infoAddressMAC=%parm[#2]%
	inpName=%parm[#3]%
	inpValue=%parm[#4]%
	inpAlarmState=%parm[#5]%&lt;/p&gt;</logmsg>
        <severity>Indeterminate</severity>
        <varbindsdecode>
            <parmid>parm[#4]</parmid>
            <decode varbinddecodedstring="off" varbindvalue="0"/>
            <decode varbinddecodedstring="on" varbindvalue="1"/>
        </varbindsdecode>
        <varbindsdecode>
            <parmid>parm[#5]</parmid>
            <decode varbinddecodedstring="normal" varbindvalue="0"/>
            <decode varbinddecodedstring="alarm" varbindvalue="1"/>
        </varbindsdecode>
    </event>
    <event>
        <mask>
            <maskelement>
                <mename>id</mename>
                <mevalue>.1.3.6.1.4.1.21796.3.3</mevalue>
            </maskelement>
            <maskelement>
                <mename>generic</mename>
                <mevalue>6</mevalue>
            </maskelement>
            <maskelement>
                <mename>specific</mename>
                <mevalue>2</mevalue>
            </maskelement>
        </mask>
        <uei>uei.opennms.org/traps/POSEIDON-MIB/sensAlarmStateChanged</uei>
        <event-label>POSEIDON-MIB defined trap event: sensAlarmStateChanged</event-label>
        <descr>&lt;p&gt;Nth sensor state entered/left alarm condition.
		This trap always follows tsTrapAlarmStart/tsTrapAlarmEnd.&lt;/p&gt;&lt;table&gt;
	&lt;tr&gt;&lt;td&gt;&lt;b&gt;

	sysName&lt;/b&gt;&lt;/td&gt;&lt;td&gt;
	%parm[#1]%;&lt;/td&gt;&lt;td&gt;&lt;p&gt;&lt;/p&gt;&lt;/td&gt;&lt;/tr&gt;
	&lt;tr&gt;&lt;td&gt;&lt;b&gt;

	infoAddressMAC&lt;/b&gt;&lt;/td&gt;&lt;td&gt;
	%parm[#2]%;&lt;/td&gt;&lt;td&gt;&lt;p&gt;&lt;/p&gt;&lt;/td&gt;&lt;/tr&gt;
	&lt;tr&gt;&lt;td&gt;&lt;b&gt;

	sensName&lt;/b&gt;&lt;/td&gt;&lt;td&gt;
	%parm[#3]%;&lt;/td&gt;&lt;td&gt;&lt;p&gt;&lt;/p&gt;&lt;/td&gt;&lt;/tr&gt;
	&lt;tr&gt;&lt;td&gt;&lt;b&gt;

	sensID&lt;/b&gt;&lt;/td&gt;&lt;td&gt;
	%parm[#4]%;&lt;/td&gt;&lt;td&gt;&lt;p&gt;&lt;/p&gt;&lt;/td&gt;&lt;/tr&gt;
	&lt;tr&gt;&lt;td&gt;&lt;b&gt;

	sensState&lt;/b&gt;&lt;/td&gt;&lt;td&gt;
	%parm[#5]%;&lt;/td&gt;&lt;td&gt;&lt;p&gt;
		invalid(0)
		normal(1)
		alarmstate(2)
		alarm(3)
	&lt;/p&gt;&lt;/td&gt;&lt;/tr&gt;
	&lt;tr&gt;&lt;td&gt;&lt;b&gt;

	sensValue&lt;/b&gt;&lt;/td&gt;&lt;td&gt;
	%parm[#6]%;&lt;/td&gt;&lt;td&gt;&lt;p&gt;&lt;/p&gt;&lt;/td&gt;&lt;/tr&gt;
	&lt;tr&gt;&lt;td&gt;&lt;b&gt;

	sensUnit&lt;/b&gt;&lt;/td&gt;&lt;td&gt;
	%parm[#7]%;&lt;/td&gt;&lt;td&gt;&lt;p&gt;
		celsius(0)
		fahrenheit(1)
		kelvin(2)
		percent(3)
		volt(4)
		miliAmper(5)
		noUnit(6)
		pulse(7)
		switch(8)
		dewPoint(9)
		absoluteHumidity(10)
		pressure(11)
		universal(12)
	&lt;/p&gt;&lt;/td&gt;&lt;/tr&gt;&lt;/table&gt;</descr>
        <logmsg dest="logndisplay">&lt;p&gt;
	sensAlarmStateChanged trap received
	sysName=%parm[#1]%
	infoAddressMAC=%parm[#2]%
	sensName=%parm[#3]%
	sensID=%parm[#4]%
	sensState=%parm[#5]%
	sensValue=%parm[#6]%
	sensUnit=%parm[#7]%&lt;/p&gt;</logmsg>
        <severity>Indeterminate</severity>
        <varbindsdecode>
            <parmid>parm[#5]</parmid>
            <decode varbinddecodedstring="invalid" varbindvalue="0"/>
            <decode varbinddecodedstring="normal" varbindvalue="1"/>
            <decode varbinddecodedstring="alarmstate" varbindvalue="2"/>
            <decode varbinddecodedstring="alarm" varbindvalue="3"/>
        </varbindsdecode>
        <varbindsdecode>
            <parmid>parm[#7]</parmid>
            <decode varbinddecodedstring="celsius" varbindvalue="0"/>
            <decode varbinddecodedstring="fahrenheit" varbindvalue="1"/>
            <decode varbinddecodedstring="kelvin" varbindvalue="2"/>
            <decode varbinddecodedstring="percent" varbindvalue="3"/>
            <decode varbinddecodedstring="volt" varbindvalue="4"/>
            <decode varbinddecodedstring="miliAmper" varbindvalue="5"/>
            <decode varbinddecodedstring="noUnit" varbindvalue="6"/>
            <decode varbinddecodedstring="pulse" varbindvalue="7"/>
            <decode varbinddecodedstring="switch" varbindvalue="8"/>
            <decode varbinddecodedstring="dewPoint" varbindvalue="9"/>
            <decode varbinddecodedstring="absoluteHumidity" varbindvalue="10"/>
            <decode varbinddecodedstring="pressure" varbindvalue="11"/>
            <decode varbinddecodedstring="universal" varbindvalue="12"/>
        </varbindsdecode>
    </event>
    <event>
        <mask>
            <maskelement>
                <mename>id</mename>
                <mevalue>.1.3.6.1.4.1.21796.3.3</mevalue>
