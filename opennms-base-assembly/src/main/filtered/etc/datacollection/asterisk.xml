<?xml version="1.0" encoding="UTF-8" standalone="no"?><datacollection-group name="Asterisk">

    <!-- Asterisk Resource Types -->

    <resourceType label="Asterisk Channel Type" name="astChanType" resourceLabel="${astChanTypeName}">
       <persistenceSelectorStrategy class="org.opennms.netmgt.collectd.PersistAllSelectorStrategy"/>
      <storageStrategy class="org.opennms.netmgt.dao.support.SiblingColumnStorageStrategy">
        <parameter key="sibling-column-name" value="astChanTypeName"/>
		<parameter key="replace-all" value="s/\s+//"/>
      </storageStrategy>
    </resourceType>

      <!-- Asterisk (Digium) MIBs -->

      <group ifType="ignore" name="asterisk-scalars">
        <mibObj alias="astNumChannels" instance="0" oid=".1.3.6.1.4.1.22736.1.5.1" type="gauge"/>
        <mibObj alias="astNumChanBridge" instance="0" oid=".1.3.6.1.4.1.22736.1.5.5.1" type="gauge"/>
        <mibObj alias="astCfgCallsActive" instance="0" oid=".1.3.6.1.4.1.22736.1.2.5" type="gauge"/>
        <mibObj alias="astCfgCallsPrcessed" instance="0" oid=".1.3.6.1.4.1.22736.1.2.6" type="counter"/>
      </group>

      <group ifType="all" name="asterisk-chantype">
        <mibObj alias="astChanTypeName" instance="astChanType" oid=".1.3.6.1.4.1.22736.1.5.4.1.2" type="string"/>
        <mibObj alias="astChanTypeChannels" instance="astChanType" oid=".1.3.6.1.4.1.22736.1.5.4.1.7" type="gauge"/>
      </group>

      <systemDef name="Asterisk">
        <sysoid>.1.3.6.1.4.1.22736.1</sysoid>
	<collect>
	  <includeGroup>mib2-host-resources-system</includeGroup>
	  <includeGroup>mib2-host-resources-memory</includeGroup>
	  <includeGroup>net-snmp-disk</includeGroup>
	  <includeGroup>net-snmp-disk-more</includeGroup>
	  <includeGroup>net-snmp-disk-highlow</includeGroup>
	  <includeGroup>ucd-loadavg</includeGroup>
	  <includeGroup>ucd-memory</includeGroup>
	  <includeGroup>ucd-sysstat</includeGroup>
	  <includeGroup>ucd-sysstat-raw</includeGroup>
	  <includeGroup>ucd-sysstat-raw-more</includeGroup>
	  <includeGroup>asterisk-scalars</includeGroup>
	  <includeGroup>asterisk-chantype</includeGroup>
	</collect>
      </systemDef>

</datacollection-group>