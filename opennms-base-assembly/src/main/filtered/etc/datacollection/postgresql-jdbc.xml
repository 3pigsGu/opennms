<?xml version="1.0" encoding="UTF-8" standalone="no"?><datacollection-group name="PostgreSQL-JDBC">

    <resourceType label="PostgreSQL Tablespace" name="pgTableSpace" resourceLabel="${spcname}">
      <persistenceSelectorStrategy class="org.opennms.netmgt.collectd.PersistAllSelectorStrategy"/>
      <storageStrategy class="org.opennms.netmgt.dao.support.IndexStorageStrategy"/>
    </resourceType>
    <resourceType label="PostgreSQL Database" name="pgDatabase" resourceLabel="${datname}">
      <persistenceSelectorStrategy class="org.opennms.netmgt.collectd.PersistAllSelectorStrategy"/>
      <storageStrategy class="org.opennms.netmgt.dao.support.IndexStorageStrategy"/>
    </resourceType>

</datacollection-group>