<?xml version="1.0" encoding="UTF-8" standalone="no"?><datacollection-group name="HWg">

    <!-- HWg Resource Types -->
    <resourceType label="HWg Sensor" name="sensIndex" resourceLabel="${sensName} (${index})">
      <persistenceSelectorStrategy class="org.opennms.netmgt.collectd.PersistAllSelectorStrategy"/>
      <storageStrategy class="org.opennms.netmgt.dao.support.IndexStorageStrategy"/>
    </resourceType>

      <group ifType="all" name="HWg-sensor">
        <mibObj alias="sensName" instance="sensIndex" oid=".1.3.6.1.4.1.21796.4.1.3.1.2" type="string"/>
        <mibObj alias="sensState" instance="sensIndex" oid=".1.3.6.1.4.1.21796.4.1.3.1.3" type="INTEGER"/>
        <mibObj alias="sensString" instance="sensIndex" oid=".1.3.6.1.4.1.21796.4.1.3.1.4" type="octetstring"/>
        <mibObj alias="sensValue" instance="sensIndex" oid=".1.3.6.1.4.1.21796.4.1.3.1.5" type="INTEGER"/>
        <mibObj alias="sensSN" instance="sensIndex" oid=".1.3.6.1.4.1.21796.4.1.3.1.6" type="string"/>
        <mibObj alias="sensUnit" instance="sensIndex" oid=".1.3.6.1.4.1.21796.4.1.3.1.7" type="INTEGER"/>
        <mibObj alias="sensID" instance="sensIndex" oid=".1.3.6.1.4.1.21796.4.1.3.1.8" type="INTEGER"/>
      </group>

      <group ifType="all" name="poseidon-sensTable">
        <mibObj alias="sensName" instance="sensIndex" oid=".1.3.6.1.4.1.21796.3.3.3.1.2" type="string"/>
        <mibObj alias="sensValue" instance="sensIndex" oid=".1.3.6.1.4.1.21796.3.3.3.1.6" type="integer"/>
        <mibObj alias="sensUnitString" instance="sensIndex" oid=".1.3.6.1.4.1.21796.3.3.3.1.10" type="string"/>
      </group>

      <group ifType="all" name="poseidon-sensSetupTable">
        <mibObj alias="sensLimitMin" instance="sensIndex" oid=".1.3.6.1.4.1.21796.3.3.99.1.2.1.6" type="integer"/>
        <mibObj alias="sensLimitMax" instance="sensIndex" oid=".1.3.6.1.4.1.21796.3.3.99.1.2.1.7" type="integer"/>
      </group>

      <systemDef name="HWg-sensor">
        <sysoidMask>.1.3.6.1.4.1.21796.</sysoidMask>
        <collect>
          <includeGroup>HWg-sensor</includeGroup>
        </collect>
      </systemDef>

      <systemDef name="HW Group Poseidon">
        <sysoidMask>.1.3.6.1.4.1.21796.</sysoidMask>
        <collect>
            <includeGroup>poseidon-sensTable</includeGroup>
            <includeGroup>poseidon-sensSetupTable</includeGroup>
        </collect>
      </systemDef>


</datacollection-group>