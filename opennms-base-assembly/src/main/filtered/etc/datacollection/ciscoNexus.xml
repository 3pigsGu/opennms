<?xml version="1.0" encoding="UTF-8" standalone="no"?><datacollection-group name="Cisco Nexus">

    <resourceType label="Cisco Nexus Environmental" name="ciscoNXEnvMonStatusIndex" resourceLabel="${entPhysicalName} (index ${index})">
      <persistenceSelectorStrategy class="org.opennms.netmgt.collectd.PersistRegexSelectorStrategy">
        <parameter key="match-expression" value="(#entPhysicalName != null) and (#entSensorType != null)"/>
      </persistenceSelectorStrategy>
      <storageStrategy class="org.opennms.netmgt.dao.support.IndexStorageStrategy"/>
    </resourceType>

    <resourceType label="Cisco Nexus System Resourses" name="ciscoNXProcessIndex" resourceLabel="${entPhysicalName} (index ${index})">
      <persistenceSelectorStrategy class="org.opennms.netmgt.collectd.PersistRegexSelectorStrategy">
        <parameter key="match-expression" value="(#cpmCPUTotalPhysicalIndex != null)"/>
      </persistenceSelectorStrategy>
      <storageStrategy class="org.opennms.netmgt.dao.support.IndexStorageStrategy"/>
    </resourceType>

      <group ifType="all" name="cisco-nx-environmental">
        <mibObj alias="entPhysicalName" instance="ciscoNXEnvMonStatusIndex" oid=".1.3.6.1.2.1.47.1.1.1.1.7" type="string"/>
        <mibObj alias="entSensorType" instance="ciscoNXEnvMonStatusIndex" oid=".1.3.6.1.4.1.9.9.91.1.1.1.1.1" type="string"/>
        <mibObj alias="entSensorValue" instance="ciscoNXEnvMonStatusIndex" oid=".1.3.6.1.4.1.9.9.91.1.1.1.1.4" type="gauge"/>
      </group>

      <!-- Stats from the CISCO-PROCESS-MIB -->
      <group ifType="ignore" name="cisco-process">
        <mibObj alias="cpmCPUTotal5minRev" instance="1" oid=".1.3.6.1.4.1.9.9.109.1.1.1.1.8" type="Gauge32"/>
        <mibObj alias="cpmCPUMemoryUsed" instance="1" oid=".1.3.6.1.4.1.9.9.109.1.1.1.1.12" type="Gauge32"/>
        <mibObj alias="cpmCPUMemoryFree" instance="1" oid=".1.3.6.1.4.1.9.9.109.1.1.1.1.13" type="Gauge32"/>
      </group>

      <!-- Stats from the CISCO-PROCESS-MIB -->
      <group ifType="ignore" name="cisco-nx-process-index">
        <mibObj alias="cpmCPUTotal5minRev" instance="ciscoNXProcessIndex" oid=".1.3.6.1.4.1.9.9.109.1.1.1.1.8" type="Gauge32"/>
        <mibObj alias="cpmCPUMemoryUsed" instance="ciscoNXProcessIndex" oid=".1.3.6.1.4.1.9.9.109.1.1.1.1.12" type="Gauge32"/>
        <mibObj alias="cpmCPUMemoryFree" instance="ciscoNXProcessIndex" oid=".1.3.6.1.4.1.9.9.109.1.1.1.1.13" type="Gauge32"/>
        <mibObj alias="entPhysicalName" instance="ciscoNXProcessIndex" oid=".1.3.6.1.2.1.47.1.1.1.1.7" type="string"/>
        <mibObj alias="cpmCPUTotalPhysicalIndex" instance="ciscoNXProcessIndex" oid=".1.3.6.1.4.1.9.9.109.1.1.1.1.2" type="string"/>
      </group>

      <systemDef name="Cisco Nexus 1000v"><!-- Nexus1000V chassis -->
        <sysoid>.1.3.6.1.4.1.9.12.3.1.3.840</sysoid>
        <collect>
          <includeGroup>cisco-process</includeGroup>
          <includeGroup>mib2-X-interfaces</includeGroup>
          <includeGroup>cisco-nx-environmental</includeGroup>
        </collect>
      </systemDef>

      <systemDef name="Cisco Nexus 1010"><!-- Nexus1010 Chassis -->
