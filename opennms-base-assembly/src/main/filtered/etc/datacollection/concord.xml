<?xml version="1.0" encoding="UTF-8" standalone="no"?><datacollection-group name="Concord">

    <resourceType label="SystemEDGE Process Group Monitor" name="pgmonIndex" resourceLabel="${pgmonDescr} (index ${index})">
      <persistenceSelectorStrategy class="org.opennms.netmgt.collectd.PersistAllSelectorStrategy"/>
      <storageStrategy class="org.opennms.netmgt.dao.support.IndexStorageStrategy"/>
    </resourceType>

      <!-- Concord Communications MIBs -->
      <group ifType="ignore" name="netHealth-loadavg">
        <mibObj alias="nhLoadavg1" instance="0" oid=".1.3.6.1.4.1.546.1.1.7.8.26" type="integer"/>
        <mibObj alias="nhLoadavg5" instance="0" oid=".1.3.6.1.4.1.546.1.1.7.8.27" type="integer"/>
        <mibObj alias="nhLoadavg15" instance="0" oid=".1.3.6.1.4.1.546.1.1.7.8.28" type="integer"/>
      </group>

      <group ifType="ignore" name="netHealth-memory">
        <mibObj alias="nhMemAvailSwap" instance="0" oid=".1.3.6.1.4.1.546.1.1.7.8.32" type="integer"/>
        <mibObj alias="nhMemAvailReal" instance="0" oid=".1.3.6.1.4.1.546.1.1.7.8.31" type="integer"/>
        <mibObj alias="nhMemTotalFree" instance="0" oid=".1.3.6.1.4.1.546.1.1.7.8.32" type="integer"/>
      </group>

      <group ifType="ignore" name="netHealth-sysstat">
        <mibObj alias="nhSwapIn" instance="0" oid=".1.3.6.1.4.1.546.1.1.7.8.18" type="integer"/>
        <mibObj alias="nhSwapOut" instance="0" oid=".1.3.6.1.4.1.546.1.1.7.8.19" type="integer"/>
        <mibObj alias="nhSysInterrupts" instance="0" oid=".1.3.6.1.4.1.546.1.1.7.8.17" type="integer"/>
        <mibObj alias="nhSysContext" instance="0" oid=".1.3.6.1.4.1.546.1.1.7.8.16" type="integer"/>
        <mibObj alias="nhCpuRawUser" instance="0" oid=".1.3.6.1.4.1.546.13.3" type="counter"/>
        <mibObj alias="nhCpuRawSystem" instance="0" oid=".1.3.6.1.4.1.546.13.4" type="counter"/>
        <mibObj alias="nhCpuRawKernel" instance="0" oid=".1.3.6.1.4.1.546.13.4" type="counter"/>
        <mibObj alias="nhCpuTotalIdlePct" instance="0" oid=".1.3.6.1.4.1.546.13.7" type="gauge"/>
        <mibObj alias="nhCpuTotalUserPct" instance="0" oid=".1.3.6.1.4.1.546.13.8" type="gauge"/>
        <mibObj alias="nhCpuTotalSysPct" instance="0" oid=".1.3.6.1.4.1.546.13.9" type="gauge"/>
        <mibObj alias="nhCpuTotalWaitPct" instance="0" oid=".1.3.6.1.4.1.546.13.10" type="gauge"/>
      </group>

      <group ifType="all" name="netHealth-pgmon">
        <mibObj alias="pgmonDescr" instance="pgmonIndex" oid=".1.3.6.1.4.1.546.15.10.1.2" type="string"/>
        <mibObj alias="pgmonNumProcs" instance="pgmonIndex" oid=".1.3.6.1.4.1.546.15.10.1.6" type="gauge"/>
        <mibObj alias="pgmonRSS" instance="pgmonIndex" oid=".1.3.6.1.4.1.546.15.10.1.14" type="gauge"/>
        <mibObj alias="pgmonSize" instance="pgmonIndex" oid=".1.3.6.1.4.1.546.15.10.1.15" type="gauge"/>
        <mibObj alias="pgmonThreadCount" instance="pgmonIndex" oid=".1.3.6.1.4.1.546.15.10.1.16" type="gauge"/>
        <mibObj alias="pgmonMEM" instance="pgmonIndex" oid=".1.3.6.1.4.1.546.15.10.1.17" type="gauge"/>
        <mibObj alias="pgmonMajorPgFlts" instance="pgmonIndex" oid=".1.3.6.1.4.1.546.15.10.1.24" type="counter"/>
      </group>

      <systemDef name="Concord Net Health">
        <sysoidMask>.1.3.6.1.4.1.546.</sysoidMask>
        <collect>
          <includeGroup>mib2-host-resources-system</includeGroup>
          <includeGroup>mib2-host-resources-memory</includeGroup>
          <includeGroup>netHealth-loadavg</includeGroup>
          <includeGroup>netHealth-memory</includeGroup>
          <includeGroup>netHealth-sysstat</includeGroup>
          <includeGroup>netHealth-pgmon</includeGroup>
        </collect>
      </systemDef>

</datacollection-group>