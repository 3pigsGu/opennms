<?xml version="1.0" encoding="UTF-8" standalone="no"?><datacollection-group name="Isilon">

  <resourceType label="Isilon Node Traffic by Protocol" name="isiProtocolName" resourceLabel="${protocolName} Stats">
    <persistenceSelectorStrategy class="org.opennms.netmgt.collectd.PersistAllSelectorStrategy"/>
    <storageStrategy class="org.opennms.netmgt.dao.support.IndexStorageStrategy"/>
  </resourceType>

  <resourceType label="Isilon Node Disk Bays" name="isiDiskPerfBay" resourceLabel="Disk ${deviceName}">
    <persistenceSelectorStrategy class="org.opennms.netmgt.collectd.PersistAllSelectorStrategy"/>
    <storageStrategy class="org.opennms.netmgt.dao.support.IndexStorageStrategy"/>
  </resourceType>

  <resourceType label="Isilon Node Fans" name="isiFanNumber" resourceLabel="${fanDescription}">
    <persistenceSelectorStrategy class="org.opennms.netmgt.collectd.PersistAllSelectorStrategy"/>
    <storageStrategy class="org.opennms.netmgt.dao.support.IndexStorageStrategy"/>
  </resourceType>

  <resourceType label="Isilon Node Temp Sensors" name="isiTempSensorNum" resourceLabel="${tempSensorDesc}">
    <persistenceSelectorStrategy class="org.opennms.netmgt.collectd.PersistAllSelectorStrategy"/>
    <storageStrategy class="org.opennms.netmgt.dao.support.IndexStorageStrategy"/>
  </resourceType>

  <group ifType="ignore" name="isilon-cluster">
    <mibObj alias="nodeCount" instance="0" oid=".1.3.6.1.4.1.12124.1.1.4" type="Integer32"/>
    <mibObj alias="ifsInBytes" instance="0" oid=".1.3.6.1.4.1.12124.1.2.1.1" type="Counter64"/>
    <mibObj alias="ifsOutBytes" instance="0" oid=".1.3.6.1.4.1.12124.1.2.1.3" type="Counter64"/>
    <mibObj alias="networkInBytes" instance="0" oid=".1.3.6.1.4.1.12124.1.2.2.1" type="Counter64"/>
    <mibObj alias="networkOutBytes" instance="0" oid=".1.3.6.1.4.1.12124.1.2.2.3" type="Counter64"/>
    <mibObj alias="ifsTotalBytes" instance="0" oid=".1.3.6.1.4.1.12124.1.3.1" type="Gauge64"/>
    <mibObj alias="ifsUsedBytes" instance="0" oid=".1.3.6.1.4.1.12124.1.3.2" type="Gauge64"/>
    <mibObj alias="ifsAvailableBytes" instance="0" oid=".1.3.6.1.4.1.12124.1.3.3" type="Gauge64"/>
  </group>

  <group ifType="ignore" name="isilon-node-network-protocol">
    <mibObj alias="protocolName" instance="isiProtocolName" oid=".1.3.6.1.4.1.12124.2.2.10.1.1" type="String"/>
    <mibObj alias="protocolOpCount" instance="isiProtocolName" oid=".1.3.6.1.4.1.12124.2.2.10.1.2" type="Gauge32"/>
    <mibObj alias="inMinBytes" instance="isiProtocolName" oid=".1.3.6.1.4.1.12124.2.2.10.1.4" type="Gauge32"/>
    <mibObj alias="inMaxBytes" instance="isiProtocolName" oid=".1.3.6.1.4.1.12124.2.2.10.1.5" type="Gauge32"/>
    <mibObj alias="inAvgBytes" instance="isiProtocolName" oid=".1.3.6.1.4.1.12124.2.2.10.1.6" type="Gauge32"/>
    <mibObj alias="inStdDevBytes" instance="isiProtocolName" oid=".1.3.6.1.4.1.12124.2.2.10.1.7" type="Gauge32"/>
    <mibObj alias="outMinBytes" instance="isiProtocolName" oid=".1.3.6.1.4.1.12124.2.2.10.1.9" type="Gauge32"/>
    <mibObj alias="outMaxBytes" instance="isiProtocolName" oid=".1.3.6.1.4.1.12124.2.2.10.1.10" type="Gauge32"/>
