    <mibObj alias="latencyMax" instance="isiProtocolName" oid=".1.3.6.1.4.1.12124.2.2.10.1.15" type="Gauge32"/>
    <mibObj alias="latencyAverage" instance="isiProtocolName" oid=".1.3.6.1.4.1.12124.2.2.10.1.16" type="Gauge32"/>
    <mibObj alias="latencyStdDev" instance="isiProtocolName" oid=".1.3.6.1.4.1.12124.2.2.10.1.17" type="Gauge32"/>
  </group>

  <group ifType="ignore" name="isilon-node-disk-perfbay">
    <mibObj alias="bay" instance="isiDiskPerfBay" oid=".1.3.6.1.4.1.12124.2.2.52.1.1" type="Integer32"/>
    <mibObj alias="deviceName" instance="isiDiskPerfBay" oid=".1.3.6.1.4.1.12124.2.2.52.1.2" type="String"/>
    <mibObj alias="opsPerSec" instance="isiDiskPerfBay" oid=".1.3.6.1.4.1.12124.2.2.52.1.3" type="Gauge32"/>
    <mibObj alias="inBitsPerSec" instance="isiDiskPerfBay" oid=".1.3.6.1.4.1.12124.2.2.52.1.4" type="Gauge32"/>
    <mibObj alias="outBitsPerSec" instance="isiDiskPerfBay" oid=".1.3.6.1.4.1.12124.2.2.52.1.5" type="Gauge32"/>
  </group>

  <group ifType="ignore" name="isilon-node-fan">
    <mibObj alias="fanNumber" instance="isiFanNumber" oid=".1.3.6.1.4.1.12124.2.53.1.1" type="Integer32"/>
    <mibObj alias="fanName" instance="isiFanNumber" oid=".1.3.6.1.4.1.12124.2.53.1.2" type="string"/>
    <mibObj alias="fanDescription" instance="isiFanNumber" oid=".1.3.6.1.4.1.12124.2.53.1.3" type="string"/>
    <mibObj alias="fanSpeed" instance="isiFanNumber" oid=".1.3.6.1.4.1.12124.2.53.1.4" type="Gauge32"/>
  </group>

  <systemDef name="Isilon Storage">
    <sysoid>.1.3.6.1.4.1.12124.1</sysoid>
    <collect>
      <includeGroup>isilon-cluster</includeGroup>
      <includeGroup>isilon-node-network-protocol</includeGroup>
      <includeGroup>isilon-node-disk-perfbay</includeGroup>
      <includeGroup>isilon-node-fan</includeGroup>
      <includeGroup>isilon-node-tempsensor</includeGroup>
    </collect>
  </systemDef>

</datacollection-group>