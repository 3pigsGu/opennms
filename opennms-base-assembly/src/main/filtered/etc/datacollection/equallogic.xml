        <mibObj alias="EqVolTx" instance="EqVol" oid=".1.3.6.1.4.1.12740.5.1.7.34.1.3" type="counter"/>
        <mibObj alias="EqVolRx" instance="EqVol" oid=".1.3.6.1.4.1.12740.5.1.7.34.1.4" type="counter"/>
        <mibObj alias="EqVolReLat" instance="EqVol" oid=".1.3.6.1.4.1.12740.5.1.7.34.1.6" type="counter"/>
        <mibObj alias="EqVolWrLat" instance="EqVol" oid=".1.3.6.1.4.1.12740.5.1.7.34.1.7" type="counter"/>
        <mibObj alias="EqVolReOp" instance="EqVol" oid=".1.3.6.1.4.1.12740.5.1.7.34.1.8" type="counter"/>
        <mibObj alias="EqVolWrOp" instance="EqVol" oid=".1.3.6.1.4.1.12740.5.1.7.34.1.9" type="counter"/>
        <mibObj alias="EqVolSize" instance="EqVol" oid=".1.3.6.1.4.1.12740.5.1.7.1.1.8" type="integer"/>
        <mibObj alias="EqVolThnResPc" instance="EqVol" oid=".1.3.6.1.4.1.12740.5.1.7.1.1.27" type="integer"/>
        <mibObj alias="EqVolAlloc" instance="EqVol" oid=".1.3.6.1.4.1.12740.5.1.7.7.1.13" type="integer"/>
      </group>

      <systemDef name="Equallogic SANs">
        <sysoidMask>.1.3.6.1.4.1.12740.</sysoidMask>
        <collect>
         <includeGroup>equallogic-volumes</includeGroup>
        </collect>
      </systemDef>

</datacollection-group>