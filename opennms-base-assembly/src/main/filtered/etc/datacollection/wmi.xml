<?xml version="1.0" encoding="UTF-8" standalone="no"?><datacollection-group name="WMI">

     <!-- Resource Type for WMI TCP/IP Network Interfaces -->
     <resourceType label="Windows TCP/IP Network Interface" name="wmiTcpipNetworkInterface" resourceLabel="${wmiIpNetIfName}">
      <persistenceSelectorStrategy class="org.opennms.netmgt.collectd.PersistAllSelectorStrategy"/>
      <storageStrategy class="org.opennms.netmgt.dao.support.IndexStorageStrategy"/>
    </resourceType>

     <!-- Resource Type for WMI CPUs -->
     <resourceType label="Windows Processor" name="wmiOSCpu" resourceLabel="CPU ${wmiOSCpuName}">
      <persistenceSelectorStrategy class="org.opennms.netmgt.collectd.PersistAllSelectorStrategy"/>
      <storageStrategy class="org.opennms.netmgt.dao.support.IndexStorageStrategy"/>
    </resourceType>

     <!-- Resource Type for WMI Physical Disks -->
     <resourceType label="Windows Physical Disk" name="wmiPhysicalDisk" resourceLabel="${wmiPDName}">
      <persistenceSelectorStrategy class="org.opennms.netmgt.collectd.PersistAllSelectorStrategy"/>
      <storageStrategy class="org.opennms.netmgt.dao.support.IndexStorageStrategy"/>
    </resourceType>

     <!-- Resource Type for WMI Logical Disks -->
     <resourceType label="Windows Logical Disk" name="wmiLogicalDisk" resourceLabel="${wmiLDName}">
      <persistenceSelectorStrategy class="org.opennms.netmgt.collectd.PersistAllSelectorStrategy"/>
      <storageStrategy class="org.opennms.netmgt.dao.support.IndexStorageStrategy"/>
    </resourceType>

     <!-- Resource Type for WMI Web Requests -->
     <resourceType label="Windows Web Requests" name="wmiW3" resourceLabel="${wmiW3Name}">
      <persistenceSelectorStrategy class="org.opennms.netmgt.collectd.PersistAllSelectorStrategy"/>
      <storageStrategy class="org.opennms.netmgt.dao.support.IndexStorageStrategy"/>
    </resourceType>

</datacollection-group>