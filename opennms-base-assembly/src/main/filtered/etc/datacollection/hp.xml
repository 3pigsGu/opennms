<?xml version="1.0" encoding="UTF-8" standalone="no"?><datacollection-group name="HP">

    <!-- HP-UX fileSystemTable resource type -->
    <resourceType label="HP-UX Filesystem" name="hpuxFSTable" resourceLabel="${hpuxFSName} on ${hpuxFSDir}">
      <persistenceSelectorStrategy class="org.opennms.netmgt.collectd.PersistAllSelectorStrategy"/>
      <storageStrategy class="org.opennms.netmgt.dao.support.SiblingColumnStorageStrategy">
        <parameter key="sibling-column-name" value="hpuxFSDir"/>
		<parameter key="replace-first" value="s/^-$/_root_fs/"/>
		<parameter key="replace-all" value="s/^-//"/>
		<parameter key="replace-all" value="s/\s//"/>
      </storageStrategy>
    </resourceType>

      <!-- Hewlett-Packard MIBs -->
      <group ifType="ignore" name="compaq-insight-manager-stats">
        <mibObj alias="cimPctHddUsed1" instance="0" oid=".1.3.6.1.4.1.232.11.2.4.1.1.5" type="integer"/>
        <mibObj alias="cimPctHddUsed2" instance="1" oid=".1.3.6.1.4.1.232.11.2.4.1.1.5" type="integer"/>
        <mibObj alias="cim5MinCpuUtilPct" instance="0" oid=".1.3.6.1.4.1.232.11.2.3.1.1.3" type="integer"/>
      </group>

      <group ifType="ignore" name="hp-procurve">
        <mibObj alias="hpSwitchCpuStat" instance="0" oid=".1.3.6.1.4.1.11.2.14.11.5.1.9.6.1" type="gauge"/>
        <mibObj alias="hpMsgBufCorrupt" instance="1" oid=".1.3.6.1.4.1.11.2.14.11.5.1.1.1.1.1.1.2" type="gauge"/>
        <mibObj alias="hpMsgBufFree" instance="1" oid=".1.3.6.1.4.1.11.2.14.11.5.1.1.1.1.1.1.3" type="gauge"/>
        <mibObj alias="hpMsgBufInit" instance="1" oid=".1.3.6.1.4.1.11.2.14.11.5.1.1.1.1.1.1.4" type="gauge"/>
        <mibObj alias="hpMsgBufMin" instance="1" oid=".1.3.6.1.4.1.11.2.14.11.5.1.1.1.1.1.1.5" type="gauge"/>
        <mibObj alias="hpMsgBufMiss" instance="1" oid=".1.3.6.1.4.1.11.2.14.11.5.1.1.1.1.1.1.6" type="gauge"/>
        <mibObj alias="hpMsgBufSize" instance="1" oid=".1.3.6.1.4.1.11.2.14.11.5.1.1.1.1.1.1.7" type="integer"/>
        <mibObj alias="hpSwitchAllocMem" instance="1" oid=".1.3.6.1.4.1.11.2.14.11.5.1.1.2.1.1.1.4" type="gauge"/>
        <mibObj alias="hpSwitchTotalMem" instance="1" oid=".1.3.6.1.4.1.11.2.14.11.5.1.1.2.1.1.1.5" type="integer"/>
        <mibObj alias="hpSwitchFreeMem" instance="1" oid=".1.3.6.1.4.1.11.2.14.11.5.1.1.2.1.1.1.6" type="gauge"/>
      </group>

      <group ifType="ignore" name="hpux">
        <mibObj alias="hpuxNumberOfUsers" instance="0" oid=".1.3.6.1.4.1.11.2.3.1.1.2" type="gauge"/>
        <mibObj alias="hpuxFreeMemory" instance="0" oid=".1.3.6.1.4.1.11.2.3.1.1.7" type="gauge"/>
        <mibObj alias="hpuxPhysMemory" instance="0" oid=".1.3.6.1.4.1.11.2.3.1.1.8" type="integer"/>
        <mibObj alias="hpuxSwapConfigured" instance="0" oid=".1.3.6.1.4.1.11.2.3.1.1.10" type="integer"/>
        <mibObj alias="hpuxSwapEnabled" instance="0" oid=".1.3.6.1.4.1.11.2.3.1.1.11" type="gauge"/>
        <mibObj alias="hpuxSwapFree" instance="0" oid=".1.3.6.1.4.1.11.2.3.1.1.12" type="gauge"/>
        <mibObj alias="hpuxCpuUsedByUsers" instance="0" oid=".1.3.6.1.4.1.11.2.3.1.1.13" type="counter"/>
        <mibObj alias="hpuxCpuUsedBySys" instance="0" oid=".1.3.6.1.4.1.11.2.3.1.1.14" type="counter"/>
        <mibObj alias="hpuxCpuIdle" instance="0" oid=".1.3.6.1.4.1.11.2.3.1.1.15" type="counter"/>
        <mibObj alias="hpuxCpuNice" instance="0" oid=".1.3.6.1.4.1.11.2.3.1.1.16" type="counter"/>
        <mibObj alias="hpuxTotalFsMounted" instance="0" oid=".1.3.6.1.4.1.11.2.3.1.2.1" type="gauge"/>
        <mibObj alias="hpuxTotalProcesses" instance="0" oid=".1.3.6.1.4.1.11.2.3.1.4.1" type="gauge"/>
      </group>

      <group ifType="all" name="hpux-filesystem">
        <mibObj alias="hpuxFSID1" instance="hpuxFSTable" oid=".1.3.6.1.4.1.11.2.3.1.2.2.1.1" type="string"/>
        <mibObj alias="hpuxFSID2" instance="hpuxFSTable" oid=".1.3.6.1.4.1.11.2.3.1.2.2.1.2" type="string"/>
        <mibObj alias="hpuxFSName" instance="hpuxFSTable" oid=".1.3.6.1.4.1.11.2.3.1.2.2.1.3" type="string"/>
        <mibObj alias="hpuxFSBlock" instance="hpuxFSTable" oid=".1.3.6.1.4.1.11.2.3.1.2.2.1.4" type="gauge"/>
        <mibObj alias="hpuxFSBfree" instance="hpuxFSTable" oid=".1.3.6.1.4.1.11.2.3.1.2.2.1.5" type="gauge"/>
        <mibObj alias="hpuxFSBavail" instance="hpuxFSTable" oid=".1.3.6.1.4.1.11.2.3.1.2.2.1.6" type="gauge"/>
        <mibObj alias="hpuxFSBsize" instance="hpuxFSTable" oid=".1.3.6.1.4.1.11.2.3.1.2.2.1.7" type="gauge"/>
        <mibObj alias="hpuxFSFiles" instance="hpuxFSTable" oid=".1.3.6.1.4.1.11.2.3.1.2.2.1.8" type="gauge"/>
        <mibObj alias="hpuxFSFfree" instance="hpuxFSTable" oid=".1.3.6.1.4.1.11.2.3.1.2.2.1.9" type="gauge"/>
        <mibObj alias="hpuxFSDir" instance="hpuxFSTable" oid=".1.3.6.1.4.1.11.2.3.1.2.2.1.10" type="string"/>
      </group>

      <systemDef name="HP Printers">
        <sysoid>.1.3.6.1.4.1.11.2.3.9.1</sysoid>
        <collect>
          <includeGroup>printer-usage</includeGroup>
          <includeGroup>printer-mib-supplies</includeGroup>
        </collect>
      </systemDef>

      <systemDef name="HP ProCurve">
        <sysoidMask>.1.3.6.1.4.1.11.2.3.7.11.</sysoidMask>
        <collect>
          <includeGroup>hp-procurve</includeGroup>
        </collect>
      </systemDef>

      <systemDef name="HP-UX">
        <sysoidMask>.1.3.6.1.4.1.11.2.3.2.</sysoidMask>
        <collect>
          <includeGroup>hpux</includeGroup>
          <includeGroup>hpux-filesystem</includeGroup>
        </collect>
      </systemDef>

</datacollection-group>