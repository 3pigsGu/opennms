<?xml version="1.0" encoding="UTF-8" standalone="no"?><datacollection-group name="IETF">

    <resourceType label="MSDP Peer" name="msdpPeerEntry" resourceLabel="Remote ${msdpPeerRemoteAddr}:${msdpPeerRemotePort} (Local ${msdpPeerLocalAddr}:${msdpPeerLocalPort})">
      <persistenceSelectorStrategy class="org.opennms.netmgt.collectd.PersistAllSelectorStrategy"/>
      <storageStrategy class="org.opennms.netmgt.dao.support.IndexStorageStrategy"/>
    </resourceType>

    <resourceType label="MPLS L3VPN VRF" name="mplsL3VpnVrf" resourceLabel="${mL3VVrfName} - ${mL3VVrfDescr}">
      <persistenceSelectorStrategy class="org.opennms.netmgt.collectd.PersistAllSelectorStrategy"/>
      <storageStrategy class="org.opennms.netmgt.dao.support.IndexStorageStrategy"/>
    </resourceType>

      <!-- Stats from the IETF IGMP-STD-MIB -->

      <group ifType="all" name="ietf-igmp-interfaces">
        <mibObj alias="igmpIfWrongVerQrys" instance="ifIndex" oid=".1.3.6.1.2.1.85.1.1.1.10" type="counter"/>
        <mibObj alias="igmpIfJoins" instance="ifIndex" oid=".1.3.6.1.2.1.85.1.1.1.11" type="counter"/>
        <mibObj alias="igmpIfGroups" instance="ifIndex" oid=".1.3.6.1.2.1.85.1.1.1.13" type="gauge"/>
      </group>

      <!-- Stats from the IETF IPMROUTE-STD-MIB -->

      <group ifType="ignore" name="ietf-ipmroute-scalars">
        <mibObj alias="ifMRouteEntryCount" instance="0" oid=".1.3.6.1.2.1.83.1.1.7" type="gauge"/>
      </group>

      <group ifType="all" name="ietf-ipmroute-interfaces">
        <mibObj alias="ifInMcastOctets" instance="ifIndex" oid=".1.3.6.1.2.1.83.1.1.4.1.5" type="counter"/>
        <mibObj alias="ifOutMcastOctets" instance="ifIndex" oid=".1.3.6.1.2.1.83.1.1.4.1.6" type="counter"/>
      </group>

      <group ifType="all" name="ietf-ipmroute-interfaces-hc">
        <mibObj alias="ifHCInMcastOctets" instance="ifIndex" oid=".1.3.6.1.2.1.83.1.1.4.1.7" type="counter"/>
        <mibObj alias="ifHCOutMcastOctets" instance="ifIndex" oid=".1.3.6.1.2.1.83.1.1.4.1.8" type="counter"/>
      </group>

      <!-- Stats from the IETF MSDP-MIB -->

      <group ifType="ignore" name="ietf-msdp-scalars">
        <mibObj alias="msdpSACacheEntries" instance="0" oid=".1.3.6.1.3.92.1.1.3" type="gauge"/>
      </group>

      <group ifType="all" name="ietf-msdp-peers">
        <mibObj alias="msdpPeerRemoteAddr" instance="msdpPeerEntry" oid=".1.3.6.1.3.92.1.1.5.1.1" type="string"/>
        <mibObj alias="msdpPeerRPFFailures" instance="msdpPeerEntry" oid=".1.3.6.1.3.92.1.1.5.1.4" type="counter"/>
        <mibObj alias="msdpPeerInSAs" instance="msdpPeerEntry" oid=".1.3.6.1.3.92.1.1.5.1.5" type="counter"/>
        <mibObj alias="msdpPeerOutSAs" instance="msdpPeerEntry" oid=".1.3.6.1.3.92.1.1.5.1.6" type="counter"/>
        <mibObj alias="msdpPeerInSAReqs" instance="msdpPeerEntry" oid=".1.3.6.1.3.92.1.1.5.1.7" type="counter"/>
        <mibObj alias="msdpPeerOutSAReqs" instance="msdpPeerEntry" oid=".1.3.6.1.3.92.1.1.5.1.8" type="counter"/>
        <mibObj alias="msdpPeerInSARsps" instance="msdpPeerEntry" oid=".1.3.6.1.3.92.1.1.5.1.9" type="counter"/>
        <mibObj alias="msdpPeerOutSARsps" instance="msdpPeerEntry" oid=".1.3.6.1.3.92.1.1.5.1.10" type="counter"/>
        <mibObj alias="msdpPeerInCtrlMsgs" instance="msdpPeerEntry" oid=".1.3.6.1.3.92.1.1.5.1.11" type="counter"/>
        <mibObj alias="msdpPeerOutCtrlMsgs" instance="msdpPeerEntry" oid=".1.3.6.1.3.92.1.1.5.1.12" type="counter"/>
        <mibObj alias="msdpPeerInDataPkts" instance="msdpPeerEntry" oid=".1.3.6.1.3.92.1.1.5.1.13" type="counter"/>
        <mibObj alias="msdpPeerOutDataPkts" instance="msdpPeerEntry" oid=".1.3.6.1.3.92.1.1.5.1.14" type="counter"/>
        <mibObj alias="msdpPeerEstabTrans" instance="msdpPeerEntry" oid=".1.3.6.1.3.92.1.1.5.1.15" type="counter"/>
        <mibObj alias="msdpPeerLocalAddr" instance="msdpPeerEntry" oid=".1.3.6.1.3.92.1.1.5.1.18" type="string"/>
        <mibObj alias="msdpPeerRemotePort" instance="msdpPeerEntry" oid=".1.3.6.1.3.92.1.1.5.1.26" type="string"/>
        <mibObj alias="msdpPeerLocalPort" instance="msdpPeerEntry" oid=".1.3.6.1.3.92.1.1.5.1.27" type="string"/>
        <mibObj alias="msdpPeerConnAttmpts" instance="msdpPeerEntry" oid=".1.3.6.1.3.92.1.1.5.1.30" type="gauge"/>
        <mibObj alias="msdpPeerInNotifs" instance="msdpPeerEntry" oid=".1.3.6.1.3.92.1.1.5.1.31" type="counter"/>
        <mibObj alias="msdpPeerOutNotifs" instance="msdpPeerEntry" oid=".1.3.6.1.3.92.1.1.5.1.32" type="counter"/>
      </group>

      <!-- Stats from the IETF MPLS-L3VPN-STD-MIB -->

      <group ifType="ignore" name="ietf-mpls-l3vpn-scalars">
        <mibObj alias="mL3VConfiguredVrfs" instance="0" oid=".1.3.6.1.2.1.10.166.11.1.1.1" type="gauge"/>
        <mibObj alias="mL3VActiveVrfs" instance="0" oid=".1.3.6.1.2.1.10.166.11.1.1.2" type="gauge"/>
        <mibObj alias="mL3VConnectedIfs" instance="0" oid=".1.3.6.1.2.1.10.166.11.1.1.3" type="gauge"/>
      </group>

      <group ifType="all" name="ietf-mpls-l3vpn-vrfs">
        <mibObj alias="mL3VVrfName" instance="mplsL3VpnVrf" oid=".1.3.6.1.2.1.10.166.11.1.2.2.1.1" type="string"/>
        <mibObj alias="mL3VVrfDescr" instance="mplsL3VpnVrf" oid=".1.3.6.1.2.1.10.166.11.1.2.2.1.3" type="string"/>
        <mibObj alias="mL3VVrfActiveIfs" instance="mplsL3VpnVrf" oid=".1.3.6.1.2.1.10.166.11.1.2.2.1.7" type="gauge"/>
        <mibObj alias="mL3VVrfAssocIfs" instance="mplsL3VpnVrf" oid=".1.3.6.1.2.1.10.166.11.1.2.2.1.8" type="gauge"/>
        <mibObj alias="mL3VVrfPerfRtsAdded" instance="mplsL3VpnVrf" oid=".1.3.6.1.2.1.10.166.11.1.3.1.1.1" type="counter"/>
        <mibObj alias="mL3VVrfPerfRtsDeled" instance="mplsL3VpnVrf" oid=".1.3.6.1.2.1.10.166.11.1.3.1.1.2" type="counter"/>
        <mibObj alias="mL3VVrfPerfCurRts" instance="mplsL3VpnVrf" oid=".1.3.6.1.2.1.10.166.11.1.3.1.1.3" type="gauge"/>
        <mibObj alias="mL3VVrfPerfRtsDrpd" instance="mplsL3VpnVrf" oid=".1.3.6.1.2.1.10.166.11.1.3.1.1.4" type="counter"/>
      </group>

</datacollection-group>