<?xml version="1.0" encoding="UTF-8" standalone="no"?><datacollection-group name="NetEnforcer">

    <resourceType label="Allot NetEnforcer Pipe Position" name="pipePosition" resourceLabel="${pipeName}">
      <persistenceSelectorStrategy class="org.opennms.netmgt.collectd.PersistAllSelectorStrategy"/>
      <storageStrategy class="org.opennms.netmgt.dao.support.IndexStorageStrategy"/>
    </resourceType>
    
    <resourceType label="Allot NetEnforcer VC Position" name="vcPipePosition" resourceLabel="${vcName}">
      <persistenceSelectorStrategy class="org.opennms.netmgt.collectd.PersistAllSelectorStrategy"/>
      <storageStrategy class="org.opennms.netmgt.dao.support.IndexStorageStrategy"/>
    </resourceType>

      <!-- Allot NetEnforcer MIBs -->
      <group ifType="ignore" name="netenforcer-allot-ne">
        <mibObj alias="neByteCountIn" instance="0" oid=".1.3.6.1.4.1.2603.1.1.1.1" type="counter"/>
        <mibObj alias="neByteCountOut" instance="0" oid=".1.3.6.1.4.1.2603.1.1.1.2" type="counter"/>
        <mibObj alias="neLiveConnections" instance="0" oid=".1.3.6.1.4.1.2603.1.1.1.4" type="counter"/>
        <mibObj alias="neNewConnections" instance="0" oid=".1.3.6.1.4.1.2603.1.1.1.5" type="counter"/>
        <mibObj alias="nePacketsIn" instance="0" oid=".1.3.6.1.4.1.2603.1.1.1.6" type="counter"/>
        <mibObj alias="nePacketsOut" instance="0" oid=".1.3.6.1.4.1.2603.1.1.1.7" type="counter"/>
      </group>

      <group ifType="all" name="netenforcer-allot-pipe">
        <mibObj alias="pipePosition" instance="pipePosition" oid=".1.3.6.1.4.1.2603.1.2.1.1.1.1" type="integer"/>
        <mibObj alias="pipeName" instance="pipePosition" oid=".1.3.6.1.4.1.2603.1.2.1.1.1.3" type="string"/>
        <mibObj alias="pipeByteCountIn" instance="pipePosition" oid=".1.3.6.1.4.1.2603.1.2.1.1.1.4" type="counter"/>
        <mibObj alias="pipeByteCountOut" instance="pipePosition" oid=".1.3.6.1.4.1.2603.1.2.1.1.1.5" type="counter"/>
        <mibObj alias="pipeLiveConnections" instance="pipePosition" oid=".1.3.6.1.4.1.2603.1.2.1.1.1.7" type="counter"/>
        <mibObj alias="pipeNewConnections" instance="pipePosition" oid=".1.3.6.1.4.1.2603.1.2.1.1.1.8" type="counter"/>
        <mibObj alias="pipePacketsIn" instance="pipePosition" oid=".1.3.6.1.4.1.2603.1.2.1.1.1.9" type="counter"/>
        <mibObj alias="pipePacketsOut" instance="pipePosition" oid=".1.3.6.1.4.1.2603.1.2.1.1.1.10" type="counter"/>
      </group>

      <group ifType="all" name="netenforcer-allot-vc">
        <mibObj alias="vcPipePosition" instance="vcPipePosition" oid=".1.3.6.1.4.1.2603.1.3.1.1.1.1" type="integer"/>
        <mibObj alias="vcName" instance="vcPipePosition" oid=".1.3.6.1.4.1.2603.1.3.1.1.1.5" type="string"/>
        <mibObj alias="vcByteCountIn" instance="vcPipePosition" oid=".1.3.6.1.4.1.2603.1.3.1.1.1.6" type="counter"/>
        <mibObj alias="vcByteCountOut" instance="vcPipePosition" oid=".1.3.6.1.4.1.2603.1.3.1.1.1.7" type="counter"/>
        <mibObj alias="vcLiveConnections" instance="vcPipePosition" oid=".1.3.6.1.4.1.2603.1.3.1.1.1.9" type="counter"/>
        <mibObj alias="vcNewConnections" instance="vcPipePosition" oid=".1.3.6.1.4.1.2603.1.3.1.1.1.10" type="counter"/>
        <mibObj alias="vcPacketsIn" instance="vcPipePosition" oid=".1.3.6.1.4.1.2603.1.3.1.1.1.11" type="counter"/>
        <mibObj alias="vcPacketsOut" instance="vcPipePosition" oid=".1.3.6.1.4.1.2603.1.3.1.1.1.12" type="counter"/>
      </group>

      <systemDef name="Allot NetEnforcer">
        <sysoid>.1.3.6.1.4.1.2603</sysoid>
        <collect>
          <includeGroup>netenforcer-allot-ne</includeGroup>
          <includeGroup>netenforcer-allot-pipe</includeGroup>
          <includeGroup>netenforcer-allot-vc</includeGroup>
        </collect>
      </systemDef>

</datacollection-group>