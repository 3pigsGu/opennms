<?xml version="1.0" encoding="UTF-8" standalone="no"?><datacollection-group name="IBM">

    <!-- IBM-AIX-MIB resource types -->
    <resourceType label="AIX Volume Group" name="aixVolumeGroup" resourceLabel="${aixVgName} (ID ${aixVgIdentifier})">
      <persistenceSelectorStrategy class="org.opennms.netmgt.collectd.PersistAllSelectorStrategy"/>
      <storageStrategy class="org.opennms.netmgt.dao.support.SiblingColumnStorageStrategy">
        <parameter key="sibling-column-name" value="aixVgName"/>
		<parameter key="replace-all" value="s/\+s//"/>
      </storageStrategy>
    </resourceType>

    <resourceType label="AIX Physical Volume" name="aixPhysicalVolume" resourceLabel="${aixPvName} (VG ${aixPvNameVG})">
      <persistenceSelectorStrategy class="org.opennms.netmgt.collectd.PersistAllSelectorStrategy"/>
      <storageStrategy class="org.opennms.netmgt.dao.support.SiblingColumnStorageStrategy">
        <parameter key="sibling-column-name" value="aixPvName"/>
		<parameter key="replace-all" value="s/\s+//"/>
      </storageStrategy>
    </resourceType>

    <resourceType label="AIX Paging Space" name="aixPagingSpace" resourceLabel="${aixPageName} (VG ${aixPvNameVG}, PV ${aixPageNamePV})">
      <persistenceSelectorStrategy class="org.opennms.netmgt.collectd.PersistAllSelectorStrategy"/>
      <storageStrategy class="org.opennms.netmgt.dao.support.SiblingColumnStorageStrategy">
        <parameter key="sibling-column-name" value="aixPageName"/>
		<parameter key="replace-all" value="s/\s+//"/>
      </storageStrategy>
    </resourceType>

    <resourceType label="AIX Print Queue" name="aixPrintQueue" resourceLabel="${aixPrtQueName} (${aixPrtQueDevice})">
      <persistenceSelectorStrategy class="org.opennms.netmgt.collectd.PersistAllSelectorStrategy"/>
      <storageStrategy class="org.opennms.netmgt.dao.support.SiblingColumnStorageStrategy">
        <parameter key="sibling-column-name" value="aixPrtQueName"/>
		<parameter key="replace-all" value="s/\s+//"/>
      </storageStrategy>
    </resourceType>

    <resourceType label="AIX Filesystem" name="aixFilesystem" resourceLabel="${aixFsName} (${aixFsMountPoint})">
      <persistenceSelectorStrategy class="org.opennms.netmgt.collectd.PersistAllSelectorStrategy"/>
      <storageStrategy class="org.opennms.netmgt.dao.support.SiblingColumnStorageStrategy">
        <parameter key="sibling-column-name" value="aixFsMountPoint"/>
		<parameter key="replace-first" value="s/^-$/_root_fs/"/>
		<parameter key="replace-all" value="s/^-//"/>
		<parameter key="replace-all" value="s/\s//"/>
      </storageStrategy>
    </resourceType>

      <!-- IBM-AIX-MIB -->

      <!-- This copy of sysObjectID just enables the aix.teaser graph -->
      <group ifType="ignore" name="aix-teaser">
        <mibObj alias="aixSysObjectID" instance="0" oid=".1.3.6.1.2.1.1.2" type="string"/>
      </group>

      <group ifType="ignore" name="aix-system-scalars">
        <mibObj alias="aixSeCPUUtilization" instance="0" oid=".1.3.6.1.4.1.2.6.191.1.2.1" type="Gauge32"/>
        <mibObj alias="aixSeCPUThreshold" instance="0" oid=".1.3.6.1.4.1.2.6.191.1.2.2" type="Gauge32"/>
        <mibObj alias="aixProcNum" instance="0" oid=".1.3.6.1.4.1.2.6.191.7.1" type="Gauge32"/>
        <mibObj alias="aixSubSystemNum" instance="0" oid=".1.3.6.1.4.1.2.6.191.5.1.1" type="Gauge32"/>
        <mibObj alias="aixSubSrvNum" instance="0" oid=".1.3.6.1.4.1.2.6.191.5.2.1" type="Gauge32"/>
      </group>

      <group ifType="all" name="aix-volume-groups">
        <mibObj alias="aixVgName" instance="aixVolumeGroup" oid=".1.3.6.1.4.1.2.6.191.2.1.2.1.1" type="string"/>
        <mibObj alias="aixVgIdentifier" instance="aixVolumeGroup" oid=".1.3.6.1.4.1.2.6.191.2.1.2.1.2" type="string"/>
        <mibObj alias="aixVgSize" instance="aixVolumeGroup" oid=".1.3.6.1.4.1.2.6.191.2.1.2.1.4" type="Gauge32"/>
        <mibObj alias="aixVgFree" instance="aixVolumeGroup" oid=".1.3.6.1.4.1.2.6.191.2.1.2.1.5" type="Gauge32"/>
        <mibObj alias="aixVgCurNumLVs" instance="aixVolumeGroup" oid=".1.3.6.1.4.1.2.6.191.2.1.2.1.6" type="Gauge32"/>
        <mibObj alias="aixVgOpenLVs" instance="aixVolumeGroup" oid=".1.3.6.1.4.1.2.6.191.2.1.2.1.7" type="Gauge32"/>
        <mibObj alias="aixVgActivePVs" instance="aixVolumeGroup" oid=".1.3.6.1.4.1.2.6.191.2.1.2.1.8" type="Gauge32"/>
      </group>

      <group ifType="all" name="aix-physical-volume">
        <mibObj alias="aixPvName" instance="aixPhysicalVolume" oid=".1.3.6.1.4.1.2.6.191.2.3.1.1.1" type="string"/>
        <mibObj alias="aixPvNameVG" instance="aixPhysicalVolume" oid=".1.3.6.1.4.1.2.6.191.2.3.1.1.2" type="string"/>
        <mibObj alias="aixPvSize" instance="aixPhysicalVolume" oid=".1.3.6.1.4.1.2.6.191.2.3.1.1.4" type="Gauge32"/>
        <mibObj alias="aixPvFree" instance="aixPhysicalVolume" oid=".1.3.6.1.4.1.2.6.191.2.3.1.1.5" type="Gauge32"/>
      </group>

      <group ifType="all" name="aix-paging-space">
        <mibObj alias="aixPageName" instance="aixPagingSpace" oid=".1.3.6.1.4.1.2.6.191.2.4.2.1.1" type="string"/>
        <mibObj alias="aixPageNameVG" instance="aixPagingSpace" oid=".1.3.6.1.4.1.2.6.191.2.4.2.1.2" type="string"/>
        <mibObj alias="aixPageNamePV" instance="aixPagingSpace" oid=".1.3.6.1.4.1.2.6.191.2.4.2.1.3" type="string"/>
        <mibObj alias="aixPageSize" instance="aixPagingSpace" oid=".1.3.6.1.4.1.2.6.191.2.4.2.1.4" type="Gauge32"/>
        <mibObj alias="aixPagePercentUsed" instance="aixPagingSpace" oid=".1.3.6.1.4.1.2.6.191.2.4.2.1.5" type="Gauge32"/>
      </group>

      <group ifType="all" name="aix-print-queue">
        <mibObj alias="aixPrtQueName" instance="aixPrintQueue" oid=".1.3.6.1.4.1.2.6.191.3.1.1.1.1" type="string"/>
        <mibObj alias="aixPrtQueDevice" instance="aixPrintQueue" oid=".1.3.6.1.4.1.2.6.191.3.1.1.1.2" type="string"/>
        <mibObj alias="aixPrtQueJobNum" instance="aixPrintQueue" oid=".1.3.6.1.4.1.2.6.191.3.1.1.1.9" type="Gauge32"/>
      </group>

      <group ifType="all" name="aix-filesystem">
        <mibObj alias="aixFsName" instance="aixFilesystem" oid=".1.3.6.1.4.1.2.6.191.6.2.1.2" type="string"/>
        <mibObj alias="aixFsMountPoint" instance="aixFilesystem" oid=".1.3.6.1.4.1.2.6.191.6.2.1.3" type="string"/>
        <mibObj alias="aixFsSize" instance="aixFilesystem" oid=".1.3.6.1.4.1.2.6.191.6.2.1.5" type="Gauge32"/>
        <mibObj alias="aixFsFree" instance="aixFilesystem" oid=".1.3.6.1.4.1.2.6.191.6.2.1.6" type="Gauge32"/>
        <mibObj alias="aixFsNumINodes" instance="aixFilesystem" oid=".1.3.6.1.4.1.2.6.191.6.2.1.7" type="Gauge32"/>
        <mibObj alias="aixFsUsedInodes" instance="aixFilesystem" oid=".1.3.6.1.4.1.2.6.191.6.2.1.8" type="Gauge32"/>
      </group>

      <!-- IBM MIBs -->
      <group ifType="ignore" name="domino-stats">
        <mibObj alias="attachedUsers" instance="0" oid=".1.3.6.1.4.1.334.72.1.1.6.3.6" type="integer"/>
        <mibObj alias="deadMail" instance="0" oid=".1.3.6.1.4.1.334.72.1.1.4.1" type="integer"/>
        <mibObj alias="deliveredMail" instance="0" oid=".1.3.6.1.4.1.334.72.1.1.4.2" type="integer"/>
        <mibObj alias="transferredMail" instance="0" oid=".1.3.6.1.4.1.334.72.1.1.4.5" type="integer"/>
        <mibObj alias="waitingMail" instance="0" oid=".1.3.6.1.4.1.334.72.1.1.4.6" type="integer"/>
        <mibObj alias="msgsWaitForDlvry" instance="0" oid=".1.3.6.1.4.1.334.72.1.1.4.7" type="integer"/>
        <mibObj alias="aveMailDeliveryTime" instance="0" oid=".1.3.6.1.4.1.334.72.1.1.4.9" type="integer"/>
        <mibObj alias="aveMailSizeDlvrd" instance="0" oid=".1.3.6.1.4.1.334.72.1.1.4.11" type="integer"/>
        <mibObj alias="mailTxFailures" instance="0" oid=".1.3.6.1.4.1.334.72.1.1.4.19" type="integer"/>
        <mibObj alias="replFailures" instance="0" oid=".1.3.6.1.4.1.334.72.1.1.5.6.4.0" type="integer"/>
        <mibObj alias="aveTransactPerMin" instance="0" oid=".1.3.6.1.4.1.334.72.1.1.6.3.2" type="integer"/>
        <mibObj alias="maxUsers" instance="0" oid=".1.3.6.1.4.1.334.72.1.1.6.3.9" type="integer"/>
        <mibObj alias="allocatedMem" instance="0" oid=".1.3.6.1.4.1.334.72.1.1.9.2" type="integer"/>
        <mibObj alias="freeMemory" instance="0" oid=".1.3.6.1.4.1.334.72.1.1.9.5" type="integer"/>
        <mibObj alias="1stHDDFreeSpace" instance="1" oid=".1.3.6.1.4.1.334.72.1.1.8.3.1.4" type="integer"/>
        <mibObj alias="2ndHDDFreeSpace" instance="2" oid=".1.3.6.1.4.1.334.72.1.1.8.3.1.4" type="integer"/>
        <mibObj alias="totalApptmtReserv" instance="0" oid=".1.3.6.1.4.1.334.72.1.1.16.1" type="integer"/>
        <mibObj alias="totalCalendarUsers" instance="0" oid=".1.3.6.1.4.1.334.72.1.1.16.6" type="integer"/>
      </group>

      <systemDef name="IBM-AIX">
        <sysoid>.1.3.6.1.4.1.2.3.1.2.1.1.3</sysoid>
        <collect>
          <includeGroup>mib2-host-resources-storage</includeGroup>
          <includeGroup>mib2-host-resources-system</includeGroup>
          <includeGroup>mib2-host-resources-memory</includeGroup>
          <includeGroup>mib2-host-resources-processor</includeGroup>
          <includeGroup>aix-teaser</includeGroup>
          <includeGroup>aix-system-scalars</includeGroup>
          <includeGroup>aix-volume-groups</includeGroup>
          <includeGroup>aix-paging-space</includeGroup>
          <includeGroup>aix-filesystem</includeGroup>
          <!-- AIX Physical Volume objects tend to time out -->
          <!-- <includeGroup>aix-physical-volume</includeGroup> -->
          <!-- Uncomment if you want AIX print queue stats -->
          <!-- <includeGroup>aix-print-queue</includeGroup> -->
        </collect>
      </systemDef>

     <systemDef name="IBM-AS400">
        <sysoid>.1.3.6.1.4.1.2.6.11</sysoid>
        <collect>
          <includeGroup>mib2-host-resources-storage-non-unique</includeGroup>
          <includeGroup>mib2-host-resources-system</includeGroup>
          <includeGroup>mib2-host-resources-memory</includeGroup>
          <includeGroup>mib2-host-resources-processor</includeGroup>
        </collect>
      </systemDef>

</datacollection-group>