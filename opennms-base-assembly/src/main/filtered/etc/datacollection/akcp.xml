<?xml version="1.0" encoding="UTF-8" standalone="no"?><datacollection-group name="AKCP sensorProbe">

  <!-- AKCP sensorProbe -->
  <resourceType label="AKCP" name="hhmsSensorArrayTemp" resourceLabel="${spTempDescr}">
    <persistenceSelectorStrategy class="org.opennms.netmgt.collectd.PersistAllSelectorStrategy"/>
    <storageStrategy class="org.opennms.netmgt.dao.support.IndexStorageStrategy"/>
  </resourceType>
  <resourceType label="AKCP" name="hhmsSensorArrayHumid" resourceLabel="${spHumidityDescr}">
    <persistenceSelectorStrategy class="org.opennms.netmgt.collectd.PersistAllSelectorStrategy"/>
    <storageStrategy class="org.opennms.netmgt.dao.support.IndexStorageStrategy"/>
  </resourceType>

  <!-- AKCP securityProbe -->
  <resourceType label="Temperature sensor" name="secTempIndex" resourceLabel="(Index: ${secTempIdx}) Temperature: ${secTempDescr}">
    <persistenceSelectorStrategy class="org.opennms.netmgt.collectd.PersistAllSelectorStrategy"/>
    <storageStrategy class="org.opennms.netmgt.dao.support.IndexStorageStrategy"/>
  </resourceType>
  <resourceType label="Humidity sensor" name="secHumidityIndex" resourceLabel="(Index: ${secHumidityIdx}) Humidity: ${secHumidityDescr}">
    <persistenceSelectorStrategy class="org.opennms.netmgt.collectd.PersistAllSelectorStrategy"/>
    <storageStrategy class="org.opennms.netmgt.dao.support.IndexStorageStrategy"/>
  </resourceType>
  <resourceType label="4 to 20mA sensor" name="sec4to20maIndex" resourceLabel="(Index: ${sec4to20maIdx}) 4 to 20mA: ${sec4to20maDescr}">
    <persistenceSelectorStrategy class="org.opennms.netmgt.collectd.PersistAllSelectorStrategy"/>
    <storageStrategy class="org.opennms.netmgt.dao.support.IndexStorageStrategy"/>
  </resourceType>
  <resourceType label="DC Voltage" name="secDcVoltIndex" resourceLabel="(Index: ${secDcVoltIdx}) DC Voltage: ${secDcVoltDescr}">
    <persistenceSelectorStrategy class="org.opennms.netmgt.collectd.PersistAllSelectorStrategy"/>
    <storageStrategy class="org.opennms.netmgt.dao.support.IndexStorageStrategy"/>
  </resourceType>
  <resourceType label="Airflow" name="secAirflowIndex" resourceLabel="(Index: ${secAirflowIdx}) Airflow: ${secAirflowDescr}">
    <persistenceSelectorStrategy class="org.opennms.netmgt.collectd.PersistAllSelectorStrategy"/>
    <storageStrategy class="org.opennms.netmgt.dao.support.IndexStorageStrategy"/>
  </resourceType>
  <resourceType label="Thermostat" name="secThermostatIndex" resourceLabel="(Index: ${secThermostatIdx}) Thermostat: ${secThermostatDescr}">
    <persistenceSelectorStrategy class="org.opennms.netmgt.collectd.PersistAllSelectorStrategy"/>
    <storageStrategy class="org.opennms.netmgt.dao.support.IndexStorageStrategy"/>
  </resourceType>

  <!-- AKCP sensorProbe -->
  <group ifType="all" name="akcp-sensor-temperature">
    <mibObj alias="spTempDescr" instance="hhmsSensorArrayTemp" oid=".1.3.6.1.4.1.3854.1.2.2.1.16.1.1" type="string"/>
    <mibObj alias="spTempDegree" instance="hhmsSensorArrayTemp" oid=".1.3.6.1.4.1.3854.1.2.2.1.16.1.3" type="gauge"/>
  </group>
  <group ifType="all" name="akcp-sensor-humidity">
    <mibObj alias="spHumidityDescr" instance="hhmsSensorArrayHumid" oid=".1.3.6.1.4.1.3854.1.2.2.1.17.1.1" type="string"/>
    <mibObj alias="spHumidityPercent" instance="hhmsSensorArrayHumid" oid=".1.3.6.1.4.1.3854.1.2.2.1.17.1.3" type="gauge"/>
  </group>

  <!-- AKCP securityProbe -->
  <group ifType="ignore" name="securityProbe-temperature">
    <mibObj alias="secTempIdx" instance="secTempIndex" oid=".1.3.6.1.4.1.3854.2.3.2.1.1" type="string"/>
    <mibObj alias="secTempDescr" instance="secTempIndex" oid=".1.3.6.1.4.1.3854.2.3.2.1.2" type="string"/>
    <mibObj alias="secTempUnit" instance="secTempIndex" oid=".1.3.6.1.4.1.3854.2.3.2.1.5" type="string"/>
    <mibObj alias="secTempLCrit" instance="secTempIndex" oid=".1.3.6.1.4.1.3854.2.3.2.1.9" type="integer"/>
    <mibObj alias="secTempLWarn" instance="secTempIndex" oid=".1.3.6.1.4.1.3854.2.3.2.1.10" type="integer"/>
    <mibObj alias="secTempHWarn" instance="secTempIndex" oid=".1.3.6.1.4.1.3854.2.3.2.1.11" type="integer"/>
    <mibObj alias="secTempHCrit" instance="secTempIndex" oid=".1.3.6.1.4.1.3854.2.3.2.1.12" type="integer"/>
    <mibObj alias="secTempDegree" instance="secTempIndex" oid=".1.3.6.1.4.1.3854.2.3.2.1.20" type="integer"/>
  </group>
  <group ifType="ignore" name="securityProbe-humidity">
    <mibObj alias="secHumidityIdx" instance="secHumidityIndex" oid=".1.3.6.1.4.1.3854.2.3.3.1.1" type="string"/>
    <mibObj alias="secHumidityDescr" instance="secHumidityIndex" oid=".1.3.6.1.4.1.3854.2.3.3.1.2" type="string"/>
    <mibObj alias="secHumidityPercent" instance="secHumidityIndex" oid=".1.3.6.1.4.1.3854.2.3.3.1.4" type="integer"/>
    <mibObj alias="secHumidityUnit" instance="secHumidityIndex" oid=".1.3.6.1.4.1.3854.2.3.3.1.5" type="string"/>
    <mibObj alias="secHumidityLCrit" instance="secHumidityIndex" oid=".1.3.6.1.4.1.3854.2.3.3.1.9" type="integer"/>
    <mibObj alias="secHumidityLWarn" instance="secHumidityIndex" oid=".1.3.6.1.4.1.3854.2.3.3.1.10" type="integer"/>
    <mibObj alias="secHumidityHWarn" instance="secHumidityIndex" oid=".1.3.6.1.4.1.3854.2.3.3.1.11" type="integer"/>
    <mibObj alias="secHumidityHCrit" instance="secHumidityIndex" oid=".1.3.6.1.4.1.3854.2.3.3.1.12" type="integer"/>
  </group>
  <group ifType="ignore" name="securityProbe-4to20ma">
    <mibObj alias="sec4to20maIdx" instance="sec4to20maIndex" oid=".1.3.6.1.4.1.3854.2.3.5.1.1" type="string"/>
    <mibObj alias="sec4to20maDescr" instance="sec4to20maIndex" oid=".1.3.6.1.4.1.3854.2.3.5.1.2" type="string"/>
    <mibObj alias="sec4to20maUnit" instance="sec4to20maIndex" oid=".1.3.6.1.4.1.3854.2.3.5.1.5" type="string"/>
    <mibObj alias="sec4to20maLCrit" instance="sec4to20maIndex" oid=".1.3.6.1.4.1.3854.2.3.5.1.9" type="integer"/>
    <mibObj alias="sec4to20maLWarn" instance="sec4to20maIndex" oid=".1.3.6.1.4.1.3854.2.3.5.1.10" type="integer"/>
    <mibObj alias="sec4to20maHWarn" instance="sec4to20maIndex" oid=".1.3.6.1.4.1.3854.2.3.5.1.11" type="integer"/>
    <mibObj alias="sec4to20maHCrit" instance="sec4to20maIndex" oid=".1.3.6.1.4.1.3854.2.3.5.1.12" type="integer"/>
    <mibObj alias="sec4to20maValue" instance="sec4to20maIndex" oid=".1.3.6.1.4.1.3854.2.3.5.1.20" type="integer"/>
  </group>
  <group ifType="ignore" name="securityProbe-DCVoltage">
    <mibObj alias="secDcVoltIdx" instance="secDcVoltIndex" oid=".1.3.6.1.4.1.3854.2.3.6.1.1" type="string"/>
    <mibObj alias="secDcVoltDescr" instance="secDcVoltIndex" oid=".1.3.6.1.4.1.3854.2.3.6.1.2" type="string"/>
    <mibObj alias="secDcVoltUnit" instance="secDcVoltIndex" oid=".1.3.6.1.4.1.3854.2.3.6.1.5" type="string"/>
    <mibObj alias="secDcVoltLCrit" instance="secDcVoltIndex" oid=".1.3.6.1.4.1.3854.2.3.6.1.9" type="integer"/>
    <mibObj alias="secDcVoltLWarn" instance="secDcVoltIndex" oid=".1.3.6.1.4.1.3854.2.3.6.1.10" type="integer"/>
    <mibObj alias="secDcVoltHWarn" instance="secDcVoltIndex" oid=".1.3.6.1.4.1.3854.2.3.6.1.11" type="integer"/>
    <mibObj alias="secDcVoltHCrit" instance="secDcVoltIndex" oid=".1.3.6.1.4.1.3854.2.3.6.1.12" type="integer"/>
    <mibObj alias="secDcVoltValue" instance="secDcVoltIndex" oid=".1.3.6.1.4.1.3854.2.3.6.1.20" type="integer"/>
  </group>
  <group ifType="ignore" name="securityProbe-Airflow">
    <mibObj alias="secAirflowIdx" instance="secAirflowIndex" oid=".1.3.6.1.4.1.3854.2.3.7.1.1" type="string"/>
    <mibObj alias="secAirflowDescr" instance="secAirflowIndex" oid=".1.3.6.1.4.1.3854.2.3.7.1.2" type="string"/>
    <mibObj alias="secAirflowUnit" instance="secAirflowIndex" oid=".1.3.6.1.4.1.3854.2.3.7.1.5" type="string"/>
    <mibObj alias="secAirflowLCrit" instance="secAirflowIndex" oid=".1.3.6.1.4.1.3854.2.3.7.1.9" type="integer"/>
    <mibObj alias="secAirflowLWarn" instance="secAirflowIndex" oid=".1.3.6.1.4.1.3854.2.3.7.1.10" type="integer"/>
    <mibObj alias="secAirflowHWarn" instance="secAirflowIndex" oid=".1.3.6.1.4.1.3854.2.3.7.1.11" type="integer"/>
    <mibObj alias="secAirflowHCrit" instance="secAirflowIndex" oid=".1.3.6.1.4.1.3854.2.3.7.1.12" type="integer"/>
