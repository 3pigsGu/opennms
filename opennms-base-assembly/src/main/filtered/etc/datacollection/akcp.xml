    <mibObj alias="secThermostatDescr" instance="secThermostatIndex" oid=".1.3.6.1.4.1.3854.2.3.20.1.2" type="string"/>
    <mibObj alias="secThermostatUnit" instance="secThermostatIndex" oid=".1.3.6.1.4.1.3854.2.3.20.1.5" type="string"/>
    <mibObj alias="secThermostatValue" instance="secThermostatIndex" oid=".1.3.6.1.4.1.3854.2.3.20.1.4" type="integer"/>
    <mibObj alias="secThermostatLCrit" instance="secThermostatIndex" oid=".1.3.6.1.4.1.3854.2.3.20.1.9" type="integer"/>
    <mibObj alias="secThermostatLWarn" instance="secThermostatIndex" oid=".1.3.6.1.4.1.3854.2.3.20.1.10" type="integer"/>
    <mibObj alias="secThermostatHWarn" instance="secThermostatIndex" oid=".1.3.6.1.4.1.3854.2.3.20.1.11" type="integer"/>
    <mibObj alias="secThermostatHCrit" instance="secThermostatIndex" oid=".1.3.6.1.4.1.3854.2.3.20.1.12" type="integer"/>
  </group>

  <systemDef name="AKCP sensorProbe">
    <sysoid>.1.3.6.1.4.1.3854.1</sysoid>
    <collect>
      <includeGroup>akcp-sensor-temperature</includeGroup>
      <includeGroup>akcp-sensor-humidity</includeGroup>
      <includeGroup>securityProbe-temperature</includeGroup>
      <includeGroup>securityProbe-humidity</includeGroup>

      <!-- SNMP MIB support has to be verified, and is prepared -->
      <!--
      <includeGroup>securityProbe-4to20ma</includeGroup>
      <includeGroup>securityProbe-DCVoltage</includeGroup>
      <includeGroup>securityProbe-Airflow</includeGroup>
      -->

      <includeGroup>securityProbe-Thermostat</includeGroup>
    </collect>
  </systemDef>

</datacollection-group>