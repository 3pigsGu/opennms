<?xml version="1.0" encoding="UTF-8" standalone="no"?><datacollection-group name="Network Appliance">

    <resourceType label="Disk Space (Network Appliance MIB)" name="naDfIndex" resourceLabel="${naDfFileSys} (index ${index})">
      <persistenceSelectorStrategy class="org.opennms.netmgt.collectd.PersistAllSelectorStrategy"/>
      <storageStrategy class="org.opennms.netmgt.dao.support.IndexStorageStrategy"/>
    </resourceType>

      <!-- Netapp MIBS -->
      <group ifType="ignore" name="netapp-cpu">
        <mibObj alias="cpuBusyTimePct" instance="0" oid=".1.3.6.1.4.1.789.1.2.1.3" type="INTEGER"/>
        <mibObj alias="cpuIdleTimePct" instance="0" oid=".1.3.6.1.4.1.789.1.2.1.5" type="INTEGER"/>
      </group>

      <group ifType="ignore" name="netapp-filesys">
        <mibObj alias="fsMaxfilesAvail" instance="0" oid=".1.3.6.1.4.1.789.1.5.1" type="INTEGER"/>
        <mibObj alias="fsMaxfilesUsed" instance="0" oid=".1.3.6.1.4.1.789.1.5.2" type="INTEGER"/>
        <mibObj alias="fsMaxfilesPossible" instance="0" oid=".1.3.6.1.4.1.789.1.5.3" type="INTEGER"/>
        <mibObj alias="fsMaxUsedBytesPct" instance="0" oid=".1.3.6.1.4.1.789.1.5.7.3" type="INTEGER"/>
        <mibObj alias="fsMaxUsedInodesPct" instance="0" oid=".1.3.6.1.4.1.789.1.5.7.4" type="INTEGER"/>
      </group>

      <group ifType="all" name="netapp-dfEntry">
        <mibObj alias="naDfFileSys" instance="naDfIndex" oid=".1.3.6.1.4.1.789.1.5.4.1.2" type="string"/>
        <mibObj alias="naDfPctKB" instance="naDfIndex" oid=".1.3.6.1.4.1.789.1.5.4.1.6" type="integer"/>
        <mibObj alias="naDfInodeUsed" instance="naDfIndex" oid=".1.3.6.1.4.1.789.1.5.4.1.7" type="integer"/>
        <mibObj alias="naDfInodeFree" instance="naDfIndex" oid=".1.3.6.1.4.1.789.1.5.4.1.8" type="integer"/>
        <mibObj alias="naDfInodePct" instance="naDfIndex" oid=".1.3.6.1.4.1.789.1.5.4.1.9" type="integer"/>
        <mibObj alias="naDfMaxFileAvail" instance="naDfIndex" oid=".1.3.6.1.4.1.789.1.5.4.1.11" type="integer"/>
        <mibObj alias="naDfMaxFileUsed" instance="naDfIndex" oid=".1.3.6.1.4.1.789.1.5.4.1.12" type="integer"/>
        <mibObj alias="naDfLowUsedKB" instance="naDfIndex" oid=".1.3.6.1.4.1.789.1.5.4.1.17" type="integer"/>
        <mibObj alias="naDfLowFreeKB" instance="naDfIndex" oid=".1.3.6.1.4.1.789.1.5.4.1.19" type="integer"/>
      </group>

      <group ifType="ignore" name="netapp-filer"> 
        <mibObj alias="naMiscLowNfsOps" instance="0" oid=".1.3.6.1.4.1.789.1.2.2.6" type="Counter"/>
        <mibObj alias="naMiscLowCifsOps" instance="0" oid=".1.3.6.1.4.1.789.1.2.2.8" type="Counter"/>
        <mibObj alias="naCifsReads" instance="0" oid=".1.3.6.1.4.1.789.1.7.3.1.1.5" type="Counter"/>
        <mibObj alias="naCifsWrites" instance="0" oid=".1.3.6.1.4.1.789.1.7.3.1.1.6" type="Counter"/>
        <mibObj alias="naV2cReads" instance="0" oid=".1.3.6.1.4.1.789.1.3.1.2.3.1.7" type="Counter"/>
        <mibObj alias="naV2cWrcaches" instance="0" oid=".1.3.6.1.4.1.789.1.3.1.2.3.1.8" type="Counter"/>
        <mibObj alias="naV2cWrites" instance="0" oid=".1.3.6.1.4.1.789.1.3.1.2.3.1.9" type="Counter"/>
        <mibObj alias="naV3cReads" instance="0" oid=".1.3.6.1.4.1.789.1.3.1.2.4.1.7" type="Counter"/>
        <mibObj alias="naV3cWrites" instance="0" oid=".1.3.6.1.4.1.789.1.3.1.2.4.1.8" type="Counter"/>
      </group>

      <group ifType="ignore" name="netapp-misc">
        <mibObj alias="naMiscLowDiskReadBytes" instance="0" oid=".1.3.6.1.4.1.789.1.2.2.16" type="Counter"/>
        <mibObj alias="naMiscLowDiskWriteBytes" instance="0" oid=".1.3.6.1.4.1.789.1.2.2.18" type="Counter"/>
      </group>

      <group ifType="all" name="netapp-sis">
        <mibObj alias="naSisPercent" instance="naDfIndex" oid=".1.3.6.1.4.1.789.1.5.4.1.28" type="integer"/>
      </group>

      <systemDef name="Network Appliance">
        <sysoidMask>.1.3.6.1.4.1.789.</sysoidMask>
        <collect>
          <includeGroup>netapp-cpu</includeGroup>
          <includeGroup>netapp-filesys</includeGroup>
        </collect>
      </systemDef>

      <systemDef name="Network Appliance Filer">
        <sysoid>.1.3.6.1.4.1.789.2.1</sysoid>
        <collect>
          <includeGroup>netapp-dfEntry</includeGroup>
          <includeGroup>netapp-filer</includeGroup>
          <includeGroup>netapp-misc</includeGroup>
          <includeGroup>netapp-sis</includeGroup>
        </collect>
      </systemDef>

      <systemDef name="Network Appliance Clustered Filer">
        <sysoid>.1.3.6.1.4.1.789.2.3</sysoid>
        <collect>
          <includeGroup>netapp-dfEntry</includeGroup>
          <includeGroup>netapp-filer</includeGroup>
          <includeGroup>netapp-misc</includeGroup>
          <includeGroup>netapp-sis</includeGroup>
        </collect>
      </systemDef>

</datacollection-group>