        <mibObj alias="swFCPortRxWords" instance="swFCPortIndex" oid=".1.3.6.1.4.1.1588.2.1.1.1.6.2.1.12" type="counter"/>
        <mibObj alias="swFCPortTxFrames" instance="swFCPortIndex" oid=".1.3.6.1.4.1.1588.2.1.1.1.6.2.1.13" type="counter"/>
        <mibObj alias="swFCPortRxFrames" instance="swFCPortIndex" oid=".1.3.6.1.4.1.1588.2.1.1.1.6.2.1.14" type="counter"/>
        <mibObj alias="swFCPortName" instance="swFCPortIndex" oid=".1.3.6.1.4.1.1588.2.1.1.1.6.2.1.36" type="string"/>
        <mibObj alias="swFCPortSpecifier" instance="swFCPortIndex" oid=".1.3.6.1.4.1.1588.2.1.1.1.6.2.1.37" type="string"/>
      </group>

      <systemDef name="Brocade FC Switches">
        <sysoidMask>.1.3.6.1.4.1.1588.</sysoidMask>
        <collect>
          <includeGroup>brocade-switch-fctable</includeGroup>
        </collect>
      </systemDef>

</datacollection-group>