<?xml version="1.0" encoding="UTF-8" standalone="no"?><datacollection-group name="Net-SNMP">

    <resourceType label="Disk IO (UCD-SNMP MIB)" name="diskIOIndex" resourceLabel="${diskIODevice} (index ${index})">
      <persistenceSelectorStrategy class="org.opennms.netmgt.collectd.PersistRegexSelectorStrategy">
        <parameter key="match-expression" value="not(#diskIODevice matches '^(loop|ram).*')"/>
      </persistenceSelectorStrategy>
      <storageStrategy class="org.opennms.netmgt.dao.support.SiblingColumnStorageStrategy">
        <parameter key="sibling-column-name" value="diskIODevice"/>
		<parameter key="replace-all" value="s/^-//"/>
		<parameter key="replace-all" value="s/\s//"/>
		<parameter key="replace-all" value="s/:\\.*//"/>
      </storageStrategy>
    </resourceType>

    <resourceType label="Disk Table Index (UCD-SNMP MIB)" name="dskIndex" resourceLabel="${ns-dskPath} (index ${index})">
      <persistenceSelectorStrategy class="org.opennms.netmgt.collectd.PersistAllSelectorStrategy"/>
      <storageStrategy class="org.opennms.netmgt.dao.support.SiblingColumnStorageStrategy">
        <parameter key="sibling-column-name" value="ns-dskPath"/>
		<parameter key="replace-first" value="s/^-$/_root_fs/"/>
		<parameter key="replace-all" value="s/^-//"/>
		<parameter key="replace-all" value="s/\s//"/>
		<parameter key="replace-all" value="s/:\\.*//"/>
      </storageStrategy>
    </resourceType>

    <resourceType label="Hardware Sensors: Temperature (lmSensors MIB)" name="lmTempIndex" resourceLabel="${lms-tempdevice} (index ${index})">
      <persistenceSelectorStrategy class="org.opennms.netmgt.collectd.PersistAllSelectorStrategy"/>
      <storageStrategy class="org.opennms.netmgt.dao.support.IndexStorageStrategy"/>
    </resourceType>

    <resourceType label="Hardware Sensors: Fan (lmSensors MIB)" name="lmFanIndex" resourceLabel="${lms-fandevice} (index ${index})">
      <persistenceSelectorStrategy class="org.opennms.netmgt.collectd.PersistAllSelectorStrategy"/>
      <storageStrategy class="org.opennms.netmgt.dao.support.IndexStorageStrategy"/>
    </resourceType>

    <resourceType label="Hardware Sensors: Voltage (lmSensors MIB)" name="lmVoltIndex" resourceLabel="${lms-voltdevice} (index ${index})">
      <persistenceSelectorStrategy class="org.opennms.netmgt.collectd.PersistAllSelectorStrategy"/>
      <storageStrategy class="org.opennms.netmgt.dao.support.IndexStorageStrategy"/>
    </resourceType>

      <!-- Net-SNMP MIBs -->
      <group ifType="all" name="net-snmp-disk">
        <mibObj alias="ns-dskPath" instance="dskIndex" oid=".1.3.6.1.4.1.2021.9.1.2" type="string"/>
        <mibObj alias="ns-dskTotal" instance="dskIndex" oid=".1.3.6.1.4.1.2021.9.1.6" type="gauge"/>
        <mibObj alias="ns-dskAvail" instance="dskIndex" oid=".1.3.6.1.4.1.2021.9.1.7" type="gauge"/>
        <mibObj alias="ns-dskUsed" instance="dskIndex" oid=".1.3.6.1.4.1.2021.9.1.8" type="gauge"/>
        <mibObj alias="ns-dskPercent" instance="dskIndex" oid=".1.3.6.1.4.1.2021.9.1.9" type="gauge"/>
      </group>

      <group ifType="all" name="net-snmp-disk-more">
        <mibObj alias="ns-dskDevice" instance="dskIndex" oid=".1.3.6.1.4.1.2021.9.1.3" type="string"/>
        <mibObj alias="ns-dskPercentNode" instance="dskIndex" oid=".1.3.6.1.4.1.2021.9.1.10" type="gauge"/>
      </group>

      <group ifType="all" name="net-snmp-disk-highlow">
        <mibObj alias="ns-dskTotalLow" instance="dskIndex" oid=".1.3.6.1.4.1.2021.9.1.11" type="gauge"/>
        <mibObj alias="ns-dskTotalHigh" instance="dskIndex" oid=".1.3.6.1.4.1.2021.9.1.12" type="gauge"/>
        <mibObj alias="ns-dskAvailLow" instance="dskIndex" oid=".1.3.6.1.4.1.2021.9.1.13" type="gauge"/>
        <mibObj alias="ns-dskAvailHigh" instance="dskIndex" oid=".1.3.6.1.4.1.2021.9.1.14" type="gauge"/>
        <mibObj alias="ns-dskUsedLow" instance="dskIndex" oid=".1.3.6.1.4.1.2021.9.1.15" type="gauge"/>
        <mibObj alias="ns-dskUsedHigh" instance="dskIndex" oid=".1.3.6.1.4.1.2021.9.1.16" type="gauge"/>
      </group>

      <group ifType="ignore" name="ucd-loadavg">
        <mibObj alias="loadavg1" instance="1" oid=".1.3.6.1.4.1.2021.10.1.5" type="integer"/>
        <mibObj alias="loadavg5" instance="2" oid=".1.3.6.1.4.1.2021.10.1.5" type="integer"/>
        <mibObj alias="loadavg15" instance="3" oid=".1.3.6.1.4.1.2021.10.1.5" type="integer"/>
      </group>

      <group ifType="ignore" name="ucd-memory">
        <!-- Total Swap Size configured for the host. -->
        <mibObj alias="memTotalSwap" instance="0" oid=".1.3.6.1.4.1.2021.4.3" type="integer"/>
        <mibObj alias="memAvailSwap" instance="0" oid=".1.3.6.1.4.1.2021.4.4" type="integer"/>
        <mibObj alias="memTotalReal" instance="0" oid=".1.3.6.1.4.1.2021.4.5" type="integer"/>
        <mibObj alias="memAvailReal" instance="0" oid=".1.3.6.1.4.1.2021.4.6" type="integer"/>
        <mibObj alias="memTotalFree" instance="0" oid=".1.3.6.1.4.1.2021.4.11" type="integer"/>
        <mibObj alias="memShared" instance="0" oid=".1.3.6.1.4.1.2021.4.13" type="integer"/>
        <mibObj alias="memBuffer" instance="0" oid=".1.3.6.1.4.1.2021.4.14" type="integer"/>
        <mibObj alias="memCached" instance="0" oid=".1.3.6.1.4.1.2021.4.15" type="integer"/>
        <!-- Error flag.  1 indicates very little swap space left -->
        <mibObj alias="memSwapError" instance="0" oid=".1.3.6.1.4.1.2021.4.100" type="integer"/>
      </group>

      <group ifType="ignore" name="ucd-sysstat">
        <mibObj alias="SwapIn" instance="0" oid=".1.3.6.1.4.1.2021.11.3" type="integer"/>
        <mibObj alias="SwapOut" instance="0" oid=".1.3.6.1.4.1.2021.11.4" type="integer"/>
        <mibObj alias="SysInterrupts" instance="0" oid=".1.3.6.1.4.1.2021.11.7" type="integer"/>
        <mibObj alias="SysContext" instance="0" oid=".1.3.6.1.4.1.2021.11.8" type="integer"/>
        <mibObj alias="CpuRawUser" instance="0" oid=".1.3.6.1.4.1.2021.11.50" type="counter"/>
        <mibObj alias="CpuRawNice" instance="0" oid=".1.3.6.1.4.1.2021.11.51" type="counter"/>
        <mibObj alias="CpuRawSystem" instance="0" oid=".1.3.6.1.4.1.2021.11.52" type="counter"/>
        <mibObj alias="CpuRawIdle" instance="0" oid=".1.3.6.1.4.1.2021.11.53" type="counter"/>
        <mibObj alias="CpuRawWait" instance="0" oid=".1.3.6.1.4.1.2021.11.54" type="counter"/>
        <mibObj alias="CpuRawKernel" instance="0" oid=".1.3.6.1.4.1.2021.11.55" type="counter"/>
        <mibObj alias="CpuRawInterrupt" instance="0" oid=".1.3.6.1.4.1.2021.11.56" type="counter"/>
        <mibObj alias="IORawSent" instance="0" oid=".1.3.6.1.4.1.2021.11.57" type="counter"/>
        <mibObj alias="IORawReceived" instance="0" oid=".1.3.6.1.4.1.2021.11.58" type="counter"/>
      </group>

      <group ifType="ignore" name="ucd-sysstat-raw">
        <mibObj alias="SysRawInterrupts" instance="0" oid=".1.3.6.1.4.1.2021.11.59" type="counter"/>
        <mibObj alias="SysRawContext" instance="0" oid=".1.3.6.1.4.1.2021.11.60" type="counter"/>
      </group>

      <group ifType="ignore" name="ucd-sysstat-raw-more">
        <mibObj alias="CpuRawSoftIRQ" instance="0" oid=".1.3.6.1.4.1.2021.11.61" type="counter"/>
        <mibObj alias="RawBlksSwapIn" instance="0" oid=".1.3.6.1.4.1.2021.11.62" type="counter"/>
        <mibObj alias="RawBlksSwapOut" instance="0" oid=".1.3.6.1.4.1.2021.11.63" type="counter"/>
      </group>

      <group ifType="all" name="ucd-diskio">
        <mibObj alias="diskIODevice" instance="diskIOIndex" oid=".1.3.6.1.4.1.2021.13.15.1.1.2" type="string"/>
        <mibObj alias="diskIONRead" instance="diskIOIndex" oid=".1.3.6.1.4.1.2021.13.15.1.1.3" type="counter"/>
        <mibObj alias="diskIONWritten" instance="diskIOIndex" oid=".1.3.6.1.4.1.2021.13.15.1.1.4" type="counter"/>
        <mibObj alias="diskIOReads" instance="diskIOIndex" oid=".1.3.6.1.4.1.2021.13.15.1.1.5" type="counter"/>
        <mibObj alias="diskIOWrites" instance="diskIOIndex" oid=".1.3.6.1.4.1.2021.13.15.1.1.6" type="counter"/>
      </group>

      <!-- lmsensors MIBs -->
      <group ifType="all" name="lmsensors-temp">
        <mibObj alias="lms-tempdevice" instance="lmTempIndex" oid=".1.3.6.1.4.1.2021.13.16.2.1.2" type="string"/>
        <mibObj alias="lms-temp" instance="lmTempIndex" oid=".1.3.6.1.4.1.2021.13.16.2.1.3" type="gauge32"/>
      </group>

      <group ifType="all" name="lmsensors-fan">
        <mibObj alias="lms-fandevice" instance="lmFanIndex" oid=".1.3.6.1.4.1.2021.13.16.3.1.2" type="string"/>
        <mibObj alias="lms-fan" instance="lmFanIndex" oid=".1.3.6.1.4.1.2021.13.16.3.1.3" type="gauge32"/>
      </group>

      <group ifType="all" name="lmsensors-volt">
        <mibObj alias="lms-voltdevice" instance="lmVoltIndex" oid=".1.3.6.1.4.1.2021.13.16.4.1.2" type="string"/>
        <mibObj alias="lms-volt" instance="lmVoltIndex" oid=".1.3.6.1.4.1.2021.13.16.4.1.3" type="gauge32"/>
      </group>

      <systemDef name="Net-SNMP (UCD)">
        <sysoidMask>.1.3.6.1.4.1.2021.250.</sysoidMask>
        <collect>
          <includeGroup>mib2-host-resources-system</includeGroup>
          <includeGroup>mib2-host-resources-memory</includeGroup>
          <includeGroup>mib2-host-resources-storage</includeGroup>
          <includeGroup>net-snmp-disk</includeGroup>
          <includeGroup>net-snmp-disk-more</includeGroup>
          <includeGroup>net-snmp-disk-highlow</includeGroup>
          <includeGroup>ucd-loadavg</includeGroup>
          <includeGroup>ucd-memory</includeGroup>
          <includeGroup>ucd-sysstat</includeGroup>
          <includeGroup>ucd-sysstat-raw</includeGroup>
          <includeGroup>ucd-sysstat-raw-more</includeGroup>
          <includeGroup>lmsensors-temp</includeGroup>
          <includeGroup>lmsensors-fan</includeGroup>
          <includeGroup>lmsensors-volt</includeGroup>
        </collect>
      </systemDef>

      <systemDef name="Net-SNMP">
        <sysoidMask>.1.3.6.1.4.1.8072.3.</sysoidMask>
        <collect>
          <includeGroup>mib2-host-resources-system</includeGroup>
          <includeGroup>mib2-host-resources-memory</includeGroup>
          <includeGroup>mib2-X-interfaces</includeGroup>
          <includeGroup>mib2-X-interfaces-pkts</includeGroup>
          <includeGroup>net-snmp-disk</includeGroup>
          <includeGroup>net-snmp-disk-more</includeGroup>
          <includeGroup>net-snmp-disk-highlow</includeGroup>
          <includeGroup>openmanage-coolingdevices</includeGroup>
          <includeGroup>openmanage-temperatureprobe</includeGroup>
          <includeGroup>openmanage-powerusage</includeGroup>
          <includeGroup>ucd-loadavg</includeGroup>
          <includeGroup>ucd-memory</includeGroup>
          <includeGroup>ucd-sysstat</includeGroup>
          <includeGroup>ucd-sysstat-raw</includeGroup>
          <includeGroup>ucd-sysstat-raw-more</includeGroup>
          <includeGroup>lmsensors-temp</includeGroup>
          <includeGroup>lmsensors-fan</includeGroup>
          <includeGroup>lmsensors-volt</includeGroup>
        </collect>
      </systemDef>

      <systemDef name="Net-SNMP 5.5 with sysObjectID bug on i386">
        <sysoid>.1.3</sysoid>
        <collect>
          <includeGroup>mib2-interfaces</includeGroup>
          <includeGroup>mib2-tcp</includeGroup>
          <includeGroup>mib2-powerethernet</includeGroup>
          <includeGroup>mib2-host-resources-system</includeGroup>
          <includeGroup>mib2-host-resources-memory</includeGroup>
          <includeGroup>mib2-X-interfaces</includeGroup>
          <includeGroup>mib2-X-interfaces-pkts</includeGroup>
          <includeGroup>net-snmp-disk</includeGroup>
          <includeGroup>net-snmp-disk-more</includeGroup>
          <includeGroup>net-snmp-disk-highlow</includeGroup>
          <includeGroup>openmanage-coolingdevices</includeGroup>
          <includeGroup>openmanage-temperatureprobe</includeGroup>
          <includeGroup>openmanage-powerusage</includeGroup>
          <includeGroup>ucd-loadavg</includeGroup>
          <includeGroup>ucd-memory</includeGroup>
          <includeGroup>ucd-sysstat</includeGroup>
          <includeGroup>ucd-sysstat-raw</includeGroup>
          <includeGroup>ucd-sysstat-raw-more</includeGroup>
          <includeGroup>lmsensors-temp</includeGroup>
          <includeGroup>lmsensors-fan</includeGroup>
          <includeGroup>lmsensors-volt</includeGroup>
        </collect>
      </systemDef>

      <systemDef name="Net-SNMP 5.5 with sysObjectID bug on x86_64">
        <sysoid>.0.1</sysoid>
        <collect>
          <includeGroup>mib2-interfaces</includeGroup>
          <includeGroup>mib2-tcp</includeGroup>
          <includeGroup>mib2-icmp</includeGroup>
          <includeGroup>mib2-powerethernet</includeGroup>
          <includeGroup>mib2-host-resources-system</includeGroup>
          <includeGroup>mib2-host-resources-memory</includeGroup>
          <includeGroup>mib2-X-interfaces</includeGroup>
          <includeGroup>mib2-X-interfaces-pkts</includeGroup>
          <includeGroup>net-snmp-disk</includeGroup>
          <includeGroup>net-snmp-disk-more</includeGroup>
          <includeGroup>net-snmp-disk-highlow</includeGroup>
          <includeGroup>openmanage-coolingdevices</includeGroup>
          <includeGroup>openmanage-temperatureprobe</includeGroup>
          <includeGroup>openmanage-powerusage</includeGroup>
          <includeGroup>ucd-loadavg</includeGroup>
          <includeGroup>ucd-memory</includeGroup>
          <includeGroup>ucd-sysstat</includeGroup>
          <includeGroup>ucd-sysstat-raw</includeGroup>
          <includeGroup>ucd-sysstat-raw-more</includeGroup>
          <includeGroup>lmsensors-temp</includeGroup>
          <includeGroup>lmsensors-fan</includeGroup>
          <includeGroup>lmsensors-volt</includeGroup>
        </collect>
      </systemDef>
</datacollection-group>