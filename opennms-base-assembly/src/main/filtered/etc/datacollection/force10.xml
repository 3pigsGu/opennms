<?xml version="1.0" encoding="UTF-8" standalone="no"?><datacollection-group name="Force10">

    <resourceType label="Force10 Stack Unit" name="force10StackUnit" resourceLabel="Unit ${index}: ${f10UnitDescr}">
      <persistenceSelectorStrategy class="org.opennms.netmgt.collectd.PersistAllSelectorStrategy"/>
      <storageStrategy class="org.opennms.netmgt.dao.support.IndexStorageStrategy"/>
    </resourceType>

      <group ifType="all" name="force10-stack-unit-stats">
        <mibObj alias="force10CpuUsage" instance="force10StackUnit" oid=".1.3.6.1.4.1.6027.3.10.1.2.9.1.3" type="gauge"/>
        <mibObj alias="force10MemUsage" instance="force10StackUnit" oid=".1.3.6.1.4.1.6027.3.10.1.2.9.1.5" type="gauge"/>
        <mibObj alias="f10UnitDescr" instance="force10StackUnit" oid=".1.3.6.1.4.1.6027.3.10.1.2.2.1.9" type="string"/>
        <mibObj alias="force10UnitTemp" instance="force10StackUnit" oid=".1.3.6.1.4.1.6027.3.10.1.2.2.1.14" type="gauge"/>
      </group>

      <systemDef name="Force10 10Gbe switches">
        <sysoidMask>.1.3.6.1.4.1.6027.</sysoidMask>
        <collect>
          <includeGroup>mib2-X-interfaces</includeGroup>
          <includeGroup>mib2-X-interfaces-pkts</includeGroup>
          <includeGroup>force10-stack-unit-stats</includeGroup>
        </collect>
      </systemDef>

</datacollection-group>