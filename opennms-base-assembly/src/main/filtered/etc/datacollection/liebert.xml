<?xml version="1.0" encoding="UTF-8" standalone="no"?><datacollection-group name="Liebert">

    <!-- Liebert Resource Types -->
    <resourceType label="Liebert Input Line Index" name="lcUpsInputLine">
      <persistenceSelectorStrategy class="org.opennms.netmgt.collectd.PersistAllSelectorStrategy"/>
      <storageStrategy class="org.opennms.netmgt.dao.support.IndexStorageStrategy"/>
    </resourceType>

    <resourceType label="Liebert Output Line Index" name="lcUpsOutputLine">
      <persistenceSelectorStrategy class="org.opennms.netmgt.collectd.PersistAllSelectorStrategy"/>
      <storageStrategy class="org.opennms.netmgt.dao.support.IndexStorageStrategy"/>
    </resourceType>

    <resourceType label="Liebert Bypass Line Index" name="lcUpsBypassLine">
      <persistenceSelectorStrategy class="org.opennms.netmgt.collectd.PersistAllSelectorStrategy"/>
      <storageStrategy class="org.opennms.netmgt.dao.support.IndexStorageStrategy"/>
    </resourceType>

    <resourceType label="Liebert Bypass Line Index" name="lsUpsBypassLine">
      <persistenceSelectorStrategy class="org.opennms.netmgt.collectd.PersistAllSelectorStrategy"/>
      <storageStrategy class="org.opennms.netmgt.dao.support.IndexStorageStrategy"/>
    </resourceType>

    <resourceType label="Liebert Bypass Line Index" name="ls43cUpsBypassLine">
      <persistenceSelectorStrategy class="org.opennms.netmgt.collectd.PersistAllSelectorStrategy"/>
      <storageStrategy class="org.opennms.netmgt.dao.support.IndexStorageStrategy"/>
    </resourceType>

    <resourceType label="Liebert Temp Index Fahrenheit" name="lgpEnvTemperatureIdDegF">
      <persistenceSelectorStrategy class="org.opennms.netmgt.collectd.PersistAllSelectorStrategy"/>
      <storageStrategy class="org.opennms.netmgt.dao.support.IndexStorageStrategy"/>
    </resourceType>

    <resourceType label="Liebert Power Measurement Index" name="lgpPwrMeasurementPtIndex" resourceLabel="Measurement Point ${subIndex(0,1)}, Phase ${subIndex(1,1)}">
      <persistenceSelectorStrategy class="org.opennms.netmgt.collectd.PersistAllSelectorStrategy"/>
      <storageStrategy class="org.opennms.netmgt.dao.support.IndexStorageStrategy"/>
    </resourceType>

      <!-- Liebert SiteNet mibs for discontinued products -->
      <group ifType="ignore" name="liebert-sitenet">
        <mibObj alias="lcUpsBatTimeRemain" instance="0" oid=".1.3.6.1.4.1.476.1.1.1.1.2.1" type="INTEGER"/>
        <mibObj alias="lcUpsBatTemperature" instance="0" oid=".1.3.6.1.4.1.476.1.1.1.1.2.2" type="INTEGER"/>
        <mibObj alias="lcUpsInputBrownOuts" instance="0" oid=".1.3.6.1.4.1.476.1.1.1.1.3.2" type="Counter"/>
        <mibObj alias="lcUpsInputBlackOuts" instance="0" oid=".1.3.6.1.4.1.476.1.1.1.1.3.3" type="Counter"/>
        <mibObj alias="lcUpsInputTransient" instance="0" oid=".1.3.6.1.4.1.476.1.1.1.1.3.4" type="Counter"/>
        <mibObj alias="lcUpsOutputLoad" instance="0" oid=".1.3.6.1.4.1.476.1.1.1.1.4.2" type="INTEGER"/>
        <mibObj alias="lcUpsOutputWatts" instance="0" oid=".1.3.6.1.4.1.476.1.1.1.1.4.5" type="INTEGER"/>
        <mibObj alias="lcUpsInverterTemp" instance="0" oid=".1.3.6.1.4.1.476.1.1.1.1.5.2" type="INTEGER"/>
        <mibObj alias="lcUpsAlarms" instance="0" oid=".1.3.6.1.4.1.476.1.1.1.1.6.1" type="Gauge"/>
      </group>

      <group ifType="all" name="liebert-sitenet-power">
        <mibObj alias="lcUpsInputLine" instance="lcUpsInputLine" oid=".1.3.6.1.4.1.476.1.1.1.1.3.6.1.1" type="INTEGER"/>
        <mibObj alias="lcUpsInputVoltage" instance="lcUpsInputLine" oid=".1.3.6.1.4.1.476.1.1.1.1.3.6.1.2" type="INTEGER"/>
        <mibObj alias="lcUpsInputCurrent" instance="lcUpsInputLine" oid=".1.3.6.1.4.1.476.1.1.1.1.3.6.1.3" type="INTEGER"/>
        <mibObj alias="lcUpsInputVA" instance="lcUpsInputLine" oid=".1.3.6.1.4.1.476.1.1.1.1.3.6.1.4" type="INTEGER"/>
        <mibObj alias="lcUpsOutputLine" instance="lcUpsOutputLine" oid=".1.3.6.1.4.1.476.1.1.1.1.4.4.1.1" type="INTEGER"/>
        <mibObj alias="lcUpsOutputVoltage" instance="lcUpsOutputLine" oid=".1.3.6.1.4.1.476.1.1.1.1.4.4.1.2" type="INTEGER"/>
        <mibObj alias="lcUpsOutputCurrent" instance="lcUpsOutputLine" oid=".1.3.6.1.4.1.476.1.1.1.1.4.4.1.3" type="INTEGER"/>
        <mibObj alias="lcUpsOutputVA" instance="lcUpsOutputLine" oid=".1.3.6.1.4.1.476.1.1.1.1.4.4.1.4" type="INTEGER"/>
        <mibObj alias="lcUpsBypassLine" instance="lcUpsBypassLine" oid=".1.3.6.1.4.1.476.1.1.1.1.13.4.1.1" type="INTEGER"/>
        <mibObj alias="lcUpsBypassVoltage" instance="lcUpsBypassLine" oid=".1.3.6.1.4.1.476.1.1.1.1.13.4.1.2" type="INTEGER"/>
        <mibObj alias="lcUpsBypassCurrent" instance="lcUpsBypassLine" oid=".1.3.6.1.4.1.476.1.1.1.1.13.4.1.3" type="INTEGER"/>
        <mibObj alias="lsUpsBypassLine" instance="lsUpsBypassLine" oid=".1.3.6.1.4.1.476.1.1.1.2.13.4.1.1" type="INTEGER"/>
        <mibObj alias="lsUpsBypassVoltage" instance="lsUpsBypassLine" oid=".1.3.6.1.4.1.476.1.1.1.2.13.4.1.2" type="INTEGER"/>
        <mibObj alias="lsUpsBypassCurrent" instance="lsUpsBypassLine" oid=".1.3.6.1.4.1.476.1.1.1.2.13.4.1.3" type="INTEGER"/>
        <mibObj alias="ls43cUpsBypassLine" instance="ls43cUpsBypassLine" oid=".1.3.6.1.4.1.476.1.1.1.10.13.4.1.1" type="INTEGER"/>
        <mibObj alias="ls43cUpsBypassVolt" instance="ls43cUpsBypassLine" oid=".1.3.6.1.4.1.476.1.1.1.10.13.4.1.2" type="INTEGER"/>
        <mibObj alias="ls43cUpsBypassCurr" instance="ls43cUpsBypassLine" oid=".1.3.6.1.4.1.476.1.1.1.10.13.4.1.3" type="INTEGER"/>
      </group>

      <!-- Liebert mibs -->
      <group ifType="ignore" name="liebert-basics">
        <mibObj alias="lgpPwrBrownOutCnt" instance="0" oid=".1.3.6.1.4.1.476.1.42.3.5.8.1" type="integer"/>
        <mibObj alias="lgpPwrBlackOutCnt" instance="0" oid=".1.3.6.1.4.1.476.1.42.3.5.8.2" type="integer"/>
        <mibObj alias="lgpPwrBattTimeRem" instance="0" oid=".1.3.6.1.4.1.476.1.42.3.5.1.18" type="integer"/>
        <mibObj alias="lgpPwrBattCapacity" instance="0" oid=".1.3.6.1.4.1.476.1.42.3.5.1.19" type="integer"/>
      </group>

      <group ifType="all" name="liebert-temp">
        <mibObj alias="lgpEnvTempDegF" instance="lgpEnvTemperatureIdDegF" oid=".1.3.6.1.4.1.476.1.42.3.4.1.2.3.1.3" type="Integer32"/>
      </group>

      <group ifType="all" name="liebert-power">
        <mibObj alias="lgpPwrCurrent" instance="lgpPwrMeasurementPtIndex" oid=".1.3.6.1.4.1.476.1.42.3.5.2.3.1.6" type="Integer32"/>
        <mibObj alias="lgpPwrMaxVolts" instance="lgpPwrMeasurementPtIndex" oid=".1.3.6.1.4.1.476.1.42.3.5.2.3.1.16" type="Integer32"/>
        <mibObj alias="lgpPwrMinVolts" instance="lgpPwrMeasurementPtIndex" oid=".1.3.6.1.4.1.476.1.42.3.5.2.3.1.17" type="Integer32"/>
        <mibObj alias="lgpPwrVolts" instance="lgpPwrMeasurementPtIndex" oid=".1.3.6.1.4.1.476.1.42.3.5.2.3.1.20" type="Integer32"/>
      </group>

      <group ifType="all" name="liebert-power-more">
        <mibObj alias="lgpPwrPwrFactor" instance="lgpPwrMeasurementPtIndex" oid=".1.3.6.1.4.1.476.1.42.3.5.2.3.1.14" type="Integer32"/>
        <mibObj alias="lgpPwrPctLoad" instance="lgpPwrMeasurementPtIndex" oid=".1.3.6.1.4.1.476.1.42.3.5.2.3.1.17" type="Integer32"/>
      </group>  

      <systemDef name="Liebert UPS">
        <sysoidMask>.1.3.6.1.4.1.476.</sysoidMask>
        <collect>
          <includeGroup>mib2-ups-rfc1628</includeGroup>
          <includeGroup>liebert-basics</includeGroup>
          <includeGroup>liebert-temp</includeGroup>
          <includeGroup>liebert-power</includeGroup>
          <includeGroup>liebert-power-more</includeGroup>
          <includeGroup>liebert-sitenet</includeGroup>
          <includeGroup>liebert-sitenet-power</includeGroup>
        </collect>
      </systemDef>

</datacollection-group>