<?xml version="1.0" encoding="UTF-8" standalone="no"?><datacollection-group name="Mikrotik">

    <!-- Mikrotik Resource Types -->

    <resourceType label="Local PtP or SU Interface (Mikrotik)" name="mtxrWlStatIndex" resourceLabel="${mtxrWlStatSsid}">
       <persistenceSelectorStrategy class="org.opennms.netmgt.collectd.PersistAllSelectorStrategy"/>
       <storageStrategy class="org.opennms.netmgt.dao.support.IndexStorageStrategy"/>
    </resourceType>

    <resourceType label="Subscriber or Remote Radio (Mikrotik)" name="mtxrWlRtabAddr" resourceLabel="SU ${index}">
       <persistenceSelectorStrategy class="org.opennms.netmgt.collectd.PersistAllSelectorStrategy"/>
       <storageStrategy class="org.opennms.netmgt.dao.support.IndexStorageStrategy"/>
    </resourceType>

      <!-- Mikrotik MIBs -->

      <group ifType="all" name="mtxrWlStatIndex">
        <mibObj alias="mtxrWlStatTxRate" instance="mtxrWlStatIndex" oid=".1.3.6.1.4.1.14988.1.1.1.1.1.2" type="Gauge32"/>
        <mibObj alias="mtxrWlStatRxRate" instance="mtxrWlStatIndex" oid=".1.3.6.1.4.1.14988.1.1.1.1.1.3" type="Gauge32"/>
        <mibObj alias="mtxrWlStatStrength" instance="mtxrWlStatIndex" oid=".1.3.6.1.4.1.14988.1.1.1.1.1.4" type="Integer32"/>
        <mibObj alias="mtxrWlStatSsid" instance="mtxrWlStatIndex" oid=".1.3.6.1.4.1.14988.1.1.1.1.1.5" type="String"/>
        <mibObj alias="mtxrWlStatBssid" instance="mtxrWlStatIndex" oid=".1.3.6.1.4.1.14988.1.1.1.1.1.6" type="OctetString"/> 
        <mibObj alias="mtxrWlStatFreq" instance="mtxrWlStatIndex" oid=".1.3.6.1.4.1.14988.1.1.1.1.1.7" type="Integer32"/>
      </group>

      <group ifType="all" name="mtxrWlRtabAddr">
        <mibObj alias="mtxrWlRtabAddr" instance="mtxrWlRtabAddr" oid=".1.3.6.1.4.1.14988.1.1.1.2.1.1" type="OctetString"/> 
        <mibObj alias="mtxrWlRtabStrength" instance="mtxrWlRtabAddr" oid=".1.3.6.1.4.1.14988.1.1.1.2.1.3" type="Integer32"/>
        <mibObj alias="mtxrWlRtabTxBytes" instance="mtxrWlRtabAddr" oid=".1.3.6.1.4.1.14988.1.1.1.2.1.4" type="Counter32"/>
        <mibObj alias="mtxrWlRtabRxBytes" instance="mtxrWlRtabAddr" oid=".1.3.6.1.4.1.14988.1.1.1.2.1.5" type="Counter32"/>
        <mibObj alias="mtxrWlRtabTxPackets" instance="mtxrWlRtabAddr" oid=".1.3.6.1.4.1.14988.1.1.1.2.1.6" type="Counter32"/>
        <mibObj alias="mtxrWlRtabRxPackets" instance="mtxrWlRtabAddr" oid=".1.3.6.1.4.1.14988.1.1.1.2.1.7" type="Counter32"/>
        <mibObj alias="mtxrWlRtabTxRate" instance="mtxrWlRtabAddr" oid=".1.3.6.1.4.1.14988.1.1.1.2.1.8" type="Gauge32"/>
        <mibObj alias="mtxrWlRtabRxRate" instance="mtxrWlRtabAddr" oid=".1.3.6.1.4.1.14988.1.1.1.2.1.9" type="Gauge32"/>
      </group>        

      <group ifType="ignore" name="mtxrHealth">
        <mibObj alias="mtxrHlCoreVolt" instance="0" oid=".1.3.6.1.4.1.14988.1.1.3.1" type="Integer32"/>
        <mibObj alias="mtxrHlThreePtThreeV" instance="0" oid=".1.3.6.1.4.1.14988.1.1.3.2" type="Integer32"/>
        <mibObj alias="mtxrHlFiveVolt" instance="0" oid=".1.3.6.1.4.1.14988.1.1.3.3" type="Integer32"/>
        <mibObj alias="mtxrHlTwelveVolt" instance="0" oid=".1.3.6.1.4.1.14988.1.1.3.4" type="Integer32"/>
        <mibObj alias="mtxrHlSensorTemp" instance="0" oid=".1.3.6.1.4.1.14988.1.1.3.5" type="Integer32"/>
        <mibObj alias="mtxrHlCpuTemp" instance="0" oid=".1.3.6.1.4.1.14988.1.1.3.6" type="Integer32"/>
        <mibObj alias="mtxrHlBoardTemp" instance="0" oid=".1.3.6.1.4.1.14988.1.1.3.7" type="Integer32"/>
      </group>     

      <systemDef name="Mikrotik Wireless Router">
        <sysoidMask>.1.3.6.1.4.1.14988.</sysoidMask>
        <collect>
          <includeGroup>mtxrWlStatIndex</includeGroup>
          <includeGroup>mtxrWlRtabAddr</includeGroup>
          <includeGroup>mtxrHealth</includeGroup>
        </collect>
      </systemDef>

</datacollection-group>