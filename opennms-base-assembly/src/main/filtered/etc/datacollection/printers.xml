<?xml version="1.0" encoding="UTF-8" standalone="no"?><datacollection-group name="Printers">

    <!-- PRINTER-MIB Resource Types -->
    <resourceType label="Printer Marker Supply" name="prtMarkerSuppliesIndex" resourceLabel="${prtMarkerSuppliesDescription}">
      <persistenceSelectorStrategy class="org.opennms.netmgt.collectd.PersistAllSelectorStrategy"/>
      <storageStrategy class="org.opennms.netmgt.dao.support.IndexStorageStrategy"/>
    </resourceType>

      <group ifType="ignore" name="printer-usage">
        <mibObj alias="lifeCount" instance="1" oid=".1.3.6.1.2.1.43.10.2.1.4.1" type="Gauge"/>
        <mibObj alias="powerOnCount" instance="1" oid=".1.3.6.1.2.1.43.10.2.1.5.1" type="Gauge"/>
      </group>

      <group ifType="all" name="printer-mib-supplies">
        <mibObj alias="prtMarkerSuppliesDescription" instance="prtMarkerSuppliesIndex" oid=".1.3.6.1.2.1.43.11.1.1.6" type="string"/>
        <mibObj alias="prtMSMaxCapacity" instance="prtMarkerSuppliesIndex" oid=".1.3.6.1.2.1.43.11.1.1.8" type="integer"/>
        <mibObj alias="prtMSLevel" instance="prtMarkerSuppliesIndex" oid=".1.3.6.1.2.1.43.11.1.1.9" type="integer"/>
      </group>

</datacollection-group>