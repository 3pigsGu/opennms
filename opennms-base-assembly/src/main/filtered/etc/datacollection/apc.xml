<?xml version="1.0" encoding="UTF-8" standalone="no"?><datacollection-group name="APC">

    <!-- APC Resource Types -->
    <resourceType label="APC PDU Amp current" name="rPDULoadStatusIndex">
      <persistenceSelectorStrategy class="org.opennms.netmgt.collectd.PersistAllSelectorStrategy"/>
      <storageStrategy class="org.opennms.netmgt.dao.support.IndexStorageStrategy"/>
    </resourceType>

      <!-- APC mibs -->
      <group ifType="ignore" name="APC">
        <mibObj alias="apcpercent" instance="0" oid=".1.3.6.1.4.1.318.1.1.1.2.2.1" type="gauge32"/>
        <mibObj alias="apctemp" instance="0" oid=".1.3.6.1.4.1.318.1.1.1.2.2.2" type="gauge32"/>
        <mibObj alias="apctime" instance="0" oid=".1.3.6.1.4.1.318.1.1.1.2.2.3" type="TimeTicks"/>
        <mibObj alias="apcoutvac" instance="0" oid=".1.3.6.1.4.1.318.1.1.1.4.2.1" type="gauge32"/>
        <mibObj alias="apcload" instance="0" oid=".1.3.6.1.4.1.318.1.1.1.4.2.3" type="gauge32"/>
        <mibObj alias="apccurrent" instance="0" oid=".1.3.6.1.4.1.318.1.1.1.4.2.4" type="gauge32"/>
        <mibObj alias="apcinvac" instance="0" oid=".1.3.6.1.4.1.318.1.1.1.3.2.1" type="gauge32"/>
      </group>

      <group ifType="all" name="APC-RackPDU">
        <mibObj alias="rPDULoadStatusLoad" instance="rPDULoadStatusIndex" oid=".1.3.6.1.4.1.318.1.1.12.2.3.1.1.2" type="Gauge"/>
      </group>

      <systemDef name="APC UPS">
        <sysoidMask>.1.3.6.1.4.1.318.</sysoidMask>
        <collect>
          <includeGroup>APC</includeGroup>
          <includeGroup>APC-RackPDU</includeGroup>
          <includeGroup>mib2-ups-rfc1628</includeGroup>
        </collect>
      </systemDef>

</datacollection-group>