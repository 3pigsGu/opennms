<?xml version="1.0" encoding="UTF-8" standalone="no"?><wmi-datacollection-config rrdRepository="${install.share.dir}/rrd/snmp/">
    <wmi-collection name="default">
        <rrd step="300">
		  <rra>RRA:AVERAGE:0.5:1:2016</rra>
		  <rra>RRA:AVERAGE:0.5:12:1488</rra>
		  <rra>RRA:AVERAGE:0.5:288:366</rra>
		  <rra>RRA:MAX:0.5:288:366</rra>
		  <rra>RRA:MIN:0.5:288:366</rra>
        </rrd>

        <wpms>
            <!--  A group for collecting System Memory statistics.
             *  Check the keyvalue "Name" - if it's there (should be) collect this whole group.
             *  wmiClass is the WMI object class.
             *  Check every recheckInterval milliseconds (3600000 = 1hr)
            -->
            <wpm ifType="all" keyvalue="Name" name="wmiOSMemory" recheckInterval="3600000" resourceType="node" wmiClass="Win32_PerfFormattedData_PerfOS_Memory" wmiNamespace="root/cimv2">
                <!-- Amount of physical memory in bytes available to processes -->
                <attrib alias="wmiOSMemAvailBytes" name="AvailableBytes" type="Gauge" wmiObject="AvailableBytes"/>
                <!-- Number of bytes currently being used by the file system cache -->
                <attrib alias="wmiOSMemCacheBytes" name="CacheBytes" type="Gauge" wmiObject="CacheBytes"/>
                <!-- Number of faults which occur when a page is not found in the file system cache -->
                <attrib alias="wmiOSMemCacheFaults" name="CacheFaultsPerSec" type="Counter" wmiObject="CacheFaultsPerSec"/>
                <!-- Amount of virtual memory, in bytes, that can be committed without having to extend the paging files -->
                <attrib alias="wmiOSMemCommitLimit" name="CommitLimit" type="Gauge" wmiObject="CommitLimit"/>
                <!-- Amount of committed virtual memory, in bytes -->
                <attrib alias="wmiOSMemCommitted" name="CommittedBytes" type="Gauge" wmiObject="CommittedBytes"/>
                <!-- Overall rate at which faulted pages are handled by the processor -->
                <attrib alias="wmiOSMemPgFaultsSec" name="PageFaultsPerSec" type="Gauge" wmiObject="PageFaultsPerSec"/>
                <!-- Number of calls to allocate space in the nonpaged pool -->
                <attrib alias="wmiOSMemNPPAllocs" name="PoolNonpagedAllocs" type="Counter" wmiObject="PoolNonpagedAllocs"/>
                <!-- Number of calls to allocate space in the paged pool -->
                <attrib alias="wmiOSMemPPAllocs" name="PoolPagedAllocs" type="Counter" wmiObject="PoolPagedAllocs"/>
            </wpm>

            <!--  A group for collecting System Objects statistics.
             *  Check the keyvalue "Name" - if it's there (should be) collect this whole group.
             *  wmiClass is the WMI object class.
             *  Check every recheckInterval milliseconds (3600000 = 1hr)
            -->
            <wpm ifType="all" keyvalue="Name" name="wmiOSObjects" recheckInterval="3600000" resourceType="node" wmiClass="Win32_PerfFormattedData_PerfOS_Objects" wmiNamespace="root/cimv2">
                <!-- Number of events in the computer at the time of data collection -->
                <!-- An event is used when two or more threads wish to synchronize execution -->
                <attrib alias="wmiOSObjEvents" name="Events" type="Gauge" wmiObject="Events"/>
                <!-- Count of the number of mutexes in the computer -->
                <attrib alias="wmiOSObjMutexes" name="Mutexes" type="Gauge" wmiObject="Mutexes"/>
                <!-- Number of processes in the computer -->
                <attrib alias="wmiOSObjProcesses" name="Processes" type="Gauge" wmiObject="Processes"/>
                <!-- Number of sections in the computer. Sounds like shared memory segments. -->
                <attrib alias="wmiOSObjSections" name="Sections" type="Gauge" wmiObject="Sections"/>
                <!-- Number of semaphores in the computer -->
                <attrib alias="wmiOSObjSemaphores" name="Semaphores" type="Gauge" wmiObject="Semaphores"/>
                <!-- Number of threads in the computer -->
                <attrib alias="wmiOSObjThreads" name="Threads" type="Gauge" wmiObject="Threads"/>
            </wpm>

            <!--  A group for collecting System Processor statistics.
             *  Check the keyvalue "Name" - if it's there (should be) collect this whole group.
             *  wmiClass is the WMI object class.
             *  Check every recheckInterval milliseconds (3600000 = 1hr)
            -->
            <wpm ifType="all" keyvalue="Name" name="wmiOSCpu" recheckInterval="3600000" resourceType="wmiOSCpu" wmiClass="Win32_PerfFormattedData_PerfOS_Processor" wmiNamespace="root/cimv2">
                <!-- Label for this Processor instance -->
                <attrib alias="wmiOSCpuName" name="ProcessorName" type="string" wmiObject="Name"/>
                <!-- Average number of hardware interrupts that the processor is receiving and servicing in each second -->
                <attrib alias="wmiOSCpuIntsPerSec" name="InterruptsPerSec" type="Gauge" wmiObject="InterruptsPerSec"/>
                <!-- Percentage of time that the processor is executing a non-idle thread -->
                <attrib alias="wmiOSCpuPctProcTime" name="PercentProcessorTime" type="Gauge" wmiObject="PercentProcessorTime"/>
                <!-- Percentage of time that the processor spent receiving and servicing deferred procedure calls -->
                <attrib alias="wmiOSCpuPctDPCTime" name="PercentDPCTime" type="Gauge" wmiObject="PercentDPCTime"/>
                <!-- Percentage of time that the processor spent receiving and servicing hardware interrupts -->
                <attrib alias="wmiOSCpuPctIntrTime" name="PercentInterruptTime" type="Gauge" wmiObject="PercentInterruptTime"/>
                <!-- Percentage of nonidle processor time spent in user mode -->
                <attrib alias="wmiOSCpuPctUserTime" name="PercentUserTime" type="Gauge" wmiObject="PercentUserTime"/>
            </wpm>

            <!--  A group for collecting Physical Disk statistics.
             *  Check the keyvalue "Name" - if it's there (should be) collect this whole group.
             *  wmiClass is the WMI object class.
             *  Check every recheckInterval milliseconds (3600000 = 1hr)
            -->
            <wpm ifType="all" keyvalue="Name" name="wmiPhysicalDisk" recheckInterval="3600000" resourceType="wmiPhysicalDisk" wmiClass="Win32_PerfFormattedData_PerfDisk_PhysicalDisk" wmiNamespace="root/cimv2">
                <!-- Label for this PhysicalDisk instance -->
                <attrib alias="wmiPDName" name="PhysDiskName" type="string" wmiObject="Name"/>
                <!-- Percentage of elapsed time that the selected disk drive is busy servicing read requests -->
                <attrib alias="wmiPDPctDiskRdTime" name="PercentPhysDiskReadTime" type="Gauge" wmiObject="PercentDiskReadTime"/>
                <!-- Percentage of elapsed time that the selected disk drive is busy servicing write requests -->
                <attrib alias="wmiPDPctDiskWrTime" name="PercentPhysDiskWriteTime" type="Gauge" wmiObject="PercentDiskWriteTime"/>
                <!-- Rate at which I/Os to the disk were split into multiple I/Os -->
                <attrib alias="wmiPDSplitIOsPerSec" name="SplitPhysIOPerSec" type="Gauge" wmiObject="SplitIOPerSec"/>
            </wpm>

            <!--  A group for collecting Logical Disk statistics.
             *  Check the keyvalue "Name" - if it's there (should be) collect this whole group.
             *  wmiClass is the WMI object class.
             *  Check every recheckInterval milliseconds (3600000 = 1hr)
            -->
            <wpm ifType="all" keyvalue="Name" name="wmiLogicalDisk" recheckInterval="3600000" resourceType="wmiLogicalDisk" wmiClass="Win32_PerfFormattedData_PerfDisk_LogicalDisk" wmiNamespace="root/cimv2">
                <!-- Label for this LogicalDisk instance -->
                <attrib alias="wmiLDName" name="LogDiskName" type="string" wmiObject="Name"/>
                <!-- Unallocated space on the disk drive in megabytes -->
                <attrib alias="wmiLDPctFreeMBytes" name="FreeMegabytes" type="Gauge" wmiObject="FreeMegabytes"/>
                <!-- Percentage of elapsed time that the selected disk drive is busy servicing read requests -->
                <attrib alias="wmiLDPctDiskRdTime" name="PercentLogDiskReadTime" type="Gauge" wmiObject="PercentDiskReadTime"/>
                <!-- Percentage of elapsed time that the selected disk drive is busy servicing write requests -->
                <attrib alias="wmiLDPctDiskWrTime" name="PercentLogDiskWriteTime" type="Gauge" wmiObject="PercentDiskWriteTime"/>
                <!-- Ratio of the free space available on the logical disk unit to the total usable space -->
                <attrib alias="wmiLDPctFreeSpace" name="PercentFreeSpace" type="Gauge" wmiObject="PercentFreeSpace"/>
                <!-- Rate at which I/Os to the disk were split into multiple I/Os -->
                <attrib alias="wmiLDSplitIOsPerSec" name="SplitLogIOPerSec" type="Gauge" wmiObject="SplitIOPerSec"/>
            </wpm>

            <!-- A collection of network interface statistics -->
            <wpm ifType="ignore" keyvalue="Name" name="wmiTcpipNetworkInterface" recheckInterval="3600000" resourceType="wmiTcpipNetworkInterface" wmiClass="Win32_PerfRawData_Tcpip_NetworkInterface" wmiNamespace="root/cimv2">
                <!-- The label for this Network Interface instance -->
                <attrib alias="wmiIpNetIfName" name="TcpIpIfName" type="string" wmiObject="Name"/>
                <!-- The total number of bytes per second, sent and received -->
                <attrib alias="wmiIpNetBytesSec" name="BytesTotalPerSec" type="Counter" wmiObject="BytesTotalPerSec"/>
                <!-- The number of bytes received per second -->
                <attrib alias="wmiIpNetIBytesSec" name="BytesReceivedPerSec" type="Counter" wmiObject="BytesReceivedPerSec"/>
                <!-- The number of bytes sent per second -->
                <attrib alias="wmiIpNetOBytesSec" name="BytesSentPerSec" type="Counter" wmiObject="BytesSentPerSec"/>
                <!-- The total number of packets per second, sent and received -->
                <attrib alias="wmiIpNetPktsSec" name="PacketsPersec" type="Counter" wmiObject="PacketsPersec"/>
                <!-- The total number of packets received per second -->
                <attrib alias="wmiIpNetIPktsSec" name="PacketsReceivedPersec" type="Counter" wmiObject="PacketsReceivedPersec"/>
                <!-- The number of non-unicast packets received per second -->
                <attrib alias="wmiIpNetINUPktsSec" name="PacketsReceivedNonUnicastPersec" type="Counter" wmiObject="PacketsReceivedNonUnicastPersec"/>
                <!-- The number of unicast packets received per second -->
                <attrib alias="wmiIpNetIUCPktsSec" name="PacketsReceivedUnicastPersec" type="Counter" wmiObject="PacketsReceivedUnicastPersec"/>
                <!-- The total number of packets sent per second -->
                <attrib alias="wmiIpNetOPktsSec" name="PacketsSentPersec" type="Counter" wmiObject="PacketsSentPersec"/>
                <!-- The number of non-unicast packets sent per second -->
                <attrib alias="wmiIpNetONUPktsSec" name="PacketsSentNonUnicastPersec" type="Counter" wmiObject="PacketsSentNonUnicastPersec"/>
                <!-- The number of unicast packets sent per second -->
                <attrib alias="wmiIpNetOUCPktsSec" name="PacketsSentUnicastPersec" type="Counter" wmiObject="PacketsSentUnicastPersec"/>
            </wpm>

            <!--  A group for collecting Terminal Service  statistics.
             *  Check the keyvalue "ActiveSessions" - if it's there (should be) collect this whole group.
             *  wmiClass is the WMI object class.
             *  Check every recheckInterval milliseconds (3600000 = 1hr)
            -->
            <wpm ifType="all" keyvalue="ActiveSessions" name="win32_terminalservices" recheckInterval="3600000" resourceType="node" wmiClass="Win32_PerfFormattedData_TermService_TerminalServices" wmiNamespace="root/cimv2">
                <!--  Collect these attributes.
                 *  Name is the unique name used for keyvalue.
                 *  Alias is the local name for the RRD file.
                 *  wmiObject is the WMI object instance.
                 *  Type is used to convert values around.
                -->
                <!-- The number of active sessions, including the console. -->
                <attrib alias="wmiTSSessActive" name="ActiveSessions" type="Gauge" wmiObject="ActiveSessions"/>
                <!-- The number of inactive (idle, disconnected) sessions. -->
                <attrib alias="wmiTSSessInactive" name="InactiveSessions" type="Gauge" wmiObject="InactiveSessions"/>
                <!-- The total number of sessions. -->
                <attrib alias="wmiTSSessTotal" name="TotalSessions" type="Gauge" wmiObject="TotalSessions"/>
            </wpm>

            <!-- A collection of server session statistics -->
            <wpm ifType="all" keyvalue="ServerSessions" name="win32_serversessions" recheckInterval="3600000" resourceType="node" wmiClass="Win32_PerfFormattedData_PerfNet_Server" wmiNamespace="root/cimv2">
                <!-- The number of sessions currently active in the server. -->
                <attrib alias="wmiServerSessions" name="ServerSessions" type="Gauge" wmiObject="ServerSessions"/>
                <!-- The number of sessions that have been closed due to unexpected error conditions. -->
                <attrib alias="wmiServerSessErrOut" name="SessionsErroredOut" type="Counter" wmiObject="SessionsErroredOut"/>
                <!-- The number of sessions that have been forced to logoff. -->
                <attrib alias="wmiServerSessFcdOff" name="SessionsForcedOff" type="Counter" wmiObject="SessionsForcedOff"/>
                <!-- The number of sessions that have terminated normally. -->
                <attrib alias="wmiServerSessLgdOff" name="SessionsLoggedOff" type="Counter" wmiObject="SessionsLoggedOff"/>
                <!-- The number of sessions that have been closed due to their idle time exceeding the autodisconnect parameter for the server. -->
                <attrib alias="wmiServerSessTmdOut" name="SessionsTimedOut" type="Counter" wmiObject="SessionsTimedOut"/>
            </wpm>

            <!-- A collection of server error statistics -->
            <wpm ifType="all" keyvalue="ErrorsSystem" name="win32_servererrors" recheckInterval="3600000" resourceType="node" wmiClass="Win32_PerfFormattedData_PerfNet_Server" wmiNamespace="root/cimv2">
                <!-- The number of times an internal Server Error was detected. -->
                <attrib alias="wmiServerErrsSystem" name="ErrorsSystem" type="Counter" wmiObject="ErrorsSystem"/>
                <!-- The number of times accesses to files opened successfully were denied. -->
                <attrib alias="wmiServerErrsGtdAxs" name="ErrorsGrantedAccess" type="Counter" wmiObject="ErrorsGrantedAccess"/>
                <!-- The number of times opens on behalf of clients have failed with STATUS_ACCESS_DENIED. -->
                <attrib alias="wmiServerErrsAxsPrm" name="ErrorsAccessPermissions" type="Counter" wmiObject="ErrorsAccessPermissions"/>
                <!-- The number of failed logon attempts to the server. -->
                <attrib alias="wmiServerErrsLogon" name="ErrorsLogon" type="Counter" wmiObject="ErrorsLogon"/>
            </wpm>

            <!-- A collection of HTTP statistics -->
            <wpm ifType="all" keyvalue="Name" name="win32_w3svc" recheckInterval="3600000" resourceType="wmiW3" wmiClass="Win32_PerfFormattedData_W3SVC_WebService" wmiNamespace="root/cimv2">
                <!-- Label for this Web instance -->
                <attrib alias="wmiW3Name" name="WebSvcName" type="string" wmiObject="Name"/>
                <!-- GET requests per second. -->
                <attrib alias="wmiW3GetReq" name="GetRequests" type="Counter" wmiObject="TotalGetRequests"/>
                <!-- HEAD requests per second. -->
                <attrib alias="wmiW3HeadReq" name="HeadRequests" type="Counter" wmiObject="TotalHeadRequests"/>
                <!-- POST requests per second. -->
                <attrib alias="wmiW3PostReq" name="PostRequests" type="Counter" wmiObject="TotalPostRequests"/>
                <!-- PUT requests per second. -->
                <attrib alias="wmiW3PutReq" name="PutRequests" type="Counter" wmiObject="TotalPutRequests"/>
            </wpm>
        </wpms>
    </wmi-collection>
</wmi-datacollection-config>