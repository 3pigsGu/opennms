<?xml version="1.0" encoding="UTF-8" standalone="no"?><maps-adapter-configuration element-dimension="100">
  <package name="example1">
    <filter>IPADDR != '0.0.0.0'</filter>
    <include-range begin="1.1.1.1" end="254.254.254.254"/>
  </package>
  <package name="router">
    <filter>catincRouters</filter>
    <include-range begin="1.1.1.1" end="254.254.254.254"/>
  </package>
  <package name="server">
    <filter>catincServers</filter>
    <include-range begin="1.1.1.1" end="254.254.254.254"/>
  </package>
  <!-- uncomment this to add all element to a map -->
  <cmaps>
  	<cmap mapAccess="RW" mapBG="images/background/italia.jpg" mapHeight="768" mapName="Italia" mapOwner="admin" mapWidth="1024">
		<csubmap label="NodoDiRoma" name="AllTheNetwork" x="485" y="350"/>
		<csubmap addwithoutelements="false" label="Server Map" name="server"/>
		<csubmap addwithoutelements="false" label="Router Map" name="router"/>
	</cmap>
  	<cmap mapAccess="RW" mapHeight="600" mapName="AllTheNetwork" mapOwner="admin" mapWidth="800">
  		<celement icon="switch" package="example1"/>
  	</cmap>
  	<cmap mapAccess="RW" mapHeight="600" mapName="server" mapOwner="admin" mapWidth="800">
  		<celement icon="server" package="server"/>
  	</cmap>
  	<cmap mapAccess="RW" mapHeight="600" mapName="router" mapOwner="admin" mapWidth="800">
  		<celement icon="router" package="router"/>
  	</cmap>
  </cmaps>
  
<!-- -->
</maps-adapter-configuration>