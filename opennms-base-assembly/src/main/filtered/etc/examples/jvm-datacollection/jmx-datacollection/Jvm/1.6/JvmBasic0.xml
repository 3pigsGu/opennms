<?xml version="1.0" encoding="UTF-8" standalone="no"?><mbeans>
  <!-- JVM Operating System -->
  <mbean name="OperatingSystem by JVM" objectname="java.lang:type=OperatingSystem">
    <attrib alias="OsFreeMemory" name="FreePhysicalMemorySize" type="gauge"/>
    <attrib alias="OsTotalMemory" name="TotalPhysicalMemorySize" type="gauge"/>
    <attrib alias="OsFreeSwapSpace" name="FreeSwapSpaceSize" type="gauge"/>
    <attrib alias="OsTotalSwapSpace" name="TotalSwapSpaceSize" type="gauge"/>
    <attrib alias="OsMaxFileDescCo" name="MaxFileDescriptorCount" type="gauge"/>
    <attrib alias="OsOpenFileDescCo" name="OpenFileDescriptorCount" type="gauge"/>
    <attrib alias="OsCommittVMemSize" name="CommittedVirtualMemorySize" type="gauge"/>
    <attrib alias="OsProcessCpuTime" name="ProcessCpuTime" type="gauge"/>
    <attrib alias="OsAvailableCpus" name="AvailableProcessors" type="gauge"/>
    <attrib alias="OsSysLoadAverage" name="SystemLoadAverage" type="gauge"/>
  </mbean>
  <!-- JVM COMPILATION -->
  <mbean name="java.lang.Compilation" objectname="java.lang:type=Compilation">
    <attrib alias="JvmTotalCompilTime" name="TotalCompilationTime" type="counter"/>
  </mbean>
  <!-- JVM HEAP / NON HEAP -->
  <mbean name="JVM Memory" objectname="java.lang:type=Memory">
    <attrib alias="ObjPendFinalCount" name="ObjectPendingFinalizationCount" type="gauge"/>
    <comp-attrib alias="HeapMemoryUsage" name="HeapMemoryUsage" type="Composite">
      <comp-member alias="HeapCommitted" name="committed" type="gauge"/>
      <comp-member alias="HeapMemInit" name="init" type="gauge"/>
      <comp-member alias="HeapMemMax" name="max" type="gauge"/>
      <comp-member alias="HeapMemUsed" name="used" type="gauge"/>
    </comp-attrib>
    <comp-attrib alias="NonHeapMemoryUsage" name="NonHeapMemoryUsage" type="Composite">
      <comp-member alias="NonCommitted" name="committed" type="gauge"/>
      <comp-member alias="NonHeapMemInit" name="init" type="gauge"/>
      <comp-member alias="NonHeapMemMax" name="max" type="gauge"/>
      <comp-member alias="NonHeapMemUsed" name="used" type="gauge"/>
    </comp-attrib>
  </mbean>
  <!-- JAVA Threading -->
  <mbean name="JVM Threading" objectname="java.lang:type=Threading">
    <attrib alias="ThreadCount" name="ThreadCount" type="gauge"/>
    <attrib alias="PeakThreadCount" name="PeakThreadCount" type="gauge"/>
    <attrib alias="DaemonThreadCount" name="DaemonThreadCount" type="gauge"/>
    <attrib alias="CurThreadCpuTime" name="CurrentThreadCpuTime" type="gauge"/>
  </mbean>
  <!-- JAVA ClassLoading -->
  <mbean name="JVM ClassLoading" objectname="java.lang:type=ClassLoading">
    <attrib alias="TotLoadedClasses" name="TotalLoadedClassCount" type="gauge"/>
    <attrib alias="LoadedClasses" name="LoadedClassCount" type="gauge"/>
    <attrib alias="UnloadedClass" name="UnloadedClassCount" type="gauge"/>
  </mbean>
  <!-- JAVA MemoryPool Eden Space -->
  <mbean name="JVM MemoryPool:Eden Space" objectname="java.lang:type=MemoryPool,name=Eden Space">
    <attrib alias="EdenCollUseThrsh" name="CollectionUsageThreshold" type="gauge"/>
    <attrib alias="EdenCollUseThrshCnt" name="CollectionUsageThresholdCount" type="gauge"/>
    <comp-attrib alias="EdenUsage" name="Usage" type="Composite">
      <comp-member alias="EdenUsageInit" name="init" type="gauge"/>
      <comp-member alias="EdenUsageMax" name="max" type="gauge"/>
      <comp-member alias="EdenUsageUsed" name="used" type="gauge"/>
      <comp-member alias="EdenUsgComit" name="committed" type="gauge"/>
    </comp-attrib>
    <comp-attrib alias="EdenPeakUsage" name="PeakUsage" type="Composite">
      <comp-member alias="EdenPeakUsageInit" name="init" type="gauge"/>
      <comp-member alias="EdenPeakUsageMax" name="max" type="gauge"/>
      <comp-member alias="EdenPeakUsageUsed" name="used" type="gauge"/>
      <comp-member alias="EdenPeakUsgComit" name="committed" type="gauge"/>
    </comp-attrib>
  </mbean>
  <!-- JVM MemoryPool SurvivorSpace -->
  <mbean name="JVM MemoryPool:Survivor Space" objectname="java.lang:type=MemoryPool,name=Survivor Space">
    <attrib alias="SurvCollUseThresh" name="CollectionUsageThreshold" type="gauge"/>
    <attrib alias="SurvCollUseThrshCnt" name="CollectionUsageThresholdCount" type="gauge"/>
    <comp-attrib alias="SurvUsage" name="Usage" type="Composite">
      <comp-member alias="SurvUsageInit" name="init" type="gauge"/>
      <comp-member alias="SurvUsageMax" name="max" type="gauge"/>
      <comp-member alias="SurvUsageUsed" name="used" type="gauge"/>
      <comp-member alias="SurvUsgComit" name="committed" type="gauge"/>
    </comp-attrib>
    <comp-attrib alias="SurvPeakUsage" name="PeakUsage" type="Composite">
      <comp-member alias="SurvPeakUsageInit" name="init" type="gauge"/>
      <comp-member alias="SurvPeakUsageMax" name="max" type="gauge"/>
      <comp-member alias="SurvPeakUsageUsed" name="used" type="gauge"/>
      <comp-member alias="SurvPeakUsgComit" name="committed" type="gauge"/>
    </comp-attrib>
  </mbean>
  <!-- JVM MemoryPool PERM GEN -->
  <mbean name="JVM MemoryPool:Perm Gen" objectname="java.lang:type=MemoryPool,name=Perm Gen">
    <attrib alias="PermCollUseThresh" name="CollectionUsageThreshold" type="gauge"/>
    <attrib alias="PermCollUseThrshCnt" name="CollectionUsageThresholdCount" type="gauge"/>
    <comp-attrib alias="PermUsage" name="Usage" type="Composite">
      <comp-member alias="PermUsageInit" name="init" type="gauge"/>
      <comp-member alias="PermUsageMax" name="max" type="gauge"/>
      <comp-member alias="PermUsageUsed" name="used" type="gauge"/>
      <comp-member alias="PermUsgComit" name="committed" type="gauge"/>
    </comp-attrib>
  </mbean>
  <!-- JVM MemoryPool OLD GEN -->
  <mbean name="JVM MemoryPool:Old Gen" objectname="java.lang:type=MemoryPool,name=Old Gen">
    <attrib alias="OGenCollUseThresh" name="CollectionUsageThreshold" type="gauge"/>
    <attrib alias="OGenCollUseThrshCnt" name="CollectionUsageThresholdCount" type="gauge"/>
    <comp-attrib alias="OGenUsage" name="Usage" type="Composite">
      <comp-member alias="OGenUsageInit" name="init" type="gauge"/>
      <comp-member alias="OGenUsageMax" name="max" type="gauge"/>
      <comp-member alias="OGenUsageUsed" name="used" type="gauge"/>
      <comp-member alias="OGenUsgComit" name="committed" type="gauge"/>
    </comp-attrib>
  </mbean>
  <!-- JVM GC Copy -->
  <mbean name="JVM GarbageCollector:Copy" objectname="java.lang:type=GarbageCollector,name=Copy">
    <attrib alias="GcCoCollCnt" name="CollectionCount" type="counter"/>
    <attrib alias="GcCoCollTime" name="CollectionTime" type="counter"/>
    <comp-attrib alias="GcCoLastGcInfo" name="LastGcInfo" type="Composite">
      <comp-member alias="GcCoThreadCnt" name="GcThreadCount" type="gauge"/>
      <comp-member alias="GcCoDuration" name="duration" type="gauge"/>
      <comp-member alias="GcCoEndTime" name="endTime" type="gauge"/>
    </comp-attrib>
  </mbean>
  <!-- JVM GC MarkSweepCompact -->
  <mbean name="JVM GarbageCollector:MarkSweepCompact" objectname="java.lang:type=GarbageCollector,name=MarkSweepCompact">
    <attrib alias="GcMscCollCnt" name="CollectionCount" type="counter"/>
    <attrib alias="GcMscCollTime" name="CollectionTime" type="counter"/>
    <comp-attrib alias="GcMscLastGcInfo" name="LastGcInfo" type="Composite">
      <comp-member alias="GcMscThreadCnt" name="GcThreadCount" type="gauge"/>
      <comp-member alias="GcMscDuration" name="duration" type="gauge"/>
      <comp-member alias="GcMscEndTime" name="endTime" type="gauge"/>
    </comp-attrib>
  </mbean>
  <!-- JVM GC ParNew -->
  <mbean name="JVM GarbageCollector:ParNew" objectname="java.lang:type=GarbageCollector,name=ParNew">
    <attrib alias="GcParNewCollCnt" name="CollectionCount" type="counter"/>
    <attrib alias="GcParNewCollTime" name="CollectionTime" type="counter"/>
    <comp-attrib alias="GcParNewLastGcInfo" name="LastGcInfo" type="Composite">
      <comp-member alias="GcParNewGcThreadCnt" name="GcThreadCount" type="gauge"/>
      <comp-member alias="GcParNewDuration" name="duration" type="gauge"/>
      <comp-member alias="GcParNewEndTime" name="endTime" type="gauge"/>
    </comp-attrib>
  </mbean>
  <!-- JVM GC ConcurrentMarkSweep -->
  <mbean name="JVM GarbageCollector:ConcurrentMarkSweep" objectname="java.lang:type=GarbageCollector,name=ConcurrentMarkSweep">
    <attrib alias="GcCmsCollCnt" name="CollectionCount" type="counter"/>
    <attrib alias="GcCmsCollTime" name="CollectionTime" type="counter"/>
    <comp-attrib alias="GcCmsLastGcInfo" name="LastGcInfo" type="Composite">
      <comp-member alias="GcCmsGcThreadCnt" name="GcThreadCount" type="gauge"/>
      <comp-member alias="GcCmsDuration" name="duration" type="gauge"/>
      <comp-member alias="GcCmsEndTime" name="endTime" type="gauge"/>
    </comp-attrib>
  </mbean>
  <!-- JVM GC PSMarkSweep -->
  <mbean name="JVM GarbageCollector:PS MarkSweep" objectname="java.lang:type=GarbageCollector,name=PS MarkSweep">
    <attrib alias="GcPsMsCollCnt" name="CollectionCount" type="counter"/>
    <attrib alias="GcPsMsCollTime" name="CollectionTime" type="counter"/>
    <comp-attrib alias="GcPsMsLastGcInfo" name="LastGcInfo" type="Composite">
      <comp-member alias="GcPsMsDuration" name="duration" type="gauge"/>
      <comp-member alias="GcPsMsEndTime" name="endTime" type="gauge"/>
    </comp-attrib>
  </mbean>
  <!-- JVM GC PSScavenge -->
  <mbean name="JVM GarbageCollector:PS Scavenge" objectname="java.lang:type=GarbageCollector,name=PS Scavenge">
    <attrib alias="GcPssCollCnt" name="CollectionCount" type="counter"/>
    <attrib alias="GcPssCollTime" name="CollectionTime" type="counter"/>
    <comp-attrib alias="GcPssLastGcInfo" name="LastGcInfo" type="Composite">
      <comp-member alias="GcPssGcThreadCnt" name="GcThreadCount" type="gauge"/>
      <comp-member alias="GcPssDuration" name="duration" type="gauge"/>
      <comp-member alias="GcPssEndTime" name="endTime" type="gauge"/>
    </comp-attrib>
  </mbean>
</mbeans>