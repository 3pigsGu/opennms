<?xml version="1.0" encoding="UTF-8" standalone="no"?><?castor class-name="org.opennms.netmgt.collectd.CollectdConfiguration"?><collectd-configuration threads="50">
  <package name="example1">
    <filter>IPADDR != '0.0.0.0'</filter>
    <include-range begin="1.1.1.1" end="254.254.254.254"/>
    <include-range begin="::1" end="ffff:ffff:ffff:ffff:ffff:ffff:ffff:ffff"/>
    <service interval="300000" name="SNMP" status="on" user-defined="false">
      <parameter key="collection" value="default"/>
      <parameter key="thresholding-enabled" value="true"/>
    </service>
    <service interval="300000" name="WMI" status="off" user-defined="false">
      <parameter key="collection" value="default"/>
      <parameter key="thresholding-enabled" value="true"/>
    </service>
    <service interval="300000" name="OpenNMS-JVM" status="on" user-defined="false">
      <parameter key="port" value="18980"/>
      <parameter key="retry" value="2"/>
      <parameter key="timeout" value="3000"/>
      <parameter key="protocol" value="rmi"/>
      <parameter key="urlPath" value="/jmxrmi"/>
      <parameter key="rrd-base-name" value="java"/>
      <parameter key="ds-name" value="opennms-jvm"/>
      <parameter key="friendly-name" value="opennms-jvm"/>
      <parameter key="collection" value="OpenNMS-JVM"/>
      <parameter key="thresholding-enabled" value="true"/>
    </service>
    <service interval="300000" name="Cassandra" status="off" user-defined="false">
      <parameter key="port" value="7199"/>
      <parameter key="retry" value="2"/>
      <parameter key="timeout" value="3000"/>
      <parameter key="protocol" value="rmi"/>
      <parameter key="urlPath" value="/jmxrmi"/>
      <parameter key="rrd-base-name" value="java"/>
      <parameter key="ds-name" value="Cassandra"/>
      <parameter key="friendly-name" value="Cassandra"/>
      <parameter key="collection" value="Cassandra"/>
      <parameter key="thresholding-enabled" value="true"/>
    </service>
    <service interval="300000" name="ActiveMQ" status="off" user-defined="false">
      <parameter key="port" value="11099"/>
      <parameter key="retry" value="2"/>
      <parameter key="timeout" value="3000"/>
      <parameter key="protocol" value="rmi"/>
      <parameter key="urlPath" value="/jmxrmi"/>
      <parameter key="rrd-base-name" value="java"/>
      <parameter key="ds-name" value="ActiveMQ"/>
      <parameter key="friendly-name" value="ActiveMQ"/>
      <parameter key="collection" value="ActiveMQ"/>
      <parameter key="thresholding-enabled" value="true"/>
    </service>
  </package>
  <collector class-name="org.opennms.netmgt.collectd.SnmpCollector" service="SNMP"/>
  <collector class-name="org.opennms.netmgt.collectd.WmiCollector" service="WMI"/>
  <collector class-name="org.opennms.netmgt.collectd.Jsr160Collector" service="OpenNMS-JVM"/>
  <collector class-name="org.opennms.netmgt.collectd.Jsr160Collector" service="Cassandra"/>
  <collector class-name="org.opennms.netmgt.collectd.Jsr160Collector" service="ActiveMQ"/>
</collectd-configuration>