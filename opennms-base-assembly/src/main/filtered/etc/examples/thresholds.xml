<?xml version="1.0" encoding="UTF-8" standalone="no"?><thresholding-config>
        <group name="default-snmp" rrdRepository="${install.share.dir}/rrd/snmp/">
                <threshold ds-name="avgBusy5" ds-type="node" rearm="50" trigger="3" type="high" value="90"/>
                <threshold ds-name="freeMem" ds-type="node" rearm="1000000" trigger="3" type="low" value="1024"/>

                <!--  We don't have good data to base the next one on, so we're commenting it out
                <threshold type="high" ds-name="bufferFail"  ds-type="node" value="1" rearm="0" trigger="3"/>

		
                                                                                                        -->

                <!--  We don't have good data to base the next one on, so we're commenting it out
                <threshold type="high" ds-name="bufferNoMem"  ds-type="node" value="1" rearm="0" trigger="3"/>
                                                                                                        -->

		<!-- SNMP Informant thresholds -->
                <threshold ds-name="sinfDskPtFreeSpcC" ds-type="node" rearm="10" trigger="1" type="low" value="5"/>
                <threshold ds-name="sinfDskPtFreeSpcD" ds-type="node" rearm="10" trigger="1" type="low" value="5"/>
                <threshold ds-name="sinfDskPtFreeSpcE" ds-type="node" rearm="10" trigger="1" type="low" value="5"/>
                <threshold ds-name="sinfDskPtFreeSpcTl" ds-type="node" rearm="10" trigger="1" type="low" value="5"/>

                <threshold ds-name="sinfCpuPtProcTime0" ds-type="node" rearm="90" trigger="3" type="high" value="100"/>
                <threshold ds-name="sinfCpuPtProcTime1" ds-type="node" rearm="90" trigger="3" type="high" value="100"/>
                <threshold ds-name="sinfCpuPtProcTime2" ds-type="node" rearm="90" trigger="3" type="high" value="100"/>
                <threshold ds-name="sinfCpuPtProcTime3" ds-type="node" rearm="90" trigger="3" type="high" value="100"/>
                <threshold ds-name="sinfCpuPtProcTimeTl" ds-type="node" rearm="90" trigger="3" type="high" value="100"/>

                <threshold ds-name="wfkernelBuffersFree" ds-type="node" rearm="1" trigger="3" type="low" value="0"/>
                <threshold ds-name="wfkernelMemoryFree" ds-type="node" rearm="1" trigger="3" type="low" value="0"/>
                <threshold ds-name="wfkernelTasksInQue" ds-type="node" rearm="0" trigger="3" type="high" value="100"/>
                <threshold ds-name="wfkernelTasksTotal" ds-type="node" rearm="0" trigger="3" type="high" value="500"/>

                <threshold ds-name="a3sysCpuUtil" ds-type="node" rearm="50" trigger="3" type="high" value="90"/>

                <threshold ds-name="cpuPercentBusy" ds-type="node" rearm="50" trigger="3" type="high" value="90"/>
                <threshold ds-name="freeSpaceOnSys" ds-type="node" rearm="100000" trigger="3" type="low" value="5000"/>
                <threshold ds-name="freeSpaceOnVol2" ds-type="node" rearm="100000" trigger="3" type="low" value="5000"/>

                <!-- The following entry is for the Checkpoint MIB -->
                <threshold ds-name="cpuUtilization" ds-type="node" rearm="50" trigger="3" type="high" value="90"/>


                <threshold ds-name="freeMemory" ds-type="node" rearm="1000" trigger="3" type="low" value="1"/>
                <threshold ds-name="1stHDDFreeSpace" ds-type="node" rearm="100000" trigger="3" type="low" value="5000"/>
                <threshold ds-name="2ndHDDFreeSpace" ds-type="node" rearm="100000" trigger="3" type="low" value="5000"/>

                <threshold ds-name="cimPctHddUsed1" ds-type="node" rearm="80" trigger="1" type="high" value="99"/>
                <threshold ds-name="cimPctHddUsed2" ds-type="node" rearm="80" trigger="1" type="high" value="99"/>
                <threshold ds-name="cim5MinCpuUtilPct" ds-type="node" rearm="50" trigger="3" type="high" value="90"/>
        </group>

        <!--group name="jboss"
                rrdRepository = "${install.share.dir}/rrd/snmp/">
                <threshold type="high" ds-name="portal-core_hits"  ds-type="node" value="10" rearm="5" trigger="3"/>
                <threshold type="high" ds-name="portal-core_access"   ds-type="node" value="10" rearm="7" trigger="3"/>
        </group-->

	<group name="icmp-latency" rrdRepository="${install.share.dir}/rrd/response/">
                <threshold ds-name="icmp" ds-type="if" rearm="25000" trigger="3" type="high" value="75000"/>
        </group>

        <group name="http-latency" rrdRepository="${install.share.dir}/rrd/response/">
                <threshold ds-name="http" ds-type="if" rearm="50" trigger="3" type="high" value="100"/>
        </group>

        <group name="http-8000-latency" rrdRepository="${install.share.dir}/rrd/response/">
                <threshold ds-name="http-8000" ds-type="if" rearm="50" trigger="3" type="high" value="100"/>
        </group>

        <group name="http-8080-latency" rrdRepository="${install.share.dir}/rrd/response/">
                <threshold ds-name="http-8080" ds-type="if" rearm="50" trigger="3" type="high" value="100"/>
        </group>

        <group name="dns-latency" rrdRepository="${install.share.dir}/rrd/response/">
                <threshold ds-name="dns" ds-type="if" rearm="50" trigger="3" type="high" value="100"/>
        </group>

        <group name="dhcp-latency" rrdRepository="${install.share.dir}/rrd/response/">
                <threshold ds-name="dhcp" ds-type="if" rearm="100" trigger="3" type="high" value="500"/>
        </group>
</thresholding-config>