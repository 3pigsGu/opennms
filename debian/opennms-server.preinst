#! /bin/sh
# preinst script for opennms
#
# see: dh_installdeb(1)

set -e

case "$1" in
    install|upgrade)
	if [ "$1" = "install" ]; then
		echo ""
		echo "Please modify the /etc/opennms files before"
		echo "starting OpenNMS for the first time."
		echo ""
	fi
	;;

    abort-upgrade)
	;;

    *)
	echo "preinst called with unknown argument \`$1'" >&2
	exit 1
	;;
esac

# dh_installdeb will replace this with shell code automatically
# generated by other debhelper scripts.

#DEBHELPER#

exit 0


