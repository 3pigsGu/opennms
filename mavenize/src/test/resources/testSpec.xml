<?xml version="1.0"?>
<project xmlns="http://xmlns.opennms.org/xsd/mavenize"
	groupId="org.opennms" artifactId="mavenize-test" version="2.0-SNAPSHOT"
	name="OpenNMS Test Mavenize">
	<module module-id="test-submodule" module-type="jar" module-name="Test Submodule">
		<sources source-type="java">
			<fileset dir="src/test/test-data/src/services">
				<include name="org/opennms/netmgt/**/*.java"/>
				<exclude name="**/*Test*.java"/>
			</fileset>
		</sources>
		<sources source-type="test">
			<fileset dir="src/test/test-data/src/services">
				<include name="org/opennms/netmgt/**/*Test*.java"/>
			</fileset>
		</sources>
		<dependencies>
			<dependency groupId="junit" artifactId="junit" version="3.8.1" scope="test" />
		</dependencies>
	</module>
	<repository repo-id="test-repo" repo-name="Test Repository" release="false" snapshot="true" url="http://www.test.com"/>
</project>

