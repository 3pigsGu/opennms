<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE sqlMap 
          PUBLIC "-//ibatis.apache.org//DTD SQL Map 2.0//EN" 
          "http://ibatis.apache.org/dtd/sql-map-2.dtd" >
<sqlMap>
	<typeAlias alias ="pathOutage" type="org.opennms.netmgt.dao.ibatis.PathOutage" />
	
	<resultMap class="pathOutage" id="result">
		<result property="nodeId" column="nodeID"/>
		<result property="criticalPathIp" column="criticalPathIp"/>
		<result property="criticalPathServiceName" column="criticalPathServiceName"/>
	</resultMap>
	
	<select id="getAllPathOutages" resultMap="result">
		select * from pathOutage
	</select>
</sqlMap>