<!DOCTYPE module PUBLIC "//gwt-module/" "http://google-web-toolkit.googlecode.com/svn/tags/1.6.2/distro-source/core/src/gwt-module.dtd">
<module>

  <!-- Various core dependencies -->
  <inherits name='com.google.gwt.user.User'/>
  <inherits name="com.google.gwt.uibinder.UiBinder" />
  <inherits name="de.novanic.eventservice.GWTEventService"/>
  <inherits name="com.google.gwt.http.HTTP"/>
  <inherits name="com.google.gwt.json.JSON" />

  <!--  inherit css based theme -->
  <inherits name='com.google.gwt.user.theme.standard.Standard'/>

  <!-- Loading Maps -->  
  <inherits name='com.google.gwt.ajaxloader.AjaxLoader' />
  <inherits name='com.google.gwt.maps.GoogleMaps' />
  <inherits name='com.google.gwt.maps.utility.GoogleMapsUtility' />
  <inherits name='com.googlecode.gwtmapquest.GWTMapquest' />
  <inherits name="com.google.gwt.gen2.table.Table"/>

  <!-- Logging -->
  <!-- 
  <inherits name="com.allen_sauer.gwt.log.gwt-log-OFF" />
  <set-property name="compiler.emulatedStack" value="false" />
  <set-configuration-property name="compiler.emulatedStack.recordLineNumbers" value="false"/>
  <set-configuration-property name="compiler.emulatedStack.recordFileNames" value="false"/>
  <set-property name="log_ConsoleLogger" value="DISABLED" />
  <set-property name="log_DivLogger" value="DISABLED" />
  <set-property name="log_FirebugLogger" value="DISABLED" />
  <set-property name="log_GWTLogger" value="DISABLED" />
  <set-property name="log_RemoteLogger" value="ENABLED" />
  <set-property name="log_SystemLogger" value="DISABLED" />
  <set-property name="log_WindowLogger" value="ENABLED" />
  -->

  <!-- Specify the application specific style sheet. -->
  <stylesheet src='Application.css' />

  <!-- Specify the app entry point class. -->
  <entry-point class='org.opennms.features.poller.remote.gwt.client.Application'/>

</module>
