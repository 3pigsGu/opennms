<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans
        http://www.springframework.org/schema/beans/spring-beans-2.0.xsd">

   <bean id="inventoryDatabasePopulator" class="org.opennms.netmgt.dao.hibernate.InventoryDatabasePopulator">
     <property name="transactionTemplate" ref="transactionTemplate" />
     <property name="distPollerDao" ref="distPollerDao" />
     <property name="nodeDao" ref="nodeDao" />
     <property name="categoryDao" ref="categoryDao" />
     <property name="eventDao" ref="eventDao" />
     <property name="serviceTypeDao" ref="serviceTypeDao" />
     <property name="inventoryCategoryDao" ref="inventoryCategoryDao" /> 
     <property name="inventoryAssetDao" ref="inventoryAssetDao" /> 
     <property name="inventoryAssetPropertyDao" ref="inventoryAssetPropertyDao" /> 
   </bean>

</beans>
