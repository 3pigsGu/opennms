<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans
	http://www.springframework.org/schema/beans/spring-beans.xsd">

    <bean id="localDatabaseReportProvider" class="org.opennms.features.reporting.reportrepository.LocalDatabaseReportProvider" />

    <bean id="connectReportProvider" class="org.opennms.features.reporting.reportrepository.ConnectReportProvider" />

    <bean id="defaultReportRepository" class="org.opennms.features.reporting.reportrepository.DefaultReportRepository">
        <property name="reportProviderList">
            <list>
                <value>localDatabaseReportsProvider</value>
                <ref bean="localDatabaseReportProvider" />
                <value>connectDatabaseReportsProvider</value>
                <ref bean="connectReportProvider" />
            </list>
        </property>
    </bean>
</beans>