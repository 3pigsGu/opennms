        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation="http://xmlns.opennms.org/xsd/config/opennms-local-reports">
    <report id="sample-report" display-name="sample JasperReport" online="true"
            report-service="jasperReportService" description="sample Jasper report using jdbc datasource" />
    <report id="online-sample-report" display-name="online sample JasperReport" online="true"
