<!DOCTYPE jasperReport
  PUBLIC "-//JasperReports//DTD Report Design//EN"
  "http://jasperreports.sourceforge.net/dtds/jasperreport.dtd">

<jasperReport name="ParameterTest">
	<parameter name="stringParameter" class="java.lang.String"/>
	<parameter name="integerParameter" class="java.lang.Integer"/>
	<parameter name="dateParameter" class="java.util.Date"/>
 <detail>
    <band height="20">
      <textField>
        <reportElement x="180" y="0" width="200" height="20"/>
        <textFieldExpression class="java.lang.String">$P{stringParameter}</textFieldExpression>
		<textFieldExpression class="java.lang.Integer">$P{integerParameter}</textFieldExpression> 
		<textFieldExpression class="java.util.Date">$P{dateParameter}</textFieldExpression>        

      </textField>
    </band>
  </detail>
</jasperReport>