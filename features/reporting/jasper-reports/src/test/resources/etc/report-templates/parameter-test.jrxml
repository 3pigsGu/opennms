<!DOCTYPE jasperReport
  PUBLIC "-//JasperReports//DTD Report Design//EN"
  "http://jasperreports.sourceforge.net/dtds/jasperreport.dtd">

<jasperReport name="ParameterTest">
        <parameter name="stringParameter" class="java.lang.String"/>
        <parameter name="integerParameter" class="java.lang.Integer"/>
        <parameter name="dateParameter" class="java.util.Date"/>
        <parameter name="sqlDateParameter" class="java.util.Date"/>
    <title>
        <band height="20">
                <staticText>
					<reportElement x="180" y="0" width="200" height="20"/>
                    <text><![CDATA[trivial test]]></text>
                </staticText>
        </band>
    </title>
    <detail>
        <band height="80">
            <textField>
            <reportElement x="180" y="0" width="200" height="80"/>
                <textFieldExpression>
                    "stringParameter: " + $P{stringParameter} + "\n" +
                    "integerParameter: " + $P{integerParameter} + "\n" +
                    "dateParameter: " + (new SimpleDateFormat("MM/dd/yyyy HH:mm:ss")).format($P{dateParameter}) + "\n" +
                    "sqlDateParameter: " + (new SimpleDateFormat("MM/dd/yyyy HH:mm:ss")).format($P{sqlDateParameter})
                </textFieldExpression>
                </textField>
            </band>
    </detail>
</jasperReport>
